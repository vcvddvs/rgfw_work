{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, computed, onMounted, watch } from 'vue';\nimport { useRoute } from 'vue-router';\nimport router from \"@/router\";\nimport { orderApi } from '@/utils/api';\nexport default {\n  __name: 'OrderComponent',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const businessName = ref(route.params.businessName || '默认店铺名称');\n    const cart = ref({});\n    const deliveryAddresses = ref([]);\n    const showAddresses = ref(false);\n    const businessId = ref(route.params.businessId || 0);\n\n    // 从 localStorage 获取选中的地址\n    const selectedAddress = ref(JSON.parse(localStorage.getItem('selectedAddress') || '{}').address || '云南大学楸苑1栋');\n\n    // 从localStorage获取订单信息\n    const orderInfo = JSON.parse(localStorage.getItem('orderInfo') || '{}');\n    console.log('从localStorage加载的订单信息:', orderInfo);\n\n    // 加载购物车和商家信息\n    if (orderInfo.cart) {\n      cart.value = orderInfo.cart;\n      if (!businessId.value) businessId.value = orderInfo.businessId;\n      if (!businessName.value) businessName.value = orderInfo.businessName;\n      console.log('成功加载购物车数据:', Object.keys(cart.value).length, '项商品');\n    } else {\n      console.warn('localStorage中没有购物车数据');\n    }\n\n    // 显示调试信息\n    console.log('当前路由参数:', route.params);\n    console.log('businessId:', businessId.value);\n    console.log('businessName:', businessName.value);\n\n    // 监听路由变化，更新订单信息\n    watch(route, newRoute => {\n      if (newRoute.name === 'Order') {\n        const newOrderInfo = JSON.parse(localStorage.getItem('orderInfo') || '{}');\n        if (newOrderInfo.cart) {\n          cart.value = newOrderInfo.cart;\n          businessId.value = newOrderInfo.businessId;\n          businessName.value = newOrderInfo.businessName;\n        }\n      }\n    });\n    if (route.query.cart) {\n      try {\n        cart.value = JSON.parse(route.query.cart);\n      } catch (error) {\n        console.error('解析购物车数据失败:', error);\n        cart.value = {}; // 确保在解析失败时设置为空对象\n      }\n    }\n    const goToAddressPage = () => {\n      const userId = localStorage.getItem('userId'); // 从localStorage获取userId\n      if (!userId) {\n        console.error('用户未登录');\n        return;\n      }\n      // 导航到 address 组件，并传递 userId\n      router.push({\n        name: 'Address',\n        params: {\n          userId: userId,\n          businessId: businessId.value,\n          businessName: businessName.value\n        }\n      });\n    };\n    onMounted(() => {\n      showAddresses.value = false;\n    });\n    const foodItems = computed(() => {\n      console.log('计算foodItems，购物车数据:', cart.value);\n      return Object.values(cart.value).map(item => ({\n        title: item.foodName || '未命名商品',\n        price: item.price || 0,\n        description: item.description || '商品描述',\n        quantity: item.quantity || 1,\n        photo: require('@/assets/img/BUSINESS/menus/img.png') // 使用默认图片\n      }));\n    });\n    const totalAmount = computed(() => {\n      return Object.values(cart.value).reduce((sum, item) => sum + item.quantity * item.price, 0);\n    });\n    const submitOrder = async () => {\n      if (Object.keys(cart.value).length === 0) {\n        alert('购物车为空，请先添加商品！');\n        return;\n      }\n      const userId = localStorage.getItem('userId');\n      if (!userId) {\n        alert('用户未登录，请先登录！');\n        return;\n      }\n      const orderDate = new Date().toISOString().slice(0, 10); // 订单日期\n      const orderTotal = totalAmount.value; // 订单总价\n      const daIdSelected = orderInfo.selectedDaId; // 使用选中的 daId\n\n      if (!daIdSelected) {\n        alert('请选择配送地址');\n        goToAddressPage();\n        return;\n      }\n      const order = {\n        userId: userId,\n        businessId: businessId.value,\n        orderDate: orderDate,\n        orderTotal: orderTotal,\n        daId: daIdSelected,\n        orderState: 0 // 未支付\n      };\n      const details = Object.values(cart.value).map(item => ({\n        foodId: item.foodId,\n        quantity: item.quantity\n      }));\n      const requestData = {\n        order,\n        details\n      };\n      console.log(\"Request body:\", requestData);\n      try {\n        // 使用orderApi创建订单\n        const response = await orderApi.createOrder(requestData);\n        console.log('订单提交成功:', response);\n        if (response && (response.code === 1 || response.success)) {\n          // 保存订单ID到localStorage，以便支付时使用\n          if (response.data && response.data.orderId) {\n            localStorage.setItem('currentOrderId', response.data.orderId);\n            console.log('保存订单ID到localStorage:', response.data.orderId);\n          }\n\n          // 清空购物车\n          localStorage.removeItem('orderInfo');\n\n          // 导航到支付页面\n          router.push({\n            name: 'Payment',\n            query: {\n              totalAmount: orderTotal,\n              businessName: businessName.value,\n              orderId: response.data?.orderId\n            }\n          });\n        } else {\n          throw new Error(response?.msg || '订单创建失败');\n        }\n      } catch (error) {\n        console.error('提交订单出错:', error);\n\n        // 直接跳转到支付页面，但这样可能会有问题\n        alert('网络连接问题，但我们将继续处理您的订单');\n        router.push({\n          name: 'Payment',\n          query: {\n            totalAmount: orderTotal,\n            businessName: businessName.value,\n            isNetworkError: true\n          }\n        });\n      }\n    };\n    const goBack = () => {\n      router.push({\n        name: 'BusinessList2',\n        params: {\n          businessId: businessId.value,\n          businessName: businessName.value\n        }\n      }); // 使用 router.back() 返回上一页\n    };\n    const __returned__ = {\n      route,\n      businessName,\n      cart,\n      deliveryAddresses,\n      showAddresses,\n      businessId,\n      selectedAddress,\n      orderInfo,\n      goToAddressPage,\n      foodItems,\n      totalAmount,\n      submitOrder,\n      goBack,\n      ref,\n      computed,\n      onMounted,\n      watch,\n      get useRoute() {\n        return useRoute;\n      },\n      get router() {\n        return router;\n      },\n      get orderApi() {\n        return orderApi;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","watch","useRoute","router","orderApi","route","businessName","params","cart","deliveryAddresses","showAddresses","businessId","selectedAddress","JSON","parse","localStorage","getItem","address","orderInfo","console","log","value","Object","keys","length","warn","newRoute","name","newOrderInfo","query","error","goToAddressPage","userId","push","foodItems","values","map","item","title","foodName","price","description","quantity","photo","require","totalAmount","reduce","sum","submitOrder","alert","orderDate","Date","toISOString","slice","orderTotal","daIdSelected","selectedDaId","order","daId","orderState","details","foodId","requestData","response","createOrder","code","success","data","orderId","setItem","removeItem","Error","msg","isNetworkError","goBack"],"sources":["D:/back_elm/vue_elm/src/components/OrderComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <header class=\"header\">\r\n      <div class=\"title\">\r\n        <div class=\"p1\">\r\n                    <!-- 使用 Element Plus 的返回图标 -->\r\n                    <el-icon @click=\"goBack\" style=\"background-color:#9F9F9F\">\r\n                      <img :src=\"require('@/assets/img_10.png')\" alt=\"\">\r\n                    </el-icon>\r\n        </div>\r\n        <div class=\"p2\">\r\n          <p>确认订单</p>\r\n        </div>\r\n      </div>\r\n      <div class=\"banner\">\r\n        <span class=\"iconfont icon-guangbogonggao\" style=\"padding-left: 2vw\"></span>\r\n        <p style=\"padding-left: 2vw\">温馨提示： 请适量点餐,避免浪费</p>\r\n      </div>\r\n      <div class=\"location\">\r\n        <div class=\"location-text\">\r\n          <p>地址: {{ selectedAddress }}</p>\r\n          <img :src=\"require('@/assets/img/orders/img.png')\"\r\n               height=\"23\"\r\n               width=\"17\" alt=\"描述\"\r\n               @click=\"goToAddressPage \" />\r\n        </div>\r\n        <!-- 显示地址列表 -->\r\n        <div v-if=\"showAddresses\" class=\"address-list\">\r\n          <div\r\n              v-for=\"address in deliveryAddresses\"\r\n              :key=\"address.daId\"\r\n              class=\"address-item\"\r\n              @click=\"selectAddress(address)\"\r\n          >\r\n            {{  address.address }}\r\n          </div>\r\n        </div>\r\n        <div class=\"location-speed\">\r\n          <p>立即送出</p>\r\n          <div class=\"location-speed-to\">\r\n            <p>预计13:40送达</p>\r\n            <img :src=\"require('@/assets/img/orders/img.png')\" height=\"23\" width=\"17\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </header>\r\n    <main class=\"body\">\r\n      <div class=\"body-title\">\r\n        <div class=\"title1\">\r\n          <p>{{ businessName }}</p>\r\n\r\n        </div>\r\n        <div class=\"title2\">\r\n          <p>蜂鸟准时达</p>\r\n        </div>\r\n      </div>\r\n      <div class=\"body-text\">\r\n        <div v-for=\"item in foodItems\" :key=\"item.title\" class=\"food\">\r\n          <div class=\"food-left\">\r\n            <img :src=\"item.photo\" alt=\"商品图片\" />\r\n          </div>\r\n          <div class=\"food-right\">\r\n            <div class=\"food-right-title\">\r\n              <div class=\"title1\">\r\n                <p>{{ item.title }}</p>\r\n              </div>\r\n              <div class=\"price\">\r\n                <p>&#165;{{ item.price }}</p>\r\n              </div>\r\n            </div>\r\n            <div class=\"name\">\r\n              <p>{{ item.description }}</p>\r\n            </div>\r\n            <div class=\"number\">\r\n              <p>x {{ item.quantity }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 其他费用和总计 -->\r\n      </div>\r\n    </main>\r\n    <footer class=\"footer\">\r\n      <div class=\"foot-left\">\r\n        <div class=\"sum-price\">\r\n          <p1>合计</p1>\r\n          <p2>{{ totalAmount.toFixed(2) }}</p2> <!-- 格式化金额显示 -->\r\n        </div>\r\n      </div>\r\n      <div class=\"foot-right\" @click=\"submitOrder\">\r\n        <p>提交订单</p>\r\n      </div>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n<script setup>\r\nimport {ref, computed, onMounted, watch} from 'vue';\r\nimport { useRoute } from 'vue-router';\r\nimport router from \"@/router\";\r\nimport { orderApi } from '@/utils/api';\r\n\r\nconst route = useRoute();\r\nconst businessName = ref(route.params.businessName || '默认店铺名称');\r\nconst cart = ref({});\r\nconst deliveryAddresses = ref([]);\r\nconst showAddresses = ref(false);\r\nconst businessId = ref(route.params.businessId || 0);\r\n\r\n// 从 localStorage 获取选中的地址\r\nconst selectedAddress = ref(JSON.parse(localStorage.getItem('selectedAddress') || '{}').address || '云南大学楸苑1栋');\r\n\r\n// 从localStorage获取订单信息\r\nconst orderInfo = JSON.parse(localStorage.getItem('orderInfo') || '{}');\r\nconsole.log('从localStorage加载的订单信息:', orderInfo);\r\n\r\n// 加载购物车和商家信息\r\nif (orderInfo.cart) {\r\n  cart.value = orderInfo.cart;\r\n  if (!businessId.value) businessId.value = orderInfo.businessId;\r\n  if (!businessName.value) businessName.value = orderInfo.businessName;\r\n  console.log('成功加载购物车数据:', Object.keys(cart.value).length, '项商品');\r\n} else {\r\n  console.warn('localStorage中没有购物车数据');\r\n}\r\n\r\n// 显示调试信息\r\nconsole.log('当前路由参数:', route.params);\r\nconsole.log('businessId:', businessId.value);\r\nconsole.log('businessName:', businessName.value);\r\n\r\n// 监听路由变化，更新订单信息\r\nwatch(route, (newRoute) => {\r\n  if (newRoute.name === 'Order') {\r\n    const newOrderInfo = JSON.parse(localStorage.getItem('orderInfo') || '{}');\r\n    if (newOrderInfo.cart) {\r\n      cart.value = newOrderInfo.cart;\r\n      businessId.value = newOrderInfo.businessId;\r\n      businessName.value = newOrderInfo.businessName;\r\n    }\r\n  }\r\n});\r\n\r\nif (route.query.cart) {\r\n  try {\r\n    cart.value = JSON.parse(route.query.cart);\r\n  } catch (error) {\r\n    console.error('解析购物车数据失败:', error);\r\n    cart.value = {}; // 确保在解析失败时设置为空对象\r\n  }\r\n}\r\n\r\nconst goToAddressPage = () => {\r\n  const userId = localStorage.getItem('userId'); // 从localStorage获取userId\r\n  if (!userId) {\r\n    console.error('用户未登录');\r\n    return;\r\n  }\r\n  // 导航到 address 组件，并传递 userId\r\n  router.push({\r\n    name: 'Address',\r\n    params: { userId: userId, businessId: businessId.value, businessName: businessName.value }\r\n  });\r\n};\r\n\r\nonMounted(() => {\r\n  showAddresses.value = false;\r\n});\r\n\r\nconst foodItems = computed(() => {\r\n  console.log('计算foodItems，购物车数据:', cart.value);\r\n  return Object.values(cart.value).map(item => ({\r\n    title: item.foodName || '未命名商品',\r\n    price: item.price || 0,\r\n    description: item.description || '商品描述',\r\n    quantity: item.quantity || 1,\r\n    photo: require('@/assets/img/BUSINESS/menus/img.png') // 使用默认图片\r\n  }));\r\n});\r\n\r\nconst totalAmount = computed(() => {\r\n  return Object.values(cart.value).reduce((sum, item) => sum + item.quantity * item.price, 0);\r\n});\r\n\r\nconst submitOrder = async () => {\r\n  if (Object.keys(cart.value).length === 0) {\r\n    alert('购物车为空，请先添加商品！');\r\n    return;\r\n  }\r\n\r\n  const userId = localStorage.getItem('userId');\r\n  if (!userId) {\r\n    alert('用户未登录，请先登录！');\r\n    return;\r\n  }\r\n\r\n  const orderDate = new Date().toISOString().slice(0, 10); // 订单日期\r\n  const orderTotal = totalAmount.value; // 订单总价\r\n  const daIdSelected = orderInfo.selectedDaId; // 使用选中的 daId\r\n\r\n  if (!daIdSelected) {\r\n    alert('请选择配送地址');\r\n    goToAddressPage();\r\n    return;\r\n  }\r\n\r\n  const order = {\r\n    userId: userId,\r\n    businessId: businessId.value,\r\n    orderDate: orderDate,\r\n    orderTotal: orderTotal,\r\n    daId: daIdSelected,\r\n    orderState: 0 // 未支付\r\n  };\r\n\r\n  const details = Object.values(cart.value).map(item => ({\r\n    foodId: item.foodId,\r\n    quantity: item.quantity\r\n  }));\r\n\r\n  const requestData = { order, details };\r\n  console.log(\"Request body:\", requestData);\r\n\r\n  try {\r\n    // 使用orderApi创建订单\r\n    const response = await orderApi.createOrder(requestData);\r\n    console.log('订单提交成功:', response);\r\n\r\n    if (response && (response.code === 1 || response.success)) {\r\n      // 保存订单ID到localStorage，以便支付时使用\r\n      if (response.data && response.data.orderId) {\r\n        localStorage.setItem('currentOrderId', response.data.orderId);\r\n        console.log('保存订单ID到localStorage:', response.data.orderId);\r\n      }\r\n\r\n      // 清空购物车\r\n      localStorage.removeItem('orderInfo');\r\n      \r\n      // 导航到支付页面\r\n      router.push({\r\n        name: 'Payment',\r\n        query: { \r\n          totalAmount: orderTotal, \r\n          businessName: businessName.value,\r\n          orderId: response.data?.orderId\r\n        }\r\n      });\r\n    } else {\r\n      throw new Error(response?.msg || '订单创建失败');\r\n    }\r\n  } catch (error) {\r\n    console.error('提交订单出错:', error);\r\n    \r\n    // 直接跳转到支付页面，但这样可能会有问题\r\n    alert('网络连接问题，但我们将继续处理您的订单');\r\n    \r\n    router.push({\r\n      name: 'Payment',\r\n      query: { \r\n        totalAmount: orderTotal, \r\n        businessName: businessName.value,\r\n        isNetworkError: true\r\n      }\r\n    });\r\n  }\r\n};\r\n\r\nconst goBack = () => {\r\n  router.push({name: 'BusinessList2', params: {businessId:businessId.value,businessName: businessName.value}}); // 使用 router.back() 返回上一页\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n/* 添加 CSS 样式 */\r\n\r\n.wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n.wrapper .header .title {\r\n  