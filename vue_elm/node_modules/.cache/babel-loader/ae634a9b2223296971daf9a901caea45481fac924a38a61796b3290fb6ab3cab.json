{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted } from 'vue';\nimport { useRoute } from 'vue-router';\nimport axios from 'axios';\nimport router from \"@/router\";\nimport { useStore } from 'vuex';\nexport default {\n  __name: 'addressComponent',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const userId = ref(route.params.userId); // 从路由参数中获取 userId\n    const businessId = ref(route.params.businessId); // 从路由参数中获取 businessId\n    const businessName = ref(route.params.businessName); // 从路由参数中获取 businessName\n    const deliveryAddresses = ref([]);\n    const store = useStore();\n    console.log(businessId.value);\n    console.log(businessName.value);\n    const fetchDeliveryAddresses = async () => {\n      if (!userId.value) {\n        console.error('用户ID未提供');\n        return;\n      }\n      try {\n        const response = await axios.get(`http://localhost:8080/elm_servlet_war_exploded/api/deliveryaddresses?userId=${userId.value}`);\n        deliveryAddresses.value = response.data;\n      } catch (error) {\n        console.error('获取地址失败:', error);\n      }\n    };\n    onMounted(fetchDeliveryAddresses);\n    const selectAddress = address => {\n      // 使用 window.confirm 弹出确认对话框\n      const isConfirmed = window.confirm('确定选择这个地址吗？');\n\n      // 根据用户的选择执行操作\n      if (isConfirmed) {\n        // 用户点击“确定”按钮时执行\n        chooseAddress(address);\n      } else {\n        // 用户点击“取消”按钮时执行\n        console.log('选择地址已取消');\n      }\n    };\n    const chooseAddress = address => {\n      store.commit('setSelectedAddress', address);\n      router.push({\n        name: 'Order',\n        params: {\n          businessId: businessId.value,\n          businessName: businessName.value\n        }\n      });\n    };\n    const __returned__ = {\n      route,\n      userId,\n      businessId,\n      businessName,\n      deliveryAddresses,\n      store,\n      fetchDeliveryAddresses,\n     