{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nexport default {\n  __name: 'IndexComponent',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const businesses = ref([]);\n    const page = ref(1);\n    const pageSize = ref(10);\n    const hasMore = ref(true);\n    const router = useRouter();\n    const fetchBusinesses = async () => {\n      try {\n        const response = await fetch(`http://localhost:8080/api/businesses?page=${page.value}&size=${pageSize.value}`);\n        if (!response.ok) {\n          throw new Error('Network response was not ok');\n        }\n        const data = await response.json();\n        if (data.length < pageSize.value) {\n          hasMore.value = false; // 如果返回的数据少于pageSize，表示没有更多数据了\n        }\n        businesses.value.push(...data);\n      } catch (error) {\n        console.error('获取商家列表失败:', error);\n      }\n    };\n    const handleScroll = () => {\n      const scrollElement = document.querySelector('.foodList');\n      if (scrollElement.scrollHeight - scrollElement.scrollTop === scrollElement.clientHeight && hasMore.value) {\n        page.value += 1; // 增加页码\n        fetchBusinesses(); // 调用获取商家列表的方法\n      }\n    };\n    onMounted(() => {\n      fetchBusinesses(); // 组件挂载后立即获取商家列表\n    });\n    const goToHistory = () => {\n      router.push('/history-component');\n    };\n    const goToLogin = () => {\n      router.push('/login-component');\n    };\n    const goToBusinessList = business => {\n      router.push({\n        name: 'BusinessList2',\n        params: {\n          businessId: business.businessId,\n          businessName: business.businessName\n        }\n      });\n    };\n    const __returned__ = {\n      businesses,\n      page,\n      pageSize,\n      hasMore,\n      router,\n      fetchBusinesses,\n      handleScroll,\n      goToHistory,\n      goToLogin,\n      goToBusinessList,\n      ref,\n      onMounted,\n      get useRouter() {\n        return useRouter;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRouter","businesses","page","pageSize","hasMore","router","fetchBusinesses","response","fetch","value","ok","Error","data","json","length","push","error","console","handleScroll","scrollElement","document","querySelector","scrollHeight","scrollTop","clientHeight","goToHistory","goToLogin","goToBusinessList","business","name","params","businessId","businessName"],"sources":["C:/Users/asus/Desktop/vue_elm/vue_elm/src/components/IndexComponent.vue"],"sourcesContent":["\r\n\r\n<template>\r\n\r\n  <div class=\"wrapper\">\r\n    <div class=\"header\">\r\n      <div class=\"location\">\r\n        <div class=\"icon\">\r\n          <i class=\"fa-solid fa-location-dot\"></i>\r\n          <div class=\"icon-text\">饿了么</div>\r\n        </div>\r\n        <div class=\"location-text\">\r\n          云南大学呈贡校区力行楼<i class=\"fa-solid fa-caret-down\"></i>\r\n        </div>\r\n      </div>\r\n      <div class=\"search\">\r\n        <div class=\"search-panel\">\r\n          <i style=\"flex: 0 0 6vw; padding-left: 3vw; font-size: 4vw;color: #17BAF9;\" class=\"fa-solid fa-expand\"></i>\r\n          <input style=\"flex:6;\" type=\"text\"  placeholder=\"请输入要查询的商品\"/>\r\n          <div style=\"flex:0 0 17vw\" class=\"search-button\" @click=\"searchClick()\">搜索</div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"foodType\">\r\n      <div class=\"foodTypeItem\">\r\n        <img :src=\"require('@/assets/img/TypeImg/class01.jpg')\" >\r\n        <p>美食外卖</p>\r\n      </div>\r\n      <div class=\"foodTypeItem\">\r\n        <img :src=\"require('@/assets/img/TypeImg/class02.jpg')\" alt=\"描述\" />\r\n        <p>超市便利</p>\r\n      </div>\r\n      <div class=\"foodTypeItem\">\r\n        <img :src=\"require('@