     <img :src=\"require('@/assets/img_2.png')\" style=\"width:80%;height: 90%\" alt=\"描述\" />\r\n    </div>\r\n    <el-form-item label=\"手机号码：\">\r\n      <el-input v-model=\"phoneNumber \" placeholder=\"账户\"></el-input>\r\n    </el-form-item>\r\n    <el-form-item label=\"密码：\">\r\n      <el-input type=\"password\" v-model=\"password\" placeholder=\"密码\"></el-input>\r\n    </el-form-item>\r\n    <el-form-item label=\"确认密码：\">\r\n      <el-input type=\"password\" v-model=\"confirmPassword\" placeholder=\"再次确认\"></el-input>\r\n    </el-form-item>\r\n    <div class=\"button-box\">\r\n      <el-button type=\"primary\" style=\"width: 75%;margin-left: 30px;margin-bottom: 10px\" @click=\"register\">注册</el-button>\r\n    </div>\r\n    <div class=\"button-box\" >\r\n      <el-button style=\"width: 75%;margin-left: 30px;\" @click=\"goToLogin\">返回登录界面</el-button>\r\n    </div>\r\n  </el-form>\r\n</template>\r\n\r\n\r\n<script setup>\r\nimport { ref } from 'vue';\r\nimport axios from 'axios';\r\nimport { useRouter } from 'vue-router';\r\n\r\nconst router = useRouter();\r\nconst phoneNumber = ref('');\r\nconst password = ref('');\r\nconst confirmPassword = ref('');\r\n\r\nconst register = () => {\r\n  if (password.value !== confirmPassword.value) {\r\n    alert('两次输入的密码不一致');\r\n    return;\r\n  }\r\n  axios.post('http://localhost:8080/elm_servlet_war_exploded/register', {\r\n    userId: phoneNumber.value,\r\n    password: password.value,\r\n    // 可以添加其他需要的字段\r\n  })\r\n      .then(response => {\r\n        if (response.data && response.data.success) {\r\n          alert('注册成功');\r\n          router.push('/Index-Component');\r\n        } else {\r\n          alert('注册失败：' + response.data.message);\r\n        }\r\n      })\r\n      .catch(error => {\r\n        alert('注册失败：' + error.message);\r\n      });\r\n};\r\n\r\nconst goToLogin = () => {\r\n  router.push('/login-component');\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 将原有的 CSS 样式复制到这里 */\r\n.wrapper {\r\n  width: 100%;\r\