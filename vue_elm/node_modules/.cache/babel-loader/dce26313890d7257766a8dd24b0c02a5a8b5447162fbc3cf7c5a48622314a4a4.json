{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useRouter } from 'vue-router';\nimport { businessApi } from '@/utils/api';\nimport axios from 'axios';\nimport { ref, onMounted, onUnmounted } from 'vue';\n\n// 定义广告数据\n\nexport default {\n  __name: 'IndexComponent',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const businesses = ref([]);\n    const page = ref(1);\n    const pageSize = ref(10);\n    const hasMore = ref(true);\n    const router = useRouter();\n    const ads = ref([{\n      key: 'ad1',\n      leftTopImg: require('@/assets/img/TypeImg/img.png'),\n      leftText: '在线订单',\n      leftContentLeftText: '一起来嗨',\n      leftContentRightImg: require('@/assets/img/TypeImg/bannerLeft-Right.png'),\n      rightItems: [{\n        title: '晚餐聚会',\n        img: require('@/assets/img/BannerImg/banner01.jpg'),\n        remark: '一起吃好点'\n      }, {\n        title: '学生福利',\n        img: require('@/assets/img/BannerImg/banner02.jpg'),\n        remark: '最高省912'\n      }]\n    }, {\n      key: 'ad2',\n      leftTopImg: require('@/assets/img/BannerImg/banner04.jpg'),\n      leftText: '深夜食堂',\n      leftContentLeftText: '都是好吃的',\n      leftContentRightImg: require('@/assets/img/BannerImg/banner03.jpg'),\n      rightItems: [{\n        title: '深夜解馋',\n        img: require('@/assets/img/BannerImg/img_5.jpg'),\n        remark: '领夜宵红包'\n      }, {\n        title: '学生福利',\n        img: require('@/assets/img/BannerImg/img_6.jpg'),\n        remark: '最高省912'\n      }]\n    }]);\n    const currentAdIndex = ref(0); // 当前激活的广告索引\n\n    const switchAd = () => {\n      currentAdIndex.value = (currentAdIndex.value + 1) % ads.value.length;\n    };\n    onMounted(() => {\n      const intervalId = setInterval(switchAd, 3000); // 每3秒切换一次广告\n      onUnmounted(() => {\n        clearInterval(intervalId);\n      });\n    });\n    const fetchBusinesses = async () => {\n      try {\n        console.log('开始获取商家列表，页码:', page.value, '每页数量:', pageSize.value);\n\n        // 使用正确的参数格式调用API\n        const response = await businessApi.getBusinessList({\n          page: page.value,\n          size: pageSize.value\n        });\n        console.log('商家列表响应:', response);\n\n        // 检查响应结构\n        if (response && response.data) {\n          // 检查response.data是否是我们需要的数据\n          let businessData;\n\n          // 如果response.data是数组，直接使用\n          if (Array.isArray(response.data)) {\n            businessData = response.data;\n          }\n          // 如果response.data是对象，可能包含一个data或content字段\n          else if (response.data.data && Array.isArray(response.data.data)) {\n            businessData = response.data.data;\n          } else if (response.data.content && Array.isArray(response.data.content)) {\n            businessData = response.data.content;\n          }\n          // 如果response.data.records存在（可能是分页对象）\n          else if (response.data.records && Array.isArray(response.data.records)) {\n            businessData = response.data.records;\n          }\n          // 如果response.data.list存在\n          else if (response.data.list && Array.isArray(response.data.list)) {\n            businessData = response.data.list;\n          }\n          // 如果response.data本身是对象，但不包含数组数据，可能是单个商家\n          else if (typeof response.data === 'object' && !Array.isArray(response.data)) {\n            businessData = [response.data];\n          }\n\n          // 如果找到了商家数据\n          if (businessData && businessData.length > 0) {\n            console.log('找到商家数据:', businessData);\n            if (businessData.length < pageSize.value) {\n              hasMore.value = false;\n            }\n\n            // 添加调试日志，检查每个商家对象\n            businessData.forEach((business, index) => {\n              console.log(`商家 ${index + 1}:`, business);\n            });\n            businesses.value.push(...businessData);\n          } else {\n            console.log('未找到商家数据或数据为空');\n            hasMore.value = false;\n          }\n        } else {\n          console.error('获取商家列表失败: 响应格式不正确', response);\n        }\n      } catch (error) {\n        console.error('获取商家列表失败:', error);\n      }\n    };\n    const handleScroll = () => {\n      const scrollElement = document.querySelector('.foodList');\n      if (scrollElement.scrollHeight - scrollElement.scrollTop === scrollElement.clientHeight && hasMore.value) {\n        page.value += 1; // 增加页码\n        fetchBusinesses(); // 调用获取商家列表的方法\n      }\n    };\n    onMounted(() => {\n      fetchBusinesses(); // 组件挂载后立即获取商家列表\n    });\n    const goToHistory = () => {\n      router.push('/history-component');\n    };\n    const goToLogin = () => {\n      router.push('/login-component');\n    };\n    const goToBusinessList = business => {\n      router.push({\n        name: 'BusinessList2',\n        params: {\n          businessId: business.businessId,\n          businessName: business.businessName,\n          businessImg: business.businessImg\n        }\n      });\n    };\n    const handleImageError = event => {\n      event.target.src = require('@/assets/img/TypeImg/img.png');\n    };\n    const processImageUrl = imgUrl => {\n      // 如果URL为空，返回默认图片\n      if (!imgUrl) {\n        console.log('图片URL为空，使用默认图片');\n        return require('@/assets/img/TypeImg/img.png');\n      }\n\n      // 如果URL是字符串类型\n      if (typeof imgUrl === 'string') {\n        // 如果已经是完整的URL或data:image格式，直接返回\n        if (imgUrl.startsWith('http') || imgUrl.startsWith('data:')) {\n          console.log('使用原始图片URL(Base64或HTTP)');\n          return imgUrl;\n        }\n\n        // 尝试作为相对路径处理\n        try {\n          console.log('尝试作为相对路径处理:', imgUrl);\n          return require('@/assets/img/TypeImg/img.png');\n        } catch (e) {\n          console.log('图片路径处理失败，使用默认图片');\n          return require('@/assets/img/TypeImg/img.png');\n        }\n      }\n\n      // 如果URL是对象或其他类型，返回默认图片\n      console.log('图片URL类型不是字符串，使用默认图片');\n      return require('@/assets/img/TypeImg/img.png');\n    };\n    const __returned__ = {\n      businesses,\n      page,\n      pageSize,\n      hasMore,\n      router,\n      ads,\n      currentAdIndex,\n      switchAd,\n      fetchBusinesses,\n      handleScroll,\n      goToHistory,\n      goToLogin,\n      goToBusinessList,\n      handleImageError,\n      processImageUrl,\n      get useRouter() {\n        return useRouter;\n      },\n      get businessApi() {\n        return businessApi;\n      },\n      get axios() {\n        return axios;\n      },\n      ref,\n      onMounted,\n      onUnmounted\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["useRouter","businessApi","axios","ref","onMounted","onUnmounted","businesses","page","pageSize","hasMore","router","ads","key","leftTopImg","require","leftText","leftContentLeftText","leftContentRightImg","rightItems","title","img","remark","currentAdIndex","switchAd","value","length","intervalId","setInterval","clearInterval","fetchBusinesses","console","log","response","getBusinessList","size","data","businessData","Array","isArray","content","records","list","forEach","business","index","push","error","handleScroll","scrollElement","document","querySelector","scrollHeight","scrollTop","clientHeight","goToHistory","goToLogin","goToBusinessList","name","params","businessId","businessName","businessImg","handleImageError","event","target","src","processImageUrl","imgUrl","startsWith","e"],"sources":["D:/back_elm/vue_elm/src/components/IndexComponent.vue"],"sourcesContent":["<template>\n\n  <div class=\"wrapper\">\n    <div class=\"header\">\n      <div class=\"location\">\n        <div class=\"icon\">\n          <div class=\"icon-text\">常点 | 推荐</div>\n        </div>\n        <div class=\"location-text\">\n          云南大学呈贡校区<i class=\"fa-solid fa-caret-down\"></i>\n        </div>\n      </div>\n      <div class=\"search\">\n        <div class=\"search-panel\">\n          <i style=\"flex: 0 0 6vw; padding-left: 3vw; font-size: 4vw;color: #17BAF9;\" class=\"fa-solid fa-expand\"></i>\n          <input style=\"flex:6;\" type=\"text\"  placeholder=\"请输入要查询的商品\"/>\n          <div style=\"flex:0 0 17vw\" class=\"search-button\" @click=\"searchClick()\">搜索</div>\n        </div>\n      </div>\n    </div>\n    <div class=\"foodType\">\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class01.jpg')\" >\n        <p>美食外卖</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class02.jpg')\" alt=\"描述\" />\n        <p>超市便利</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class03.jpg')\" alt=\"描述\" />\n        <p>水果鲜花</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class04.jpg')\" alt=\"描述\" />\n        <p>买菜</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class05.jpg')\" alt=\"描述\" />\n        <p>买药</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class06.jpg')\" alt=\"描述\" />\n        <p>甜品饮品</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class07.jpg')\" alt=\"描述\" />\n        <p>天天爆红包</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class08.jpg')\" alt=\"描述\" />\n        <p>0元领水果</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class09.jpg')\" alt=\"描述\" />\n        <p>跑腿</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class10.jpg')\" alt=\"描述\" />\n        <p>天天特价</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class11.jpg')\" alt=\"描述\" />\n        <p>借钱</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class12.jpg')\" alt=\"描述\" />\n        <p>粥粉面点</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class13.jpg')\" alt=\"描述\" />\n        <p>宠物用品</p>\n      </div>\n      <div class=\"foodTypeItem\" onclick=\"location.href='businessList2.html'\">\n        <img :src=\"require('@/assets/img/TypeImg/class14.jpg')\" alt=\"描述\" />\n        <p>奶茶果汁</p>\n      </div>\n      <div class=\"foodTypeItem\">\n        <img :src=\"require('@/assets/img/TypeImg/class15.jpg')\" alt=\"描述\" />\n        <p>全部</p>\n      </div>\n    </div>\n    <!-- <button class=\"banner-carousel-btn prev\">Prev</button>\n    <button class=\"banner-carousel-btn next\">Next</button> -->\n    <!--广告部分-->\n    <div class=\"banner-carousel\">\n      <div\n          v-for=\"(ad, index) in ads\"\n          :key=\"ad.key\"\n          class=\"banner\"\n          :class=\"{ active: index === currentAdIndex }\"\n      >\n        <div class=\"banner-left\">\n          <div class=\"banner-left-top\">\n            <img :src=\"ad.leftTopImg\" alt=\"描述\" height=\"27\" width=\"27\"/>\n            <p class=\"e-little-baby\">饿小宝</p>\n            <p style=\"font-weight: bold;font-size:3.3vw;margin-top: 1vw;margin-left: 20px\">{{ ad.leftText }}</p>\n          </div>\n          <div class=\"banner-left-content\">\n            <div class=\"banner-left-content-left\">\n              <p class=\"banner-left-text\">{{ ad.leftContentLeftText }}</p>\n              <div class=\"banner-left-content-left-button\">去看看</div>\n            </div>\n            <div class=\"banner-left-content-right\">\n              <img :src=\"ad.leftContentRightImg\" alt=\"描述\" />\n            </div>\n          </div>\n        </div>\n        <div class=\"banner_right\">\n          <div v-for=\"item in ad.rightItems\" :key=\"item.title\" class=\"banner_right_item\">\n            <p class=\"title\">{{ item.title }}</p>\n            <img :src=\"item.img\" alt=\"描述\" />\n            <p class=\"remark\">{{ item.remark }}</p>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"recommend\">\n      <div>\n        <p style=\"color: #17BAF9;font-weight: bolder\">换换口味</p>\n      </div>\n      <div>\n        <p>天天爆红包</p>\n      </div>\n      <div>\n        <p>减配送费</p>\n      </div>\n      <div>\n        <p>学生特价</p>\n      </div>\n      <div>\n        <i class=\"fa-solid fa-bars\"></i>\n      </div>\n    </div>\n    <div class=\"foodList\" @scroll=\"handleScroll\">\n      <div class=\"food\" v-for=\"business in businesses\" :key=\"business.businessId\" @click=\"goToBusinessList(business)\">\n        <img :src=\"processImageUrl(business.businessImg)\" \n             alt=\"商家图片\" \n             height=\"120\" \n             width=\"110\"\n             @error=\"handleImageError\" />\n        <div class=\"business-into\">\n          <div class=\"business-h\">\n            <h>{{ business.businessName }}</h>\n          </div>\n          <div class=\"star\">\n            <p style=\"color: #ff4a25;font-weight: bolder;font-size: 14px\">{{ business.starPrice }}分</p>\n            <p2>月售{{ business.orderTypeId }}+</p2>\n            <p3>{{ business.deliveryPrice }}分钟 {{ business.distance }}km</p3>\n          </div>\n          <div class=\"for-sell\">\n            <p1>起送费: {{ business.starPrice }}元</p1>\n            <p2>配送费: {{ business.deliveryPrice }}元</p2>\n          </div>\n          <!-- 根据实际业务需求添加更多信息 -->\n        </div>\n      </div>\n      <!-- 加载更多提示 -->\n      <div v-if=\"!hasMore\" class=\"load-more\">\n        已加载全部商家\n      </div>\n    </div>\n\n    <div>&nbsp;</div>\n    <div class=\"footer\">\n      <div>\n        <i class=\"fa-solid fa-bowl-rice\"></i>\n        <p>首页</p>\n      </div>\n      <div>\n        <i class=\"fa-solid fa-store\"></i>\n        <p>全能超市</p>\n      </div>\n      <div @click=\"goToHistory\">\n        <i class=\"fa-solid fa-comment-dots\"></i>\n        <p>历史消息</p>\n      </div>\n      <div>\n        <i class=\"fa-solid fa-cart-plus\"></i>\n        <p>购物车</p>\n      </div>\n      <div @click=\"goToLogin\">\n        <i class=\"fa-solid fa-user\"></i>\n        <p>登录</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { useRouter } from 'vue-router';\nimport { businessApi } from '@/utils/api';\nimport axios from 'axios';\n\nconst businesses = ref([]);\nconst page = ref(1);\nconst pageSize = ref(10);\nconst hasMore = ref(true);\nconst router = useRouter();\nimport { ref, onMounted, onUnmounted } from 'vue';\n\n// 定义广告数据\nconst ads = ref([\n  {\n    key: 'ad1',\n    leftTopImg: require('@/assets/img/TypeImg/img.png'),\n    leftText: '在线订单',\n    leftContentLeftText: '一起来嗨',\n    leftContentRightImg: require('@/assets/img/TypeImg/bannerLeft-Right.png'),\n    rightItems: [\n      { title: '晚餐聚会', img: require('@/assets/img/BannerImg/banner01.jpg'), remark: '一起吃好点' },\n      { title: '学生福利', img: require('@/assets/img/BannerImg/banner02.jpg'), remark: '最高省912' },\n    ],\n  },\n  {\n    key: 'ad2',\n    leftTopImg: require('@/assets/img/BannerImg/banner04.jpg'),\n    leftText: '深夜食堂',\n    leftContentLeftText: '都是好吃的',\n    leftContentRightImg: require('@/assets/img/BannerImg/banner03.jpg'),\n    rightItems: [\n      { title: '深夜解馋', img: require('@/assets/img/BannerImg/img_5.jpg'), remark: '领夜宵红包' },\n      { title: '学生福利', img: require('@/assets/img/BannerImg/img_6.jpg'), remark: '最高省912' },\n    ],\n  },\n]);\n\nconst currentAdIndex = ref(0); // 当前激活的广告索引\n\nconst switchAd = () => {\n  currentAdIndex.value = (currentAdIndex.value + 1) % ads.value.length;\n};\n\nonMounted(() => {\n  const intervalId = setInterval(switchAd, 3000); // 每3秒切换一次广告\n  onUnmounted(() => {\n    clearInterval(intervalId);\n  });\n});\nconst fetchBusinesses = async () => {\n  try {\n    console.log('开始获取商家列表，页码:', page.value, '每页数量:', pageSize.value);\n    \n    // 使用正确的参数格式调用API\n    const response = await businessApi.getBusinessList({\n      page: page.value,\n      size: pageSize.value\n    });\n    \n    console.log('商家列表响应:', response);\n    \n    // 检查响应结构\n    if (response && response.data) {\n      // 检查response.data是否是我们需要的数据\n      let businessData;\n      \n      // 如果response.data是数组，直接使用\n      if (Array.isArray(response.data)) {\n        businessData = response.data;\n      } \n      // 如果response.data是对象，可能包含一个data或content字段\n      else if (response.data.data && Array.isArray(response.data.data)) {\n        businessData = response.data.data;\n      }\n      else if (response.data.content && Array.isArray(response.data.content)) {\n        businessData = response.data.content;\n      }\n      // 如果response.data.records存在（可能是分页对象）\n      else if (response.data.records && Array.isArray(response.data.records)) {\n        businessData = response.data.records;\n      }\n      // 如果response.data.list存在\n      else if (response.data.list && Array.isArray(response.data.list)) {\n        businessData = response.data.list;\n      }\n      // 如果response.data本身是对象，但不包含数组数据，可能是单个商家\n      else if (typeof response.data === 'object' && !Array.isArray(response.data)) {\n        businessData = [response.data];\n      }\n      \n      // 如果找到了商家数据\n      if (businessData && businessData.length > 0) {\n        console.log('找到商家数据:', businessData);\n        \n        if (businessData.length < pageSize.value) {\n          hasMore.value = false;\n        }\n        \n        // 添加调试日志，检查每个商家对象\n        businessData.forEach((business, index) => {\n          console.log(`商家 ${index + 1}:`, business);\n        });\n        \n        businesses.value.push(...businessData);\n      } else {\n        console.log('未找到商家数据或数据为空');\n        hasMore.value = false;\n      }\n    } else {\n      console.error('获取商家列表失败: 响应格式不正确', response);\n    }\n  } catch (error) {\n    console.error('获取商家列表失败:', error);\n  }\n};\n\nconst handleScroll = () => {\n  const scrollElement = 