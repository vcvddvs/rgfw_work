{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted, computed } from 'vue';\nimport { useRoute } from 'vue-router';\nimport router from \"@/router\";\nimport { v4 as uuidv4 } from 'uuid';\nimport { ElMessage } from 'element-plus';\nimport { businessApi, cartApi } from '@/utils/api';\nexport default {\n  __name: 'businesslist2Component',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const businessImg = ref(route.params.businessImg);\n    const businessId = ref(route.params.businessId);\n    const businessName = ref(route.params.businessName || '默认店铺名称');\n    const foods = ref([]);\n    const cart = ref({});\n    const businessDetails = ref({});\n    const showDebug = ref(false);\n    console.log(businessId.value);\n\n    // 获取商家详情\n    const fetchBusinessDetails = async () => {\n      try {\n        console.log('开始获取商家详情，商家ID:', businessId.value);\n        const response = await businessApi.getBusinessById(businessId.value);\n        console.log('原始商家详情响应:', JSON.stringify(response));\n        if (response && response.code === 1 && response.data) {\n          businessDetails.value = response.data;\n          console.log('获取到的商家详情:', JSON.stringify(businessDetails.value));\n\n          // 确保商家图片可用\n          if (businessDetails.value.businessImg) {\n            console.log('商家图片可用:', businessDetails.value.businessImg.substring(0, 30) + '...');\n          } else {\n            console.log('商家图片不可用');\n          }\n        } else if (response && response.data) {\n          businessDetails.value = response.data;\n          console.log('获取到的商家详情(直接对象):', JSON.stringify(businessDetails.value));\n        } else {\n          console.error('获取商家详情失败: 返回数据格式不正确', response);\n        }\n      } catch (error) {\n        console.error('获取商家详情失败:', error);\n      }\n    };\n    const fetchFoods = async () => {\n      try {\n        console.log('开始获取菜品数据，商家ID:', businessId.value);\n        const response = await businessApi.getFoodsByBusinessId(businessId.value);\n        console.log('原始菜品响应:', JSON.stringify(response));\n\n        // 直接使用后端返回的数据，不做任何修改或添加默认值\n        if (response && response.code === 1 && Array.isArray(response.data)) {\n          console.log('菜品数据格式: Result<List<Food>>');\n          foods.value = response.data;\n          console.log('使用原始菜品数据:', foods.value);\n        } else if (Array.isArray(response)) {\n          console.log('菜品数据格式: List<Food>');\n          foods.value = response;\n          console.log('使用原始菜品数据(直接数组):', foods.value);\n        } else {\n          console.error('获取菜单数据失败: 返回数据格式不正确', response);\n        }\n      } catch (error) {\n        console.error('获取菜单数据失败:', error);\n      }\n    };\n    onMounted(() => {\n      fetchBusinessDetails();\n      fetchFoods();\n    });\n    function addToCart(itemName, price, photo, foodId, quantity) {\n      if (cart.value[itemName]) {\n        cart.value[itemName].quantity += quantity;\n      } else {\n        cart.value[itemName] = {\n          price: price,\n          photo: photo,\n          quantity: quantity,\n          foodId: foodId\n        };\n      }\n    }\n    // 计算购物车中商品的总数量\n    const totalQuantity = computed(() => {\n      return Object.values(cart.value).reduce((sum, item) => sum + item.quantity, 0);\n    });\n\n    // 如果商品数量减至0，则从购物车中移除\n    function decrementQuantity(itemName) {\n      if (cart.value[itemName] && cart.value[itemName].quantity > 0) {\n        cart.value[itemName].quantity -= 1;\n      }\n    }\n    const totalPrice = computed(() => {\n      return Object.values(cart.value).reduce((sum, item) => sum + item.quantity * item.price, 0);\n    });\n    function goToPayment() {\n      if (Object.keys(cart.value).length === 0) {\n        ElMessage.warning('购物车为空，请先添加商品！');\n        return;\n      }\n\n      // 从localStorage获取用户ID\n      const userId = localStorage.getItem('userId');\n      if (!userId) {\n        ElMessage.warning('请先登录！');\n        router.push('/login-component');\n        return;\n      }\n      const orderInfo = {\n        businessId: businessId.value,\n        businessName: businessName.value,\n        cart: cart.value\n      };\n      localStorage.setItem('orderInfo', JSON.stringify(orderInfo));\n      // 使用 uuidv4 生成订单ID\n      const orderId = uuidv4();\n\n      // 构建一个只包含 Cart 对象的数组\n      const carts = Object.values(cart.value).map(item => {\n        return {\n          foodId: item.foodId,\n          businessId: businessId.value,\n          userId: userId,\n          quantity: item.quantity,\n          orderId: orderId\n        };\n      });\n      cartApi.checkout(carts).then(data => {\n        console.log('Checkout successful:', data);\n        router.push({\n          name: 'Order',\n          params: {\n            businessName: businessName.value,\n            businessId: businessId.value\n          },\n          query: {\n            cart: JSON.stringify(cart.value)\n          }\n        });\n        ElMessage.success('结算成功！');\n      }).catch(error => {\n        console.error('Checkout error:', error);\n        ElMessage.error('结算失败，请重试');\n      });\n    }\n    function goBack() {\n      router.push('/Index-Component');\n    }\n    function handleImageError(event) {\n      event.target.src = require('@/assets/img/BUSINESS/menus/img.png');\n    }\n\n    // 处理图片URL，确保能正确显示\n    function processImageUrl(url) {\n      console.log('处理图片URL类型:', typeof url);\n\n      // 如果URL为空，返回默认图片\n      if (!url) {\n        console.log('图片URL为空，使用默认图片');\n        return require('@/assets/img/BUSINESS/menus/img.png');\n      }\n\n      // 如果URL是字符串类型\n      if (typeof url === 'string') {\n        // 如果已经是完整的URL或data:image格式，直接返回\n        if (url.startsWith('http') || url.startsWith('data:')) {\n          console.log('使用原始图片URL(Base64或HTTP)');\n          return url;\n        }\n\n        // 尝试作为相对路径处理\n        try {\n          console.log('尝试作为相对路径处理:', url);\n          return require('@/assets/img/BUSINESS/menus/img.png');\n        } catch (e) {\n          console.log('图片路径处理失败，使用默认图片');\n          return require('@/assets/img/BUSINESS/menus/img.png');\n        }\n      }\n\n      // 如果URL是对象或其他类型，返回默认图片\n      console.log('图片URL类型不是字符串，使用默认图片');\n      return require('@/assets/img/BUSINESS/menus/img.png');\n    }\n\n    // 处理价格显示\n    function formatPrice(price) {\n      console.log('处理价格:', price, typeof price);\n      if (price === undefined || price === null) {\n        console.log('价格为空，返回0.00');\n        return '0.00';\n      }\n\n      // 确保price是数字\n      const numPrice = Number(price);\n      if (isNaN(numPrice)) {\n        console.log('价格不是数字，返回0.00');\n        return '0.00';\n      }\n      return numPrice.toFixed(2);\n    }\n    function toggleDebug() {\n      showDebug.value = !showDebug.value;\n    }\n    const __returned__ = {\n      route,\n      businessImg,\n      businessId,\n      businessName,\n      foods,\n      cart,\n      businessDetails,\n      showDebug,\n      fetchBusinessDetails,\n      fetchFoods,\n      addToCart,\n      totalQuantity,\n      decrementQuantity,\n      totalPrice,\n      goToPayment,\n      goBack,\n      handleImageError,\n      processImageUrl,\n      formatPrice,\n      toggleDebug,\n      ref,\n      onMounted,\n      computed,\n      get useRoute() {\n        return useRoute;\n      },\n      get router() {\n        return router;\n      },\n      get uuidv4() {\n        return uuidv4;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get businessApi() {\n        return businessApi;\n      },\n      get cartApi() {\n        return cartApi;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","useRoute","router","v4","uuidv4","ElMessage","businessApi","cartApi","route","businessImg","params","businessId","businessName","foods","cart","businessDetails","showDebug","console","log","value","fetchBusinessDetails","response","getBusinessById","JSON","stringify","code","data","substring","error","fetchFoods","getFoodsByBusinessId","Array","isArray","addToCart","itemName","price","photo","foodId","quantity","totalQuantity","Object","values","reduce","sum","item","decrementQuantity","totalPrice","goToPayment","keys","length","warning","userId","localStorage","getItem","push","orderInfo","setItem","orderId","carts","map","checkout","then","name","query","success","catch","goBack","handleImageError","event","target","src","require","processImageUrl","url","startsWith","e","formatPrice","undefined","numPrice","Number","isNaN","toFixed","toggleDebug"],"sources":["D:/back_elm/vue_elm/src/components/businesslist2Component.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div class=\"header\">\n      <div class=\"title\">\n        <div class=\"img1\" @click=\"goBack\">\n          <img :src=\"require('@/assets/img/BUSINESS/img_1.png')\" height=\"22\" width=\"13\"/>\n        </div>\n        <div class=\"img2\">\n          <img :src=\"require('@/assets/img/BUSINESS/img.png')\" height=\"auto\" width=\"100%\"/>\n        </div>\n      </div>\n      <div class=\"header-all\">\n        <div class=\"header-text\">\n          <div class=\"text-left-header\">\n            <h>{{businessName}}</h>\n          </div>\n          <div class=\"star\">\n            <p1>{{ businessDetails.starPrice || '暂无' }}分 </p1>\n            <p2> 蜂鸟准时达</p2>\n            <p3>约{{ businessDetails.deliveryPrice || '暂无' }}分钟  月售{{ businessDetails.orderTypeId || '0' }}+</p3>\n          </div>\n          <div class=\"ticket\">\n            <img :src=\"require('@/assets/img/BannerImg/img_1.png')\" height=\"23\" width=\"100\"/>\n          </div>\n          <div class=\"for-sell\">\n            <p1>起送费: {{ businessDetails.starPrice || '暂无' }}元</p1>\n            <p2>配送费: {{ businessDetails.deliveryPrice || '暂无' }}元</p2>\n          </div>\n        </div>\n        <div class=\"text-picture\">\n          <img :src=\"processImageUrl(businessImg)\" height=\"155\" width=\"157\"/>\n        </div>\n      </div>\n      <div class=\"banner\">\n        <img :src=\"require('@/assets/img/BannerImg/img_3.png')\" height=\"15\" width=\"100\"/>\n        <p1>{{ businessDetails.businessExplain || '商家说明' }}</p1>\n        <p2>{{ businessDetails.remarks || '每日更新' }}</p2>\n      </div>\n    </div>\n    <div class=\"body\">\n      <!-- ... 其他代码 ... -->\n      <div class=\"recommend\">\n        <div class=\"recommend-left\">\n          <div>\n            <p1>点餐</p1>\n          </div>\n          <div>\n            <p2>评价</p2>\n          </div>\n          <div>\n            <p3>品牌</p3>\n          </div>\n        </div>\n        <div class=\"recommend-right\">\n          <p>好友拼单</p>\n        </div>\n      </div>\n\n      <div class=\"foodList\">\n        <div class=\"foodList-left\">\n          <div class=\"title0\">\n            <img :src=\"require('@/assets/img/BUSINESS/title/img.png')\" height=\"26\" width=\"30\"/>\n            <p> 学生专享</p>\n          </div>\n          <div class=\"title1\">\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_1.png')\" height=\"26\" width=\"30\"/>\n            <p>热销</p>\n          </div>\n          <div class=\"title1\">\n            <img :src=\"require('@/assets/img/BUSINESS/title/img.png')\" height=\"26\" width=\"30\"/>\n            <p> 优惠</p>\n          </div>\n          <div class=\"title2\">\n            <p>热销必点</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_2.png')\" height=\"16\" width=\"46\"/>\n          </div>\n          <div class=\"title2\">\n            <p> 新品尝鲜</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_3.png')\" height=\"13\" width=\"42\"/>\n          </div>\n          <div class=\"title2\">\n            <p>招牌全鸡</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_4.png')\" height=\"20\" width=\"42\"/>\n          </div>\n          <div class=\"title2\">\n            <p>休闲小吃</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_5.png')\" height=\"22\" width=\"39\"/>\n          </div>\n          <div class=\"title2\">\n            <p>卷堡主食</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_6.png')\" height=\"20\" width=\"42\"/>\n          </div>\n          <div class=\"title2\">\n            <p>饮品专区</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_7.png')\" height=\"20\" width=\"40\"/>\n          </div>\n          <div class=\"title2\">\n            <p>调料专区</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_8.png')\" height=\"18\" width=\"40\"/>\n          </div>\n          <div class=\"title2\">\n            <p>品牌活动</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_9.png')\" height=\"19\" width=\"41\"/>\n          </div>\n\n\n        </div>\n        <div class=\"foodList-right\">\n          <div class=\"menus\" v-for=\"food in foods\" :key=\"food.foodId\">\n\n            <div class=\"menus-body\">\n              <div class=\"menus-body-left\">\n                <img :src=\"processImageUrl(food.foodImg)\" \n                     height=\"124\" \n                     width=\"150\"\n                     @error=\"handleImageError\" />\n              </div>\n              <div class=\"menus-body-right\">\n                <div class=\"mbr-title\">\n                  <p1>{{ food.foodName || '未命名菜品' }}</p1>\n                </div>\n                <div class=\"mbr-banner\">\n                  <p2>{{ food.foodExplain || '暂无说明' }}</p2>\n                </div>\n                <div class=\"mbr-count\">\n                  <p3>{{ food.remarks || '暂无销售数据' }}</p3>\n                </div>\n                <div class=\"mbr-sell\" v-if=\"food.remarks && food.remarks.includes('折')\">\n                  <p4>{{ food.remarks }}</p4>\n                  <p5>优惠价</p5>\n                </div>\n                <div class=\"mbr-price\">\n                  <p>&#165;{{ formatPrice(food.foodPrice) }}</p>\n                  <div class=\"cart-actions\">\n                    <img\n                        v-if=\"cart[food.foodName] && cart[food.foodName].quantity > 0\"\n                        :src=\"require('@/assets/img/BUSINESS/menus/img_4.png')\"\n                        height=\"27\"\n                        width=\"28\"\n                        @click=\"decrementQuantity(food.foodName)\"\n                    />\n                    <span v-if=\"cart[food.foodName] && cart[food.foodName].quantity > 0\">{{ cart[food.foodName].quantity }}</span>\n                    <img\n                        :src=\"require('@/assets/img/BUSINESS/menus/img_1.png')\"\n                        height=\"27\"\n                        width=\"28\"\n                        @click=\"addToCart(food.foodName, food.foodPrice,food.foodImg,food.foodId,1)\"\n                    />\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n\n        </div>\n\n      <div class=\"footer\">\n        <!-- ... 其他代码 ... -->\n        <div class=\"cart-left\">\n          <div class=\"cart-left-icon\">\n            <i class=\"fa fa-shopping-cart\"></i>\n          </div>\n          <!-- 当购物车中有商品时显示数量 -->\n          <div class=\"car-left-icon-quantity\" v-if=\"totalQuantity > 0\">{{ totalQuantity }}</div>\n          <div style=\"margin-top: 15px\">价格: {{ totalPrice }}元</div>\n        </div>\n        <div class=\"cart-right\">\n          <div class=\"cart-right-item\" @click=\"goToPayment\">去结算</div>\n        </div>\n      </div>\n      \n      <!-- 调试信息，仅在开发环境显示 -->\n      <div v-if=\"showDebug\" style=\"margin-top: 20px; padding: 10px; background-color: #f5f5f5; border: 1px solid #ddd;\">\n        <h3>调试信息</h3>\n        <button @click=\"toggleDebug\">隐藏调试信息</button>\n        <div>\n          <h4>商家信息:</h4>\n          <pre>{{ JSON.stringify(businessDetails, null, 2) }}</pre>\n        </div>\n        <div>\n          <h4>菜品信息:</h4>\n          <pre>{{ JSON.stringify(foods, null, 2) }}</pre>\n        </div>\n      </div>\n      <div v-else style=\"margin-top: 20px; text-align: center;\">\n        <button @click=\"toggleDebug\">显示调试信息</button>\n      </div>\n  </div>\n\n</template>\n\n\n<script setup>\nimport {ref, onMounted, computed} from 'vue';\nimport { useRoute } from 'vue-router';\nimport router from \"@/router\";\nimport { v4 as uuidv4 } from 'uuid';\nimport { ElMessage } from 'element-plus';\nimport { businessApi, cartApi } from '@/utils/api';\n\nconst route = useRoute();\nconst businessImg=ref(route.params.businessImg);\nconst businessId = ref(route.params.businessId);\nconst businessName = ref(route.params.businessName || '默认店铺名称')\nconst foods = ref([]);\nconst cart = ref({});\nconst businessDetails = ref({});\nconst showDebug = ref(false);\n\nconsole.log(businessId.value);\n\n// 获取商家详情\nconst fetchBusinessDetails = async () => {\n  try {\n    console.log('开始获取商家详情，商家ID:', businessId.value);\n    const response = await businessApi.getBusinessById(businessId.value);\n    console.log('原始商家详情响应:', JSON.stringify(response));\n    \n    if (response && response.code === 1 && response.data) {\n      businessDetails.value = response.data;\n      console.log('获取到的商家详情:', JSON.stringify(businessDetails.value));\n      \n      // 确保商家图片可用\n      if (businessDetails.value.businessImg) {\n        console.log('商家图片可用:', businessDetails.value.businessImg.substring(0, 30) + '...');\n      } else {\n        console.log('商家图片不可用');\n      }\n    } else if (response && response.data) {\n      businessDetails.value = response.data;\n      console.log('获取到的商家详情(直接对象):', JSON.stringify(businessDetails.value));\n    } else {\n      console.error('获取商家详情失败: 返回数据格式不正确', response);\n    }\n  } catch (error) {\n    console.error('获取商家详情失败:', error);\n  }\n};\n\nconst fetchFoods = async () => {\n  try {\n    console.log('开始获取菜品数据，商家ID:', businessId.value);\n    const response = await businessApi.getFoodsByBusinessId(businessId.value);\n    console.log('原始菜品响应:', JSON.stringify(response));\n    \n    // 直接使用后端返回的数据，不做任何修改或添加默认值\n    if (response && response.code === 1 && Array.isArray(response.data)) {\n      console.log('菜品数据格式: Result<List<Food>>');\n      foods.value = response.data;\n      console.log('使用原始菜品数据:', foods.value);\n    } else if (Array.isArray(response)) {\n      console.log('菜品数据格式: List<Food>');\n      foods.value = response;\n      console.log('使用原始菜品数据(直接数组):', foods.value);\n    } else {\n      console.error('获取菜单数据失败: 返回数据格式不正确', response);\n    }\n  } catch (error) {\n    console.error('获取菜单数据失败:', error);\n  }\n};\n\nonMounted(() => {\n  fetchBusinessDetails();\n  fetchFoods();\n});\n\nfunction addToCart(itemName, price, photo, foodId, quantity) {\n  if (cart.value[itemName]) {\n    cart.value[itemName].quantity += quantity;\n  } else {\n    cart.value[itemName] = { price: price, photo: photo, quantity: quantity, foodId: foodId };\n  }\n}\n// 计算购物车中商品的总数量\nconst totalQuantity = computed(() => {\n  return Object.values(cart.value).reduce((sum, item) => sum + item.quantity, 0);\n});\n\n// 如果商品数量减至0，则从购物车中移除\nfunction decrementQuantity(itemName) {\n  if (cart.value[itemName] && cart.value[itemName].quantity > 0) {\n    cart.value[itemName].quantity -= 1;\n  }\n}\n\n\nconst totalPrice = computed(() => {\n  return Object.values(cart.value).reduce((sum, item) => sum + item.quantity * item.price, 0);\n});\n\nfunction goToPayment() {\n  if (Object.keys(cart.value).length === 0) {\n    ElMessage.warning('购物车为空，请先添加商品！');\n    return;\n  }\n\n  // 从localStorage获取用户ID\n  const userId = localStorage.getItem('userId');\n  if (!userId) {\n    ElMessage.warning('请先登录！');\n    router.push('/login-component');\n    return;\n  }\n\n  const orderInfo = {\n    businessId: businessId.value,\n    businessName: businessName.value,\n    cart: cart.value\n  };\n\n  localStorage.setItem('orderInfo', JSON.stringify(orderInfo));\n  // 使用 uuidv4 生成订单ID\n  const orderId = uuidv4();\n\n  // 构建一个只包含 Cart 对象的数组\n  const carts = Object.values(cart.value).map(item => {\n    return {\n      foodId: item.foodId,\n      businessId: businessId.value,\n      userId: userId,\n      quantity: item.quantity,\n      orderId: orderId\n    };\n  });\n\n  cartApi.checkout(carts)\n    .then(data => {\n      console.log('Checkout successful:', data);\n      router.push({\n        name: 'Order',\n        params: { \n          businessName: businessName.value, \n          businessId: businessId.value \n        },\n        query: { cart: JSON.stringify(cart.value) }\n      });\n      ElMessage.success('结算成功！');\n    })\n    .catch(error => {\n      console.error('Checkout error:', error);\n      ElMessage.error('结算失败，请重试');\n    });\n}\n\n\nfunction goBack() {\n  router.push('/Index-Component');\n}\n\nfunction handleImageError(event) {\n  event.target.src = require('@/assets/img/BUSINESS/menus/img.png');\n}\n\n// 处理图片URL，确保能正确显示\nfunction processImageUrl(url) {\n  console.log('处理图片URL类型:', typeof url);\n  \n  // 如果URL为空，返回默认图片\n  if (!url) {\n    console.log('图片URL为空，使用默认图片');\n    return require('@/assets/img/BUSINESS/menus/img.png');\n  }\n  \n  // 如果URL是字符串类型\n  if (typeof url === 'string') {\n    // 如果已经是完整的URL或data:image格式，直接返回\n    if (url.startsWith('http') || url.startsWith('data:')) {\n      console.log('使用原始图片URL(Base64或HTTP)');\n      return url;\n    }\n    \n    // 尝试作为相对路径处理\n    try {\n      console.log('尝试作为相对路径处理:', url);\n      return require('@/assets/img/BUSINESS/menus/img.png');\n    } catch (e) {\n      console.log('图片路径处理失败，使用默认图片');\n      return require('@/assets/img/BUSINESS/menus/img.png');\n    }\n  }\n  \n  // 如果URL是对象或其他类型，返回默认图片\n  console.log('图片URL类型不是字符串，使用默认图片');\n  return require('@/assets/img/BUSINESS/menus/img.png');\n}\n\n// 处理价格显示\nfunction formatPrice(price) {\n  console