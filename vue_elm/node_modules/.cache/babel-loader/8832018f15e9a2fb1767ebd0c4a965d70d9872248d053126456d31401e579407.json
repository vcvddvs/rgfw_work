{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { orderApi } from '@/utils/api';\nimport { getToken, hasValidToken } from '@/utils/auth';\nconst baseUrl = 'http://localhost:8090'; // 添加基础URL用于图片路径\n\nexport default {\n  __name: 'historyComponent',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const orders = ref([]);\n    const userId = ref(localStorage.getItem('userId') || '');\n    const showLoginPrompt = ref(false);\n    onMounted(() => {\n      fetchOrders();\n    });\n    const goBack = () => {\n      router.push('/Index-Component');\n    };\n    const goToLogin = () => {\n      router.push('/login-component');\n    };\n\n    // 处理图片加载错误\n    const handleImgError = event => {\n      // 使用默认图片替换加载失败的图片\n      event.target.src = require('@/assets/img/sp01.png');\n    };\n\n    // 处理商家图片加载错误\n    const handleBusinessImgError = event => {\n      // 使用默认商家图片替换加载失败的图片\n      event.target.src = require('@/assets/img/sj01.png');\n    };\n    const getMockOrders = () => {\n      console.log('使用模拟订单数据');\n      return [{\n        orderId: 1,\n        businessName: '测试商家1',\n        businessImg: require('@/assets/img/sj01.png'),\n        orderTotal: 35.5,\n        deliveryPrice: 5,\n        foodImgs: [require('@/assets/img/business2/food/img.png'), require('@/assets/img/business2/food/img_1.png')],\n        quantities: [1, 2],\n        orderDate: '2023-06-29 12:30:45'\n      }, {\n        orderId: 2,\n        businessName: '测试商家2',\n        businessImg: require('@/assets/img/sj02.png'),\n        orderTotal: 42.8,\n        deliveryPrice: 5,\n        foodImgs: [require('@/assets/img/business2/food/img_2.png')],\n        quantities: [3],\n        orderDate: '2023-06-28 18:15:22'\n      }];\n    };\n    const fetchOrders = async () => {\n      // 检查是否有有效的token和userId\n      if (!hasValidToken() || !userId.value) {\n        console.log('用户未登录或token无效，显示登录提示');\n        showLoginPrompt.value = true;\n        orders.value = getMockOrders(); // 仍然显示模拟数据\n        return;\n      }\n      try {\n        console.log('获取用户历史订单，userId:', userId.value);\n        console.log('当前token:', getToken());\n        const response = await orderApi.getOrdersByUserId(userId.value);\n        console.log('Order response:', response);\n        if (response && response.code === 1 && Array.isArray(response.data)) {\n          orders.value = response.data.map(order => {\n            // 处理商家图片路径\n            let businessImg = order.businessImg;\n            if (businessImg && !businessImg.startsWith('http') && !businessImg.startsWith('data:') && !businessImg.startsWith('/')) {\n              businessImg = require(`@/assets/img/${businessImg}`);\n            } else if (businessImg && businessImg.startsWith('/')) {\n              businessImg = `${baseUrl}${businessImg}`;\n            } else if (!businessImg) {\n              businessImg = require('@/assets/img/sj01.png');\n            }\n\n            // 处理食品图片路径\n            let foodImgsArray = [];\n            if (Array.isArray(order.foodImgs)) {\n              foodImgsArray = order.foodImgs;\n            } else if (order.foodImgs) {\n              foodImgsArray = order.foodImgs.split(',');\n            }\n\n            // 转换食品图片路径\n            const processedFoodImgs = foodImgsArray.map(img => {\n              if (!img) return require('@/assets/img/business2/food/img.png');\n              if (img.startsWith('http') || img.startsWith('data:')) return img;\n              if (img.startsWith('/')) return `${baseUrl}${img}`;\n              try {\n                return require(`@/assets/img/${img}`);\n              } catch (e) {\n                return require('@/assets/img/business2/food/img.png');\n              }\n            });\n\n            // 处理数量\n            const quantities = Array.isArray(order.quantities) ? order.quantities : order.quantities ? order.quantities.split(',').map(q => parseInt(q, 10)) : [];\n            return {\n              ...order,\n              businessImg,\n              foodImgs: processedFoodImgs,\n              quantities\n            };\n          });\n        } else {\n          console.error('获取订单响应格式错误:', response);\n          orders.value = getMockOrders();\n        }\n      } catch (error) {\n        console.error('Error fetching orders:', error);\n\n        // 如果是401错误，显示登录提示\n        if (error.response && error.response.status === 401) {\n          console.log('认证失败，显示登录提示');\n          showLoginPrompt.value = true;\n        }\n\n        // 无论如何都显示模拟数据\n        orders.value = getMockOrders();\n      }\n    };\n    const __returned__ = {\n      router,\n      orders,\n      userId,\n      showLoginPrompt,\n      baseUrl,\n      goBack,\n      goToLogin,\n      handleImgError,\n      handleBusinessImgError,\n      getMockOrders,\n      fetchOrders,\n      ref,\n      onMounted,\n      get useRouter() {\n        return useRouter;\n      },\n      get orderApi() {\n        return orderApi;\n      },\n      get getToken() {\n        return getToken;\n      },\n      get hasValidToken() {\n        return hasValidToken;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRouter","orderApi","getToken","hasValidToken","baseUrl","router","orders","userId","localStorage","getItem","showLoginPrompt","fetchOrders","goBack","push","goToLogin","handleImgError","event","target","src","require","handleBusinessImgError","getMockOrders","console","log","orderId","businessName","businessImg","orderTotal","deliveryPrice","foodImgs","quantities","orderDate","value","response","getOrdersByUserId","code","Array","isArray","data","map","order","startsWith","foodImgsArray","split","processedFoodImgs","img","e","q","parseInt","error","status"],"sources":["D:/back_elm/vue_elm/src/components/historyComponent.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div class=\"header\">\n      <div class=\"search\">\n        <div class=\"search-bottom\">\n          <div class=\"back\">\n            <span class=\"iconfont icon-back\" @click=\"goBack\"></span>\n          </div>\n          <div class=\"search-pt\">\n            <span class=\"iconfont icon-sousuo\" style=\"margin-left: 2vw\"></span>\n            <input style=\"flex:6;\" type=\"text\" placeholder=\"历史信息搜索\"/>\n          </div>\n          <div class=\"box\">\n            <span class=\"iconfont icon-gongjuxiang\"></span>\n            <span class=\"iconfont icon-24gf-ellipsis\"></span>\n          </div>\n        </div>\n        <div class=\"information\">\n          <p>全部</p>\n          <p>进行中</p>\n          <p>待评价</p>\n          <p>退款</p>\n        </div>\n      </div>\n    </div>\n    <div class=\"body\" style=\"margin-top: 25vw\">\n      <div v-if=\"showLoginPrompt\" class=\"login-prompt\">\n        <p>请先登录查看您的历史订单</p>\n        <button @click=\"goToLogin\" class=\"login-btn\">去登录</button>\n      </div>\n      <div v-else-if=\"orders.length === 0\" class=\"no-orders\">\n        <p>暂无历史订单</p>\n      </div>\n      <div v-else v-for=\"order in orders\" :key=\"order.orderId\" class=\"food\">\n        <div class=\"food-title\">\n          <div class=\"title-left\">\n            <img :src=\"order.businessImg\" height=\"52\" width=\"52\" @error=\"handleBusinessImgError\"/>\n          </div>\n          <div class=\"title-right\">\n            <div class=\"t-r-1\">\n              <div class=\"t-r-1-nb1\">\n                <p>{{ order.businessName }}</p>\n              </div>\n              <div class=\"t-r-1-nb2\">\n                <p>&#165;{{ order.orderTotal }}</p>\n              </div>\n            </div>\n            <div class=\"t-r-2\">\n              <p>{{ order.deliveryPrice }}元无门槛</p>\n            </div>\n          </div>\n        </div>\n        <div class=\"food-body\">\n          <div class=\"picture\" style=\"display: flex; flex-wrap: wrap; gap: 10px;\">\n            <div v-for=\"(img, index) in order.foodImgs\" :key=\"index\" class=\"food-item\" style=\"position: relative;\">\n              <img :src=\"img\" height=\"88\" width=\"88\" @error=\"handleImgError\"/>\n              <div class=\"quantity\">{{ order.quantities[index] }}</div>\n            </div>\n          </div>\n        </div>\n        <div class=\"time\">\n          <p>{{ order.orderDate }}</p>\n        </div>\n        <div class=\"one-more\">\n          <p>再来一单</p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { orderApi } from '@/utils/api';\nimport { getToken, hasValidToken } from '@/utils/auth';\n\nconst router = useRouter();\nconst orders = ref([]);\nconst userId = ref(localStorage.getItem('userId') || '');\nconst showLoginPrompt = ref(false);\nconst baseUrl = 'http://localhost:8090'; // 添加基础URL用于图片路径\n\nonMounted(() => {\n  fetchOrders();\n});\n\nconst goBack = () => {\n  router.push('/Index-Component');\n};\n\nconst goToLogin = () => {\n  router.push('/login-component');\n};\n\n// 处理图片加载错误\nconst handleImgError = (event) => {\n  // 使用默认图片替换加载失败的图片\n  event.target.src = require('@/assets/img/sp01.png');\n};\n\n// 处理商家图片加载错误\nconst handleBusinessImgError = (event) => {\n  // 使用默认商家图片替换加载失败的图片\n  event.target.src = require('@/assets/img/sj01.png');\n};\n\nconst getMockOrders = () => {\n  console.log('使用模拟订单数据');\n  return [\n    {\n      orderId: 1,\n      businessName: '测试商家1',\n      businessImg: require('@/assets/img/sj01.png'),\n      orderTotal: 35.5,\n      deliveryPrice: 5,\n      foodImgs: [\n        require('@/assets/img/business2/food/img.png'),\n        require('@/assets/img/business2/food/img_1.png')\n      ],\n      quantities: [1, 2],\n      orderDate: '2023-06-29 12:30:45'\n    },\n    {\n      orderId: 2,\n      businessName: '测试商家2',\n      businessImg: require('@/assets/img/sj02.png'),\n      orderTotal: 42.8,\n      deliveryPrice: 5,\n      foodImgs: [\n        require('@/assets/img/business2/food/img_2.png')\n      ],\n      quantities: [3],\n      orderDate: '2023-06-28 18:15:22'\n    }\n  ];\n};\n\nconst fetchOrders = async () => {\n  // 检查是否有有效的token和userId\n  if (!hasValidToken() || !userId.value) {\n    console.log('用户未登录或token无效，显示登录提示');\n    showLoginPrompt.value = true;\n    orders.value = getMockOrders(); // 仍然显示模拟数据\n    return;\n  }\n  \n  try {\n    console.log('获取用户历史订单，userId:', userId.value);\n    console.log('当前token:', getToken());\n    \n    const response = await orderApi.getOrdersByUserId(userId.value);\n    console.log('Order response:', response);\n\n    if (response && response.code === 1 && Array.isArray(response.data)) {\n      orders.value = response.data.map(order => {\n        // 处理商家图片路径\n        let businessImg = order.businessImg;\n        if (businessImg && !businessImg.startsWith('http') && !businessImg.startsWith('data:') && !businessImg.startsWith('/')) {\n          businessImg = require(`@/assets/img/${businessImg}`);\n        } else if (businessImg && businessImg.startsWith('/')) {\n          businessImg = `${baseUrl}${businessImg}`;\n        } else if (!businessImg) {\n          businessImg = require('@/assets/img/sj01.png');\n        }\n        \n        // 处理食品图片路径\n        let foodImgsArray = [];\n        if (Array.isArray(order.foodImgs)) {\n          foodImgsArray = order.foodImgs;\n        } else if (order.foodImgs) {\n          foodImgsArray = order.foodImgs.split(',');\n        }\n        \n        // 转换食品图片路径\n        const processedFoodImgs = foodImgsArray.map(img => {\n          if (!img) return require('@/assets/img/business2/food/img.png');\n          if (img.startsWith('http') || img.startsWith('data:')) return img;\n          if (img.startsWith('/')) return `${baseUrl}${img}`;\n          try {\n            return require(`@/assets/img/${img}`);\n          } catch (e) {\n            return require('@/assets/img/business2/food/img.png');\n          }\n        });\n        \n        // 处理数量\n        const quantities = Array.isArray(order.quantities) \n          ? order.quantities \n          : (order.quantities ? order.quantities.split(',').map(q => parseInt(q, 10)) : []);\n          \n        return {\n          ...order,\n          businessImg,\n          foodImgs: processedFoodImgs,\n          quantities\n        };\n      });\n    } else {\n      console.error('获取订单响应格式错误:', response);\n      orders.value = getMockOrders();\n    }\n  } catch (error) {\n    console.error('Error fetching orders:', error);\n    \n    // 如果是401错误，显示登录提示\n    if (error.response && error.response.status === 401) {\n      console.log('认证失败，显示登录提示');\n      showLoginPrompt.value = true;\n    }\n    \n    // 无论如何都显示模拟数据\n    orders.value = getMockOrders();\n  }\n};\n</script>\n\n<style scoped>\n.wrapper{\n  width: 100%;\n  height: 100%;\n  background-color: aliceblue;\n}\n\n\n.wrapper .header{\n  background-color: aliceblue;\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  width: 100%;\n}\n\n\n.wrapper .header .search .search-bottom{\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding-top: 3vw;\n  height: 30px;\n\n}\n\n.wrapper .header .search .search-bottom .back .icon-back{\n  font-size: 20px;\n}\n\n\n\n.wrapper .header .search .search-bottom .search-pt{\n  position: relative;\n}\n.wrapper .header .search .search-bottom .search-pt input{\n  border: none;\n  padding-left: 30px;\n  margin-right: 3vw;\n  height: 25px;\n  border-radius: 15px;\n  font-size: 3.5vw;\n\n}\n\n\n.wrapper .header .search .search-bottom .search-pt .icon-sousuo{\n  position: relative;\n  top: 30%;\n  left: 30px; /* 调整图标在输入框中的位置 */\n  transform: translateY(-50%);\n  font-size: 15px; /* 调整图标的大小 */\n}\n.wrapper .header .search .search-bottom .back{\n  padding: 0 