{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted, computed } from 'vue';\nimport { useRouter, useRoute } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { cartApi, businessApi } from '@/utils/api';\nimport { getToken } from '@/utils/auth';\nimport axios from 'axios';\nexport default {\n  __name: 'businesslist2Component',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const route = useRoute();\n    const businessImg = ref(route.params.businessImg);\n    const businessId = ref(route.params.businessId);\n    const businessName = ref(route.params.businessName || '默认店铺名称');\n    const foods = ref([]);\n    const cart = ref({});\n    const businessDetails = ref({});\n    const showDebug = ref(false);\n    console.log(businessId.value);\n\n    // 获取商家详情\n    const fetchBusinessDetails = async () => {\n      try {\n        console.log('开始获取商家详情，商家ID:', businessId.value);\n\n        // 直接使用axios发送请求，查看原始响应\n        const axiosResponse = await axios.get(`http://localhost:8090/business/${businessId.value}`);\n        console.log('Axios商家详情原始响应:', axiosResponse);\n        const response = await businessApi.getBusinessById(businessId.value);\n        console.log('原始商家详情响应:', JSON.stringify(response));\n        if (response && response.code === 1 && response.data) {\n          businessDetails.value = response.data;\n          console.log('获取到的商家详情:', JSON.stringify(businessDetails.value));\n\n          // 确保商家图片可用\n          if (businessDetails.value.businessImg) {\n            console.log('商家图片可用:', businessDetails.value.businessImg.substring(0, 30) + '...');\n          } else {\n            console.log('商家图片不可用');\n          }\n        } else if (response && response.data) {\n          businessDetails.value = response.data;\n          console.log('获取到的商家详情(直接对象):', JSON.stringify(businessDetails.value));\n        } else {\n          console.error('获取商家详情失败: 返回数据格式不正确', response);\n        }\n      } catch (error) {\n        console.error('获取商家详情失败:', error);\n      }\n    };\n    const fetchFoods = async () => {\n      try {\n        console.log('开始获取菜品数据，商家ID:', businessId.value);\n\n        // 直接使用axios发送请求，查看原始响应\n        const axiosResponse = await axios.get(`http://localhost:8090/food/listByBusinessId/${businessId.value}`);\n        console.log('Axios原始响应:', axiosResponse);\n        const response = await businessApi.getFoodsByBusinessId(businessId.value);\n        console.log('原始菜品响应:', JSON.stringify(response));\n\n        // 直接使用后端返回的数据，不做任何修改或添加默认值\n        if (response && response.code === 1 && Array.isArray(response.data)) {\n          console.log('菜品数据格式: Result<List<Food>>');\n          foods.value = response.data;\n          console.log('使用原始菜品数据:', foods.value);\n        } else if (Array.isArray(response)) {\n          console.log('菜品数据格式: List<Food>');\n          foods.value = response;\n          console.log('使用原始菜品数据(直接数组):', foods.value);\n        } else {\n          console.error('获取菜单数据失败: 返回数据格式不正确', response);\n        }\n      } catch (error) {\n        console.error('获取菜单数据失败:', error);\n      }\n    };\n    onMounted(() => {\n      fetchBusinessDetails();\n      fetchFoods();\n    });\n\n    // 结算功能\n    const checkout = async () => {\n      try {\n        // 检查是否登录\n        const token = getToken();\n        const userId = localStorage.getItem('userId');\n        if (!token || !userId) {\n          ElMessage.warning('请先登录');\n          router.push('/login-component');\n          return;\n        }\n\n        // 检查购物车是否为空\n        if (Object.keys(cart.value).length === 0) {\n          ElMessage.warning('购物车为空');\n          return;\n        }\n\n        // 准备结算数据\n        const checkoutData = {\n          userId: userId,\n          businessId: parseInt(businessId.value),\n          // 确保是数字类型\n          // 添加默认配送地址ID\n          daId: 1,\n          // 使用默认配送地址ID，或从用户配置中获取\n          items: Object.values(cart.value).map(item => ({\n            foodId: parseInt(item.foodId),\n            // 确保是数字类型\n            quantity: parseInt(item.quantity),\n            // 确保是数字类型\n            price: parseFloat(item.price) // 确保是数字类型\n          }))\n        };\n        console.log('发送结算请求:', JSON.stringify(checkoutData));\n        // 检查数据类型\n        console.log('userId类型:', typeof checkoutData.userId);\n        console.log('businessId类型:', typeof checkoutData.businessId);\n        console.log('items类型:', typeof checkoutData.items);\n        console.log('items长度:', checkoutData.items.length);\n        if (checkoutData.items.length > 0) {\n          console.log('第一个item:', JSON.stringify(checkoutData.items[0]));\n          console.log('foodId类型:', typeof checkoutData.items[0].foodId);\n          console.log('quantity类型:', typeof checkoutData.items[0].quantity);\n          console.log('price类型:', typeof checkoutData.items[0].price);\n        }\n        const response = await cartApi.checkout(checkoutData);\n        if (response && response.code === 1) {\n          ElMessage.success('结算成功');\n          // 清空购物车\n          cart.value = {};\n          // 跳转到订单页面 - 提供必要的参数\n          router.push({\n            name: 'Order',\n            params: {\n              businessId: businessId.value,\n              businessName: businessName.value || '商家名称'\n            }\n          });\n        } else {\n          throw new Error(response?.msg || '结算失败');\n        }\n      } catch (error) {\n        console.error('结算错误:', error);\n        if (error.isAuthError) {\n          ElMessage.error('登录已过期，请重新登录');\n          router.push('/login-component');\n          return;\n        }\n        if (error.response) {\n          switch (error.response.status) {\n            case 400:\n              ElMessage.error('请求数据格式错误');\n              break;\n            case 403:\n              ElMessage.error('没有权限进行此操作');\n              break;\n            case 500:\n              ElMessage.error('服务器内部错误');\n              break;\n            default:\n              ElMessage.error(`结算失败: ${error.response.status}`);\n          }\n        } else if (error.request) {\n          ElMessage.error('无法连接到服务器，请检查网络');\n        } else {\n          ElMessage.error('结算失败: ' + error.message);\n        }\n      }\n    };\n\n    // 添加到购物车\n    const addToCart = food => {\n      const cartItem = cart.value[food.foodId];\n      if (cartItem) {\n        cartItem.quantity += 1;\n      } else {\n        cart.value[food.foodId] = {\n          foodId: food.foodId,\n          foodName: food.foodName,\n          price: food.foodPrice,\n          quantity: 1,\n          businessId: businessId.value\n        };\n      }\n      ElMessage.success('已添加到购物车');\n    };\n\n    // 从购物车移除\n    const removeFromCart = item => {\n      if (cart.value[item.foodId]) {\n        if (cart.value[item.foodId].quantity > 1) {\n          cart.value[item.foodId].quantity -= 1;\n        } else {\n          delete cart.value[item.foodId];\n        }\n        ElMessage.success('已从购物车移除');\n      }\n    };\n\n    // 计算购物车总价\n    const totalPrice = computed(() => {\n      return Object.values(cart.value).reduce((total, item) => {\n        return total + item.price * item.quantity;\n      }, 0);\n    });\n\n    // 计算购物车总数量\n    const totalQuantity = computed(() => {\n      return Object.values(cart.value).reduce((total, item) => {\n        return total + item.quantity;\n      }, 0);\n    });\n    function goBack() {\n      router.push('/Index-Component');\n    }\n    function handleImageError(event) {\n      event.target.src = require('@/assets/img/BUSINESS/menus/img.png');\n    }\n\n    // 处理图片URL，确保能正确显示\n    function processImageUrl(url) {\n      console.log('处理图片URL类型:', typeof url);\n\n      // 如果URL为空，返回默认图片\n      if (!url) {\n        console.log('图片URL为空，使用默认图片');\n        return require('@/assets/img/BUSINESS/menus/img.png');\n      }\n\n      // 如果URL是字符串类型\n      if (typeof url === 'string') {\n        // 如果已经是完整的URL或data:image格式，直接返回\n        if (url.startsWith('http') || url.startsWith('data:')) {\n          console.log('使用原始图片URL(Base64或HTTP)');\n          return url;\n        }\n\n        // 尝试作为相对路径处理\n        try {\n          console.log('尝试作为相对路径处理:', url);\n          return require('@/assets/img/BUSINESS/menus/img.png');\n        } catch (e) {\n          console.log('图片路径处理失败，使用默认图片');\n          return require('@/assets/img/BUSINESS/menus/img.png');\n        }\n      }\n\n      // 如果URL是对象或其他类型，返回默认图片\n      console.log('图片URL类型不是字符串，使用默认图片');\n      return require('@/assets/img/BUSINESS/menus/img.png');\n    }\n\n    // 处理价格显示\n    function formatPrice(price) {\n      console.log('处理价格:', price, typeof price);\n      if (price === undefined || price === null) {\n        console.log('价格为空，返回0.00');\n        return '0.00';\n      }\n\n      // 确保price是数字\n      const numPrice = Number(price);\n      if (isNaN(numPrice)) {\n        console.log('价格不是数字，返回0.00');\n        return '0.00';\n      }\n      return numPrice.toFixed(2);\n    }\n    function toggleDebug() {\n      showDebug.value = !showDebug.value;\n    }\n    const __returned__ = {\n      router,\n      route,\n      businessImg,\n      businessId,\n      businessName,\n      foods,\n      cart,\n      businessDetails,\n      showDebug,\n      fetchBusinessDetails,\n      fetchFoods,\n      checkout,\n      addToCart,\n      removeFromCart,\n      totalPrice,\n      totalQuantity,\n      goBack,\n      handleImageError,\n      processImageUrl,\n      formatPrice,\n      toggleDebug,\n      ref,\n      onMounted,\n      computed,\n      get useRouter() {\n        return useRouter;\n      },\n      get useRoute() {\n        return useRoute;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get cartApi() {\n        return cartApi;\n      },\n      get businessApi() {\n        return businessApi;\n      },\n      get getToken() {\n        return getToken;\n      },\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","useRouter","useRoute","ElMessage","cartApi","businessApi","getToken","axios","router","route","businessImg","params","businessId","businessName","foods","cart","businessDetails","showDebug","console","log","value","fetchBusinessDetails","axiosResponse","get","response","getBusinessById","JSON","stringify","code","data","substring","error","fetchFoods","getFoodsByBusinessId","Array","isArray","checkout","token","userId","localStorage","getItem","warning","push","Object","keys","length","checkoutData","parseInt","daId","items","values","map","item","foodId","quantity","price","parseFloat","success","name","Error","msg","isAuthError","status","request","message","addToCart","food","cartItem","foodName","foodPrice","removeFromCart","totalPrice","reduce","total","totalQuantity","goBack","handleImageError","event","target","src","require","processImageUrl","url","startsWith","e","formatPrice","undefined","numPrice","Number","isNaN","toFixed","toggleDebug"],"sources":["D:/back_elm/vue_elm/src/components/businesslist2Component.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div class=\"header\">\n      <div class=\"title\">\n        <div class=\"img1\" @click=\"goBack\">\n          <img :src=\"require('@/assets/img/BUSINESS/img_1.png')\" height=\"22\" width=\"13\"/>\n        </div>\n        <div class=\"img2\">\n          <img :src=\"require('@/assets/img/BUSINESS/img.png')\" height=\"auto\" width=\"100%\"/>\n        </div>\n      </div>\n      <div class=\"header-all\">\n        <div class=\"header-text\">\n          <div class=\"text-left-header\">\n            <h>{{businessName}}</h>\n          </div>\n          <div class=\"star\">\n            <p1>{{ businessDetails.starPrice || '暂无' }}分 </p1>\n            <p2> 蜂鸟准时达</p2>\n            <p3>约{{ businessDetails.deliveryPrice || '暂无' }}分钟  月售{{ businessDetails.orderTypeId || '0' }}+</p3>\n          </div>\n          <div class=\"ticket\">\n            <img :src=\"require('@/assets/img/BannerImg/img_1.png')\" height=\"23\" width=\"100\"/>\n          </div>\n          <div class=\"for-sell\">\n            <p1>起送费: {{ businessDetails.starPrice || '暂无' }}元</p1>\n            <p2>配送费: {{ businessDetails.deliveryPrice || '暂无' }}元</p2>\n          </div>\n        </div>\n        <div class=\"text-picture\">\n          <img :src=\"processImageUrl(businessImg)\" height=\"155\" width=\"157\"/>\n        </div>\n      </div>\n      <div class=\"banner\">\n        <img :src=\"require('@/assets/img/BannerImg/img_3.png')\" height=\"15\" width=\"100\"/>\n        <p1>{{ businessDetails.businessExplain || '商家说明' }}</p1>\n        <p2>{{ businessDetails.remarks || '每日更新' }}</p2>\n      </div>\n    </div>\n    <div class=\"body\">\n      <!-- ... 其他代码 ... -->\n      <div class=\"recommend\">\n        <div class=\"recommend-left\">\n          <div>\n            <p1>点餐</p1>\n          </div>\n          <div>\n            <p2>评价</p2>\n          </div>\n          <div>\n            <p3>品牌</p3>\n          </div>\n        </div>\n        <div class=\"recommend-right\">\n          <p>好友拼单</p>\n        </div>\n      </div>\n\n      <div class=\"foodList\">\n        <div class=\"foodList-left\">\n          <div class=\"title0\">\n            <img :src=\"require('@/assets/img/BUSINESS/title/img.png')\" height=\"26\" width=\"30\"/>\n            <p> 学生专享</p>\n          </div>\n          <div class=\"title1\">\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_1.png')\" height=\"26\" width=\"30\"/>\n            <p>热销</p>\n          </div>\n          <div class=\"title1\">\n            <img :src=\"require('@/assets/img/BUSINESS/title/img.png')\" height=\"26\" width=\"30\"/>\n            <p> 优惠</p>\n          </div>\n          <div class=\"title2\">\n            <p>热销必点</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_2.png')\" height=\"16\" width=\"46\"/>\n          </div>\n          <div class=\"title2\">\n            <p> 新品尝鲜</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_3.png')\" height=\"13\" width=\"42\"/>\n          </div>\n          <div class=\"title2\">\n            <p>招牌全鸡</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_4.png')\" height=\"20\" width=\"42\"/>\n          </div>\n          <div class=\"title2\">\n            <p>休闲小吃</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_5.png')\" height=\"22\" width=\"39\"/>\n          </div>\n          <div class=\"title2\">\n            <p>卷堡主食</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_6.png')\" height=\"20\" width=\"42\"/>\n          </div>\n          <div class=\"title2\">\n            <p>饮品专区</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_7.png')\" height=\"20\" width=\"40\"/>\n          </div>\n          <div class=\"title2\">\n            <p>调料专区</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_8.png')\" height=\"18\" width=\"40\"/>\n          </div>\n          <div class=\"title2\">\n            <p>品牌活动</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_9.png')\" height=\"19\" width=\"41\"/>\n          </div>\n\n\n        </div>\n        <div class=\"foodList-right\">\n          <div class=\"menus\" v-for=\"food in foods\" :key=\"food.foodId\">\n\n            <div class=\"menus-body\">\n              <div class=\"menus-body-left\">\n                <img :src=\"processImageUrl(food.foodImg)\" \n                     height=\"124\" \n                     width=\"150\"\n                     @error=\"handleImageError\" />\n              </div>\n              <div class=\"menus-body-right\">\n                <div class=\"mbr-title\">\n                  <p1>{{ food.foodName || '未命名菜品' }}</p1>\n                </div>\n                <div class=\"mbr-banner\">\n                  <p2>{{ food.foodExplain || '暂无说明' }}</p2>\n                </div>\n                <div class=\"mbr-count\">\n                  <p3>{{ food.remarks || '暂无销售数据' }}</p3>\n                </div>\n                <div class=\"mbr-sell\" v-if=\"food.remarks && food.remarks.includes('折')\">\n                  <p4>{{ food.remarks }}</p4>\n                  <p5>优惠价</p5>\n                </div>\n                <div class=\"mbr-price\">\n                  <p>&#165;{{ formatPrice(food.foodPrice) }}</p>\n                  <div class=\"cart-actions\">\n                    <img\n                        v-if=\"cart[food.foodId] && cart[food.foodId].quantity > 0\"\n                        :src=\"require('@/assets/img/BUSINESS/menus/img_4.png')\"\n                        height=\"27\"\n                        width=\"28\"\n                        @click=\"decrementQuantity(food.foodId)\"\n                    />\n                    <span v-if=\"cart[food.foodId] && cart[food.foodId].quantity > 0\">{{ cart[food.foodId].quantity }}</span>\n                    <img\n                        :src=\"require('@/assets/img/BUSINESS/menus/img_1.png')\"\n                        height=\"27\"\n                        width=\"28\"\n                        @click=\"addToCart(food)\"\n                    />\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n\n        </div>\n\n      <div class=\"footer\">\n        <!-- ... 其他代码 ... -->\n        <div class=\"cart-left\">\n          <div class=\"cart-left-icon\">\n            <i class=\"fa fa-shopping-cart\"></i>\n          </div>\n          <!-- 当购物车中有商品时显示数量 -->\n          <div class=\"car-left-icon-quantity\" v-if=\"totalQuantity > 0\">{{ totalQuantity }}</div>\n          <div style=\"margin-top: 15px\">价格: {{ totalPrice }}元</div>\n        </div>\n        <div class=\"cart-right\">\n          <div class=\"cart-right-item\" @click=\"checkout\">去结算</div>\n        </div>\n      </div>\n      \n      <!-- 调试信息，仅在开发环境显示 -->\n      <div v-if=\"showDebug\" style=\"margin-top: 20px; padding: 10px; background-color: #f5f5f5; border: 1px solid #ddd;\">\n        <h3>调试信息</h3>\n        <button @click=\"toggleDebug\">隐藏调试信息</button>\n        <div>\n          <h4>商家信息:</h4>\n          <pre>{{ JSON.stringify(businessDetails, null, 2) }}</pre>\n        </div>\n        <div>\n          <h4>菜品信息:</h4>\n          <pre>{{ JSON.stringify(foods, null, 2) }}</pre>\n        </div>\n      </div>\n      <div v-else style=\"margin-top: 20px; text-align: center;\">\n        <button @click=\"toggleDebug\">显示调试信息</button>\n      </div>\n\n      <!-- 购物车部分 -->\n      <div class=\"cart-container\" v-if=\"totalQuantity > 0\">\n        <h3>购物车</h3>\n        <div class=\"cart-items\">\n          <div v-for=\"item in Object.values(cart)\" :key=\"item.foodId\" class=\"cart-item\">\n            <span>{{ item.foodName }}</span>\n            <div class=\"quantity-controls\">\n              <button @click=\"removeFromCart(item)\" class=\"quantity-btn\">-</button>\n              <span>{{ item.quantity }}</span>\n              <button @click=\"addToCart(item)\" class=\"quantity-btn\">+</button>\n            </div>\n            <span>¥{{ formatPrice(item.price * item.quantity) }}</span>\n          </div>\n        </div>\n        <div class=\"cart-footer\">\n          <div class=\"total\">总计: ¥{{ formatPrice(totalPrice) }}</div>\n          <button @click=\"checkout\" class=\"checkout-btn\">结算</button>\n        </div>\n      </div>\n\n      <!-- 商品列表部分 -->\n      <div class=\"food-list\">\n        <div v-for=\"food in foods\" :key=\"food.foodId\" class=\"food-item\">\n          <img :src=\"processImageUrl(food.foodImg)\" :alt=\"food.foodName\" class=\"food-img\">\n          <div class=\"food-info\">\n            <h4>{{ food.foodName }}</h4>\n            <p>¥{{ formatPrice(food.foodPrice) }}</p>\n            <button @click=\"addToCart(food)\" class=\"add-to-cart-btn\">添加到购物车</button>\n          </div>\n        </div>\n      </div>\n  </div>\n\n</template>\n\n\n<script setup>\nimport { ref, onMounted, computed } from 'vue';\nimport { useRouter, useRoute } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { cartApi, businessApi } from '@/utils/api';\nimport { getToken } from '@/utils/auth';\nimport axios from 'axios';\n\nconst router = useRouter();\nconst route = useRoute();\n\nconst businessImg = ref(route.params.businessImg);\nconst businessId = ref(route.params.businessId);\nconst businessName = ref(route.params.businessName || '默认店铺名称');\nconst foods = ref([]);\nconst cart = ref({});\nconst businessDetails = ref({});\nconst showDebug = ref(false);\n\nconsole.log(businessId.value);\n\n// 获取商家详情\nconst fetchBusinessDetails = async () => {\n  try {\n    console.log('开始获取商家详情，商家ID:', businessId.value);\n    \n    // 直接使用axios发送请求，查看原始响应\n    const axiosResponse = await axios.get(`http://localhost:8090/business/${businessId.value}`);\n    console.log('Axios商家详情原始响应:', axiosResponse);\n    \n    const response = await businessApi.getBusinessById(businessId.value);\n    console.log('原始商家详情响应:', JSON.stringify(response));\n    \n    if (response && response.code === 1 && response.data) {\n      businessDetails.value = response.data;\n      console.log('获取到的商家详情:', JSON.stringify(businessDetails.value));\n      \n      // 确保商家图片可用\n      if (businessDetails.value.businessImg) {\n        console.log('商家图片可用:', businessDetails.value.businessImg.substring(0, 30) + '...');\n      } else {\n        console.log('商家图片不可用');\n      }\n    } else if (response && response.data) {\n      businessDetails.value = response.data;\n      console.log('获取到的商家详情(直接对象):', JSON.stringify(businessDetails.value));\n    } else {\n      console.error('获取商家详情失败: 返回数据格式不正确', response);\n    }\n  } catch (error) {\n    console.error('获取商家详情失败:', error);\n  }\n};\n\nconst fetchFoods = async () => {\n  try {\n    console.log('开始获取菜品数据，商家ID:', businessId.value);\n    \n    // 直接使用axios发送请求，查看原始响应\n    const axiosResponse = await axios.get(`http://localhost:8090/food/listByBusinessId/${businessId.value}`);\n    console.log('Axios原始响应:', axiosResponse);\n    \n    const response = await businessApi.getFoodsByBusinessId(businessId.value);\n    console.log('原始菜品响应:', JSON.stringify(response));\n    \n    // 直接使用后端返回的数据，不做任何修改或添加默认值\n    if (response && response.code === 1 && Array.isArray(response.data)) {\n      console.log('菜品数据格式: Result<List<Food>>');\n      foods.value = response.data;\n      console.log('使用原始菜品数据:', foods.value);\n    } else if (Array.isArray(response)) {\n      console.log('菜品数据格式: List<Food>');\n      foods.value = response;\n      console.log('使用原始菜品数据(直接数组):', foods.value);\n    } else {\n      console.error('获取菜单数据失败: 返回数据格式不正确', response);\n    }\n  } catch (error) {\n    console.error('获取菜单数据失败:', error);\n  }\n};\n\nonMounted(() => {\n  fetchBusinessDetails();\n  fetchFoods();\n});\n\n// 结算功能\nconst checkout = async () => {\n  try {\n    // 检查是否登录\n    const token = getToken();\n    const userId = localStorage.getItem('userId');\n    \n    if (!token || !userId) {\n      ElMessage.warning('请先登录');\n      router.push('/login-component');\n      return;\n    }\n\n    // 检查购物车是否为空\n    if (Object.keys(cart.value).length === 0) {\n      ElMessage.warning('购物车为空');\n      return;\n    }\n\n    // 准备结算数据\n    const checkoutData = {\n      userId: userId,\n      businessId: parseInt(businessId.value),  // 确保是数字类型\n      // 添加默认配送地址ID\n      daId: 1,  // 使用默认配送地址ID，或从用户配置中获取\n      items: Object.values(cart.value).map(item => ({\n        foodId: parseInt(item.foodId),  // 确保是数字类型\n        quantity: parseInt(item.quantity),  // 确保是数字类型\n        price: parseFloat(item.price)  // 确保是数字类型\n      }))\n    };\n\n    console.log('发送结算请求:', JSON.stringify(checkoutData));\n    // 检查数据类型\n    console.log('userId类型:', typeof checkoutData.userId);\n    console.log('businessId类型:', typeof checkoutData.businessId);\n    console.log('items类型:', typeof checkoutData.items);\n    console.log('items长度:', checkoutData.items.length);\n    if (checkoutData.items.length > 0) {\n      console.log('第一个item:', JSON.stringify(checkoutData.items[0]));\n      console.log('foodId类型:', typeof checkoutData.items[0].foodId);\n      console.log('quantity类型:', typeof checkoutData.items[0].quantity);\n      console.log('price类型:', typeof checkoutData.items[0].price);\n    }\n\n    const response = await cartApi.checkout(checkoutData);\n    \n    if (response && response.code === 1) {\n      ElMessage.success('结算成功');\n      // 清空购物车\n      cart.value = {};\n      // 跳转到订单页面 - 提供必要的参数\n      router.push({\n        name: 'Order',\n        params: {\n          businessId: businessId.value,\n          businessName: businessName.value || '商家名称'\n        }\n      });\n    } else {\n      throw new Error(response?.msg || '结算失败');\n    }\n  } catch (error) {\n    console.error('结算错误:', error);\n    \n    if (error.isAuthError) {\n      ElMessage.error('登录已过期，请重新登录');\n      router.push('/login-component');\n      return;\n    }\n\n    if (error.response) {\n      switch (error.response.status) {\n        case 400:\n          ElMessage.error('请求数据格式错误');\n          break;\n        case 403:\n          ElMessage.error('没有权限进行此操作');\n          break;\n        case 500:\n          ElMessage.error('服务器内部错误');\n          break;\n        default:\n          ElMessage.error(`结算失败: ${error.response.status}`);\n      }\n    } else if (error.request) {\n      ElMessage.error('无法连接到服务器，请检查网络');\n    } else {\n      ElMessage.error('结算失败: ' + error.message);\n    }\n  }\n};\n\n// 添加到购物车\nconst addToCart = (food) => {\n  const cartItem = cart.value[food.foodId];\n  \n  if (cartItem) {\n    cartItem.quantity += 1;\n  } else {\n    cart.value[food.foodId] = {\n      foodId: food.foodId,\n      foodName: food.foodName,\n      price: food.foodPrice,\n      quantity: 1,\n      businessId: businessId.value\n    };\n  }\n  \n  ElMessage.success('已添加到购物车');\n};\n\n// 从购物车移除\nconst removeFromCart = (item) => {\n  if (cart.value[item.foodId]) {\n    if (cart.value[item.foodId].quantity > 1) {\n      cart.value[item.foodId].quantity -= 1;\n    } else {\n      delete cart.value[item.foodId];\n    }\n    ElMessage.success('已从购物车移除');\n  }\n};\n\n// 计算购物车总价\nconst totalPrice = computed(() => {\n  return Object.values(cart.value).reduce((total, item) => {\n    return total + (item.price * item.quantity);\n  }, 0);\n});\n\n// 计算购物车总数量\nconst totalQuantity = computed(() => {\n  return Object.values(cart.value).reduce((total, item) => {\n    return total + item.quantity;\n  }, 0);\n});\n\nfunction goBack() {\n  router.push('/Index-Component');\n}\n\nfunction handleImageError(event) {\n  event.target.src = require('@/assets/img/BUSINESS/menus/img.png');\n}\n\n// 处理图片URL，确保能正确显示\nfunction processImageUrl(url) {\n  console.log('处理图片URL类型:', typeof url);\n  \n  // 如果URL为空，返回默认图片\n  if (!url) {\n    console.log('图片URL为空，使用默认图片');\n    return require('@/assets/img/BUSINESS/menus/img.png');\n  }\n  \n  // 如果URL是字符串类型\n  if (typeof url === 'string') {\n    // 如果已经是完整的URL或data:image格式，直接返回\n    if (url.startsWith('http') || url.startsWith('data:')) {\n      console.log('使用原始图片URL(Base64或HTTP)');\n      return url;\n    }\n    \n    // 尝试作为相对路径处理\n    try {\n      console.log('尝试作为相对路径处理:', url);\n      return require('@/assets/img/BUSINESS/menus/img.png');\n    } catch (e) {\n      console.log('图片路径处理失败，使用默认图片');\n      return require('@/assets/img/BUSINESS/menus/img.png');\n    }\n  }\n  \n  // 如果URL是对象或其他类型，返回默认图片\n  console.log('图片URL类型不是字符串，使用默认图片');\n  return require('@/assets/img/BUSINESS/menus/img.png');\n}\n\n// 处理价格显示\nfunction formatPrice(price) {\n  console.log('处理价格:', price, typeof price);\n  \n  if (price === undefined || price === null) {\n    console.log('价格为空，返回0.00');\n    return '0.00';\n  }\n  \n  // 确保price是数字\n  const numPrice = Number(price);\n  if (isNaN(numPrice)) {\n    console.log('价格不是数字，返回0.00');\n    return '0.00';\n  }\n  \n  return numPrice.toFixed(2);\n}\n\nfunction toggleDebug() {\n  showDebug.value = !showDebug.value;\n}\n</script>\n\n<style scoped>\n.cart-actions {\n  display: flex;\n  align-items: center;\n}\n\n.cart-actions img {\n  margin-left: 5px; /* 根据需要调整间距 */\n  cursor: pointer;\n}\n/* ... 将 businessList.html 中的 CSS 样式复制到这里 ... */\n.wrapper {\n  width: 100%;\n  height: 100%;\n}\n\n.wrapper .header .title {\n  width: 430px;\n  height: 160px;\n  position: relative;\n\n\n}\n\n.wrapper .header .title .img2{\n  width: 100%;\n  height: 60%;\n\n\n}\n\n.wrapper .header .title .img1{\n  position: relative;\n  z-index: 1;\n  top: 108px; /* 你可以根据需要调整垂直位置 */\n  left: 18px; /* 你可以根据需要调整水平位置 */\n  width: 90%;\n\n\n}\n\n.wrapper .header .header-all{\n\n  width: 92%;\n  display: flex;\n  margin-left: 3.5vw;\n  margin-top: -3vw;\n  justify-content: space-between;\n  border: 1px solid red;\n  border-radius: 10px;\n}\n\n\n.wrapper .header .header-all .header-text{\n  flex: 7;\n  border-radius: 10px;\n  width: 100%;\n  box-sizing: border-box;\n  padding-left: 3vw;\n}\n\n.wrapper .header .header-all .header-text .text-left-header {\n\n  display: flex;\n  align-items: center;\n  margin-bottom: 2vw;\n}\n\n.wrapper .header .header-all .header-text .text-left-header h{\n  color: black;\n  font-size:5vw;\n  font-weight: bolder;\n}\n\n.wrapper .header .header-all .header-text .star{\n  display: flex;\n  font-size: 2.7vw;\n  align-items: center;\n  margin-bottom: 2vw;\n\n}\n\n\n.wrapper .header .header-all .header-text .star p1{\n  color: #ff4a25;\n  font-weight: bolder;\n}\n\n.wrapper .header .header-all .header-text .star p2{\n  color:#17BAF9;\n  padding:0 1vw 0 1vw;\n  border-radius: 5px;\n  border: 1px solid #17BAF9;\n  margin: 0 2vw 0 2vw;\n}\n\n.wrapper .header .header-all .header-text .star p3{\n  color: #999999;\n}\n\n.wrapper .header .header-all .header-text .for-sell{\n  display: flex;\n  align-items: center;\n  font-size: 2.7vw;\n  margin-bottom: 3vw;\n}\n\n.wrapper .header .header-all .header-text .for-sell p1{\n  border: 1px solid #ff8b39;\n  color:#ff8b39 ;\n  padding-left: 1vw;\n  padding-right: 1vw;\n  border-radius: 5px;\n}\n\n.wrapper .header .header-all .header-text .for-sell p2{\n  margin-left: 3vw;\n  border: 1px solid red;\n  color:red;\n  padding-left: 1vw;\n  padding-right: 1vw;\n  border-radius: 5px;\n}\n\n.wrapper .header .header-all .text-picture {\n  flex: 3;\n}\n\n.wrapper .header .header-all .text-picture img{\n  width: 13vw;\n  height: 13vw;\n  margin-left: 10vw;\n}\n.wrapper .header .banner{\n  display: flex;\n  align-items: center;\n  border: 1px red solid;\n  border-radius: 5px;\n  margin:2.5vw 4vw 0 3.5vw;\n  padding: 2vw 2vw 2vw 2vw ;\n  font-size: 3vw;\n  color:chocolate;\n}\n\n.wrapper .header .banner p2{\n  padding-left: 10vw;\n}\n\n\n/*主体点餐*/\n.wrapper .body .picture{\n  display: flex;\n  align-items: center;\n}\n.wrapper .body .picture img{\n  width: 95%;\n  height: 50%;\n  margin: 0 auto;\n\n}\n.wrapper .body .recommend{\n  display: flex;\n  margin: 2vw 4.5vw 2vw 0vw;\n  padding: 1vw 0 1vw 0;\n}\n.wrapper .body .recommend .recommend-left{\n  display: flex;\n  flex:7;\n  margin-right: 10vw;\n  justify-content:space-around;\n  align-items: center;\n}\n\n.wrapper .body .recommend .recommend-right{\n  flex: 3;\n  border: 1px solid red;\n  justify-content: space-around;\n  align-items: center;\n  border-radius: 10px;\n}\n\n.wrapper .body .recommend .recommend-right p{\n  margin: 1vw 1vw 1vw 7vw;\n  font-size: 3vw;\n  color: red;\n}\n\n\n.wrapper .body .foodList{\n  display: flex;\n\n}\n\n.wrapper .body .foodList .foodList-left{\n  flex: 2;\n  position: sticky;\n  top: 0; /* 指定元素在屏幕上固定时距离顶部的位置 */\n\n  background-color: aliceblue;\n}\n\n.wrapper .body .foodList .foodList-left .title0{\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  margin-bottom: 2vw;\n}\n\n.wrapper .body .foodList .foodList-left .title0 p{\n  margin-left: 1vw;\n  margin-right: 4vw;\n  font-size: 3vw;\n\n}\n.wrapper .body .foodList .foodList-left .title1{\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  margin-bottom: 4vw;\n}\n\n.wrapper .body .foodList .foodList-left .title1 p{\n  margin-left: 1vw;\n  margin-right: 4vw;\n  font-size: 3vw;\n\n}\n\n.wrapper .body .foodList .foodList-left .title2{\n  margin-left: 3vw;\n  margin-bottom: 7vw;\n}\n\n.wrapper .body .foodList .foodList-left .title2 p{\n  font-size: 3vw;\n  margin-bottom: 0;\n\n}\n\n.wrapper .body .foodList .foodList-right{\n  flex: 8;\n  margin-left: 2vw;\n  margin-top: 2vw;\n  margin-right: 4vw;\n}\n\n\n\n.wrapper .body .foodList .foodList-right .menus .menus-title{\n  display: flex;\n  margin-top: 2vw;\n}\n\n.wrapper .body .foodList .foodList-right .menus .menus-title p1{\n  font-weight: bolder;\n  margin:0 2vw 3vw 2vw ;\n  font-size: 3vw;\n}\n\n.wrapper .body .foodList .foodList-right .menus .menus-title p2{\n  font-size: 2.7vw;\n  color: #9F9F9F;\n}\n\n.wrapper .body .foodList .foodList-right .menus .menus-body{\n\n  display: flex;\n}\n\n\n.wrapper .body .foodList .foodList-right .menus .menus-body .menus-body-left{\n  flex: 3;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.wrapper .body .foodList .foodList-right .menus .menus-body .menus-body-left img{\n  width: 110%;\n  height: 90%;\n}\n\n.wrapper .body .foodList .foodList-right .menus .menus-body .menus-body-right{\n  flex: 7;\n  margin-left: 2vw;\n}\n\n.wrapper .body .foodList .foodList-right .menus .menus-body .menus-body-right .mbr-title{\n  font-weight: bolder;\n  margin-bottom: 1vw;\n}\n\n\n.wrapper .body .foodList .foodList-right .menus .menus-body .menus-body-right .mbr-banner{\n  font-size: 2.9vw;\n  margin-bottom: 1vw;\n  color: #9F9F9F;\n}\n.wrapper .body .foodList .foodList-right .menus .menus-body .menus-body-right .mbr-count{\