title\">\n          <div class=\"title-left\">\n            <img :src=\"order.businessImg\" height=\"52\" width=\"52\"/>\n          </div>\n          <div class=\"title-right\">\n            <div class=\"t-r-1\">\n              <div class=\"t-r-1-nb1\">\n                <p>{{ order.businessName }}</p>\n              </div>\n              <div class=\"t-r-1-nb2\">\n                <p>&#165;{{ order.orderTotal }}</p>\n              </div>\n            </div>\n            <div class=\"t-r-2\">\n              <p>{{ order.deliveryPrice }}元无门槛</p>\n            </div>\n          </div>\n        </div>\n        <div class=\"food-body\">\n          <div class=\"picture\" style=\"display: flex; flex-wrap: wrap; gap: 10px;\">\n            <div v-for=\"(img, index) in order.foodImgs\" :key=\"index\" class=\"food-item\" style=\"position: relative;\">\n              <img :src=\"img\" height=\"88\" width=\"88\"/>\n              <div class=\"quantity\">{{ order.quantities[index] }}</div>\n            </div>\n          </div>\n        </div>\n        <div class=\"time\">\n          <p>{{ order.orderDate }}</p>\n        </div>\n        <div class=\"one-more\">\n          <p>再来一单</p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { orderApi } from '@/utils/api';\n\nconst router = useRouter();\nconst orders = ref([]);\nconst userId = ref(localStorage.getItem('userId') || '');\n\nonMounted(() => {\n  fetchOrders();\n});\n\nconst goBack = () => {\n  router.push('/Index-Component');\n};\n\nconst fetchOrders = async () => {\n  if (!userId.value) {\n    console.error('用户ID未提供，无法获取历史订单');\n    // 添加一些模拟数据用于测试\n    orders.value = [\n      {\n        orderId: 1,\n        businessName: '测试商家1',\n        businessImg: require('@/assets/img/sj01.png'),\n        orderTotal: 35.5,\n        deliveryPrice: 5,\n        foodImgs: [\n          require('@/assets/img/business2/food/img.png'),\n          require('@/assets/img/business2/food/img_1.png')\n        ],\n        quantities: [1, 2],\n        orderDate: '2023-06-29 12:30:45'\n      },\n      {\n        orderId: 2,\n        businessName: '测试商家2',\n        businessImg: require('@/assets/img/sj02.png'),\n        orderTotal: 42.8,\n        deliveryPrice: 5,\n        foodImgs: [\n          require('@/assets/img/business2/food/img_2.png')\n        ],\n        quantities: [3],\n        orderDate: '2023-06-28 18:15:22'\n      }\n    ];\n    return;\n  }\n  \n  try {\n    console.log('获取用户历史订单，userId:', userId.value);\n    const response = await orderApi.getOrdersByUserId(userId.value);\n    console.log('Order response:', response);\n\n    if (response && response.code === 1 && Array.isArray(response.data)) {\n      orders.value = response.data.map(order => ({\n        ...order,\n        foodImgs: Array.isArray(order.foodImgs) \n          ? order.foodImgs \n          : (order.foodImgs ? order.foodImgs.split(',') : []),\n        quantities: Array.isArray(order.quantities) \n          ? order.quantities \n          : (order.quantities ? order.quantities.split(',').map(q => parseInt(q, 10)) : [])\n      }));\n    } else {\n      console.error('获取订单响应格式错误:', response);\n      // 使用模拟数据\n      orders.value = [\n        {\n          orderId: 1,\n          businessName: '测试商家1',\n          businessImg: require('@/assets/img/sj01.png'),\n          orderTotal: 35.5,\n          deliveryPrice: 5,\n          foodImgs: [\n            require('@/assets/img/business2/food/img.png'),\n            require('@/assets/img/business2/food/img_1.png')\n          ],\n          quantities: [1, 2],\n          orderDate: '2023-06-29 12:30:45'\n        },\n        {\n          orderId: 2,\n          businessName: '测试商家2',\n          businessImg: require('@/assets/img/sj02.png'),\n          orderTotal: 42.8,\n          deliveryPrice: 5,\n          foodImgs: [\n            require('@/assets/img/business2/food/img_2.png')\n          ],\n          quantities: [3],\n          orderDate: '2023-06-28 18:15:22'\n        }\n      ];\n    }\n  } catch (error) {\n    console.error('Error fetching orders:', error);\n    // 使用模拟数据\n    orders.value = [\n      {\n        orderId: 1,\n        businessName: '测试商家1',\n        businessImg: require('@/assets/img/sj01.png'),\n        orderTotal: 35.5,\n        deliveryPrice: 5,\n        foodImgs: [\n          require('@/assets/img/business2/food/img.png'),\n          require('@/assets/img/business2/food/img_1.png')\n        ],\n        quantities: [1, 2],\n        orderDate: '2023-06-29 12:30:45'\n      },\n      {\n        orderId: 2,\n        businessName: '测试商家2',\n        businessImg: require('@/assets/img/sj02.png'),\n        orderTotal: 42.8,\n        deliveryPrice: 5,\n        foodImgs: [\n          require('@/assets/img/business2/food/img_2.png')\n        ],\n        quantities: [3],\n        orderDate: '2023-06-28 18:15:22'\n      }\n    ];\n  }\n};\n</script>\n\n<style scoped>\n.wrapper{\n  width: 100%;\n  height: 100%;\n  background-color: aliceblue;\n}\n\n\n.wrapper .header{\n  background-color: aliceblue;\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  width: 100%;\n}\n\n\n.wrapper .header .search .search-bottom{\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding-top: 3vw;\n  height: 30px;\n\n}\n\n.wrapper .header .search .search-bottom .back .icon-back{\n  font-size: 20px;\n}\n\n\n\n.wrapper .header .search .search-bottom .search-pt{\n  position: relative;\n}\n.wrapper .header .search .search-bottom .search-pt input{\n  border: none;\n  padding-left: 30px;\n  margin-right: 3vw;\n  height: 25px;\n  border-radius: 15px;\n  font-size: 3.5vw;\n\n}\n\n\n.wrapper .header .search .search-bottom .search-pt .icon-sousuo{\n  position: relative;\n  top: 30%;\n  left: 30px; /* 调整图标在输入框中的位置 */\n  transform: translateY(-50%);\n  font-size: 15px; /* 调整图标的大小 */\n}\n.wrapper .header .search .search-bottom .back{\n  padding: 0 0 0 2vw;\n}\n\n\n.wrapper .header .search .search-bottom .box span{\n  padding: 0 2vw 0 2vw;\n  font-size: 20px;\n}\n\n.wrapper .header .search .information{\n  display: flex;\n  justify-content: space-around;\n\n}\n\n/*订单列表部分*/\n.wrapper .body .food{\n\n  width: 95%;\n  margin: 0 auto;\n  border-radius: 10px;\n  background-color: white;\n  margin-bottom: 2vw;\n\n}\n.wrapper .body .food .food-title{\n  display: flex;\n  align-items: center;\n  height: 60px;\n}\n\n.wrapper .body .food .food-title .title-left{\n\n}\n.wrapper .body .food .food-title .title-left img{\n  width: 40px;\n  height: 40px;\n  padding: 2vw 2vw 2vw 2vw ;\n}\n\n\n.wrapper .body .food .food-title .title-right .t-r-1{\n  height: 5vw;\n  display: flex;\n  align-items: center;\n  justify-content: space-around;\n}\n\n.wrapper .body .food .food-title .title-right .t-r-1 .t-r-1-nb1{\n  display: flex;\n  align-items: center;\n  font-size: 4vw;\n}\n\n.wrapper .body .food .food-title .title-right .t-r-1 .t-r-1-nb1 p{\n  margin-top: 6vw;\n\n}\n\n.wrapper .body .food .food-title .title-right .t-r-1 .t-r-1-nb2{\n  padding-left: 13vw;\n  font-size: 3vw;\n  color: #9F9F9F;\n}\n\n.wrapper .body .food .food-title .title-right .t-r-1 .t-r-1-nb1 img{\n  height: 15px;\n  width: 10px;\n}\n\n.wrapper .body .food .food-title .title-right .t-r-2 p{\n  font-size: 2.6vw;\n  margin-top: 3vw;\n  border: 1px solid saddlebrown;\n  color: saddlebrown;\n  width: 55px;\n  display: flex;\n  justify-content: center;\n  border-radius: 5px;\n}\n\n\n.wrapper .body .food .food-body{\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 0  2vw 0 2vw;\n}\n.wrapper .body .food .food-body .price p1{\n  font-weight: bolder;\n}\n\n.wrapper .body .food .food-body .price p2{\n  font-size: 3vw;\n  color: #9F9F9F;\n  display: flex;\n  justify-content: flex-end;\n  padding-top: 2vw;\n}\n\n.wrapper .body .food .time p{\n  font-size: 3vw;\n  color: #9F9F9F;\n  padding-left: 2vw;\n}\n\n.wrapper .body .food .one-more{\n  display: flex;\n  justify-content: flex-end;\n  padding-right: 5vw;\n}\n\n.wrapper .body .food .one-more p{\n  border: 1px solid #b3d4fc;\n  padding: 1vw 2vw 1vw 2vw;\n  border-radius: 10px;\n  color: #17BAF9;\n}\n\n.quantity {\n  position: absolute;\n  bottom: 0;\n  right: 0;\n  background-color: rgba(0, 0, 0, 0.7);\n  color: white;\n  padding: 2px 5px;\n  border-radius: 5px;\n  font-size: 12px;\n}\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAS;;EAwBbA,KAAK,EAAC,MAAM;EAACC,KAAwB,EAAxB;IAAA;EAAA;;;EAETD,KAAK,EAAC;AAAY;;EAChBA,KAAK,EAAC;AAAY;mBA5BjC;;EA+BeA,KAAK,EAAC;AAAa;;EACjBA,KAAK,EAAC;AAAO;;EACXA,KAAK,EAAC;AAAW;;EAGjBA,KAAK,EAAC;AAAW;;EAInBA,KAAK,EAAC;AAAO;;EAKjBA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC,SAAS;EAACC,KAAkD,EAAlD;IAAA;IAAA;IAAA;EAAA;;oBA9C/B;;EAiDmBD,KAAK,EAAC;AAAU;;EAItBA,KAAK,EAAC;AAAM;;uBApDvBE,mBAAA,CA4DM,OA5DNC,UA4DM,GA3DJC,mBAAA,CAsBM;IAtBDJ,KAAK,EAAC;EAAQ,IACjBI,mBAAA,CAoBM;IApBDJ,KAAK,EAAC;EAAQ,IACjBI,mBAAA,CAYM;IAZDJ,KAAK,EAAC;EAAe,IACxBI,mBAAA,CAEM;IAFDJ,KAAK,EAAC;EAAM,IACfI,mBAAA,CAAwD;IAAlDJ,KAAK,EAAC,oBAAoB;IAAEK,OAAK,EAAEC,MAAA,CAAAC;kCANrDC,kBAAA,0Y,6BAiBQJ,mBAAA,CAKM;IALDJ,KAAK,EAAC;EAAa,IACtBI,mBAAA,CAAS,WAAN,IAAE,GACLA,mBAAA,CAAU,WAAP,KAAG,GACNA,mBAAA,CAAU,WAAP,KAAG,GACNA,mBAAA,CAAS,WAAN,IAAE,E,0BAIXA,mBAAA,CAmCM,OAnCNK,UAmCM,I,kBAlCJP,mBAAA,CAiCMQ,SAAA,QA3DZC,WAAA,CA0B2BL,MAAA,CAAAM,MAAM,EAAfC,KAAK;yBAAjBX,mBAAA,CAiCM;MAjCwBY,GAAG,EAAED,KAAK,CAACE,OAAO;MAAEf,KAAK,EAAC;QACtDI,mBAAA,CAiBM,OAjBNY,UAiBM,GAhBJZ,mBAAA,CAEM,OAFNa,UAEM,GADJb,mBAAA,CAAsD;MAAhDc,GAAG,EAAEL,KAAK,CAACM,WAAW;MAAEC,MAAM,EAAC,IAAI;MAACC,KAAK,EAAC;4BA7B5DC,UAAA,E,GA+BUlB,mBAAA,CAYM,OAZNmB,UAYM,GAXJnB,mBAAA,CAOM,OAPNoB,UAOM,GANJpB,mBAAA,CAEM,OAFNqB,UAEM,GADJrB,mBAAA,CAA+B,WAAAsB,gBAAA,CAAzBb,KAAK,CAACc,YAAY,iB,GAE1BvB,mBAAA,CAEM,OAFNwB,UAEM,GADJxB,mBAAA,CAAmC,WAAhC,GAAM,GAAAsB,gBAAA,CAAGb,KAAK,CAACgB,UAAU,iB,KAGhCzB,mBAAA,CAEM,OAFN0B,WAEM,GADJ1B,mBAAA,CAAoC,WAAAsB,gBAAA,CAA9Bb,KAAK,CAACkB,aAAa,IAAG,MAAI,gB,OAItC3B,mBAAA,CAOM,OAPN4B,WAOM,GANJ5B,mBAAA,CAKM,