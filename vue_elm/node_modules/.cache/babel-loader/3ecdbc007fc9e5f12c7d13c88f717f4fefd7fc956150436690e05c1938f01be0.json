Id.value);\r\n    console.log('地址响应原始数据:', response);\r\n    \r\n    // 强制清空当前地址列表\r\n    deliveryAddresses.value = [];\r\n    \r\n    // 处理各种可能的响应格式\r\n    if (Array.isArray(response)) {\r\n      // 1. 直接返回数组\r\n      deliveryAddresses.value = response;\r\n      console.log('成功获取到地址列表(直接数组):', response.length, '个地址');\r\n    } else if (response && response.code === 1 && response.data) {\r\n      // 2. 标准Result包装格式 {code: 1, data: [...]}\r\n      const addressData = response.data;\r\n      console.log('地址数据类型:', typeof addressData, '内容:', JSON.stringify(addressData));\r\n      \r\n      if (Array.isArray(addressData)) {\r\n        // 2.1 data是数组\r\n        deliveryAddresses.value = addressData;\r\n        console.log('成功获取到地址列表(标准格式):', addressData.length, '个地址');\r\n      } else if (addressData && typeof addressData === 'object' && addressData.daId) {\r\n        // 2.2 data是单个地址对象\r\n        deliveryAddresses.value = [addressData];\r\n        console.log('成功获取到单个地址对象(标准格式)');\r\n      } else if (addressData && typeof addressData === 'object' && addressData.success === true) {\r\n        // 2.3 可能是{success:true}格式的空响应\r\n        console.log('服务端返回成功，但没有地址数据');\r\n      }\r\n    } else if (response && typeof response === 'object' && response.daId) {\r\n      // 3. 直接返回单个地址对象(不在数组中)\r\n      deliveryAddresses.value = [response];\r\n      console.log('成功获取到单个地址对象(直接对象)');\r\n    } else {\r\n      console.warn('无法识别的地址数据格式:', response);\r\n    }\r\n    \r\n    // 输出最终处理结果\r\n    console.log('最终地址列表:', deliveryAddresses.value);\r\n  } catch (error) {\r\n    console.error('获取地址时发生错误:', error);\r\n  }\r\n};\r\n\r\nonMounted(fetchDeliveryAddresses);\r\n\r\nconst selectAddress = (address) => {\r\n  const isConfirmed = window.confirm('确定选择这个地址吗？');\r\n  if (isConfirmed) {\r\n    chooseAddress(address);\r\n  } else {\r\n    console.log('选择地址已取消');\r\n  }\r\n};\r\n\r\nconst chooseAddress = (address) => {\r\n  localStorage.setItem('selectedAddress', JSON.stringify(address));\r\n