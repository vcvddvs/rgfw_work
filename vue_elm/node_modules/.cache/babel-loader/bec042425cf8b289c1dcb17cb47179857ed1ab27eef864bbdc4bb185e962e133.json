{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport router from \"../router\"; // 确保正确导入 router 实例\n\nexport default {\n  data() {\n    return {\n      userId: '',\n      password: ''\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:8080/login', {\n          userId: this.userId,\n          password: this.password\n        });\n        // 检查后端是否返回了预期的响应\n        if (response.data && response.data.success) {\n          alert('登录成功');\n          // 处理登录成功，跳转到IndexComponent\n          router.push('/Index-Component');\n        } else {\n          // 处理登录失败\n          alert('登录失败:', response.data ? response.data.message : '未知错误');\n        }\n      } catch (error) {\n        alert('登录失败:', error.message || '网络错误');\n      }\n    },\n    register() {\n      router.push('/Register-component'); // 更改注册跳转方式\n    },\n    goHome() {\n      window.location.href = 'index.html'; // 更改首页跳转方式\n    }\n  }\n};","map":{"version":3,"names":["axios","router","data","userId","password","methods","login","response","post","success","alert","push","message","error","register","goHome","window","location","href"],"sources":["C:\\Users\\asus\\Desktop\\vue_elm\\vue_elm\\src\\components\\loginComponent.vue"],"sourcesContent":["<script>\r\nimport axios from \"axios\";\r\nimport router from \"../router\"; // 确保正确导入 router 实例\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      userId: '',\r\n      password: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      try {\r\n        const response = await axios.post('http://localhost:8080/login', {\r\n          userId: this.userId,\r\n          password: this.password\r\n        });\r\n        // 检查后端是否返回了预期的响应\r\n        if (response.data && response.data.success) {\r\n          alert('登录成功');\r\n          // 处理登录成功，跳转到IndexComponent\r\n          router.push('/Index-Component');\r\n        } else {\r\n          // 处理登录失败\r\n          alert('登录失败:', response.data ? response.data.message : '未知错误');\r\n        }\r\n      } catch (error) {\r\n        alert('登录失败:', error.message || '网络错误');\r\n      }\r\n    },\r\n    register() {\r\n      router.push('/Register-component'); // 更改注册跳转方式\r\n    },\r\n    goHome() {\r\n      window.location.href = 'index.html'; // 更改首页跳转方式\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div class=\"wrapper\">\r\n    <p>登录页面</p>\r\n    <!-- 其他内容 -->\r\n    <div class=\"header\">\r\n      <p>用户登录</p>\r\n    </div>\r\n    <div class=\"body\">\r\n      <div class=\"logins\">\r\n        <div>\r\n          <div class=\"title\">账户:</div>\r\n          <div class=\"content\">\r\n            <input type=\"text\" v-model=\"userId\" placeholder=\"账户\">\r\n          </div>\r\n        </div>\r\n        <div>\r\n          <div class=\"title\">密码：</div>\r\n          <div class=\"content\">\r\n            <input type=\"password\" v-model=\"password\" placeholder=\"密码\">\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"button-box\">\r\n        <button @click=\"login\">登录</button>\r\n      </div>\r\n      <div class=\"button-box\">\r\n        <button @click=\"register\">注册</button>\r\n      </div>\r\n    </div>\r\n    <div class=\"footer\">\r\n      <div @click=\"goHome\">\r\n        <i class=\"fa-solid fa-bowl-rice\"></i>\r\n        <p>首页</p>\r\n      </div>\r\n      <div>\r\n        <i class=\"fa-solid fa-store\"></i>\r\n        <p>全能超市</p>\r\n      </div>\r\n      <div>\r\n        <i class=\"fa-solid fa-comment-dots\"></i>\r\n        <p>历史消息</p>\r\n      </div>\r\n      <div>\r\n        <i class=\"fa-solid fa-cart-plus\"></i>\r\n        <p>购物车</p>\r\n      </div>\r\n      <div>\r\n        <i class=\"fa-solid fa-user\"></i>\r\n        <p>我的</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n/*顶部样式*/\r\n.button-box button:hover {\r\n  background-color: #33b56c; /* 鼠标悬停时的背景色 */\r\n}\r\n\r\n.button-box button:active {\r\n  background-color: #288b52; /* 鼠标点击时的背景色 */\r\n}\r\n/*logo样式*/\r\n.wrapper .header {\r\n  width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  color:  white;\r\n  align-items: center;\r\n  background-color: #17BAF9;\r\n\r\n}\r\n\r\n.wrapper .header p{\r\n  font-size: 5vw;\r\n  font-weight: bolder;\r\n}\r\n\r\n/*主体部分*/\r\n.wrapper .body .logins{\r\n  width: 100%;\r\n  margin-top: 2vw;\r\n  box-sizing: border-box;\r\n\r\n}\r\n.wrapper .body .logins div{\r\n  display: flex;\r\n  box-sizing: border-box;\r\n  padding:4vw 3vw 0 3vw;\r\n  align-items: center;\r\n}\r\n\r\n\r\n\r\n.wrapper .body .logins div .title{\r\n  flex: 0 0 21vw;\r\n  font-size: 4vw;\r\n  padding: 5vw 0 1vw 0;\r\n  font-weight: bolder;\r\n}\r\n.wrapper .body .logins div .content{\r\n  flex: 1;\r\n}\r\n\r\n.wrapper .body .logins div .content input{\r\n  border:none;\r\n  outline: none;\r\n  height: 6vw;\r\n  background-color: white;\r\n}\r\n\r\n\r\n.wrapper .body .button-box{\r\n  width: 90%;\r\n  box-sizing: border-box;\r\n  padding: 2vw 3vw 4vw 3vw;\r\n  margin-left: 3vw;\r\n\r\n\r\n}\r\n\r\n.wrapper .body .button-box button{\r\n  font-size: 5vw;\r\n  width: 100%;\r\n  font-weight: 700;\r\n  color: white;\r\n  background-color: #38ca73;\r\n  border: none;\r\n  border-radius: 15px;\r\n  height: 10vw;\r\n}\r\n\r\n\r\n\r\n/*底部样式*/\r\n.wrapper .footer {\r\n  width: 100%;\r\n  height: 14vw;\r\n  border-top: solid 1px #DDD;\r\n  background-color: white;\r\n  position: fixed;\r\n  left: 0;\r\n  bottom: 0;\r\n\r\n  display: flex;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n}\r\n\r\n.wrapper .footer div {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  color: #999999;\r\n  cursor: pointer;\r\n  user-select: none;\r\n  padding-top: 4vw;\r\n}\r\n\r\n.wrapper .footer div p {\r\n  font-size: 2.8vw;\r\n}\r\n\r\n.wrapper .footer div i {\r\n  font-size: 6vw;\r\n}\r\n</style>"],"mappings":";AACA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,WAAW,EAAE;;AAEhC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,IAAI,CAAC,6BAA6B,EAAE;UAC/DL,MAAM,EAAE,IAAI,CAACA,MAAM;UACnBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACF;QACA,IAAIG,QAAQ,CAACL,IAAG,IAAKK,QAAQ,CAACL,IAAI,CAACO,OAAO,EAAE;UAC1CC,KAAK,CAAC,MAAM,CAAC;UACb;UACAT,MAAM,CAACU,IAAI,CAAC,kBAAkB,CAAC;QACjC,OAAO;UACL;UACAD,KAAK,CAAC,OAAO,EAAEH,QAAQ,CAACL,IAAG,GAAIK,QAAQ,CAACL,IAAI,CAACU,OAAM,GAAI,MAAM,CAAC;QAChE;MACF,EAAE,OAAOC,KAAK,EAAE;QACdH,KAAK,CAAC,OAAO,EAAEG,KAAK,CAACD,OAAM,IAAK,MAAM,CAAC;MACzC;IACF,CAAC;IACDE,QAAQA,CAAA,EAAG;MACTb,MAAM,CAACU,IAAI,CAAC,qBAAqB,CAAC,EAAE;IACtC,CAAC;IACDI,MAAMA,CAAA,EAAG;MACPC,MAAM,CAACC,QAAQ,CAACC,IAAG,GAAI,YAAY,EAAE;IACvC;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}