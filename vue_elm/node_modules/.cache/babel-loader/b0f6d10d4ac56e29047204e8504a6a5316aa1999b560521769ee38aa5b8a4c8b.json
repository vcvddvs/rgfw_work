{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted, computed } from 'vue';\nimport { useRoute } from 'vue-router';\nimport router from \"@/router\";\nimport { v4 as uuidv4 } from 'uuid';\nimport { ElMessage } from 'element-plus';\nimport { businessApi, cartApi } from '@/utils/api';\nexport default {\n  __name: 'businesslist2Component',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const businessImg = ref(route.params.businessImg);\n    const businessId = ref(route.params.businessId);\n    const businessName = ref(route.params.businessName || '默认店铺名称');\n    const foods = ref([]);\n    const cart = ref({});\n    console.log(businessId.value);\n    const fetchFoods = async () => {\n      try {\n        const response = await businessApi.getFoodsByBusinessId(businessId.value);\n        if (Array.isArray(response)) {\n          foods.value = response;\n        } else {\n          console.error('获取菜单数据失败: 返回数据格式不正确', response);\n        }\n      } catch (error) {\n        console.error('获取菜单数据失败:', error);\n      }\n    };\n    onMounted(() => {\n      fetchFoods();\n    });\n    function addToCart(itemName, price, photo, foodId, quantity) {\n      if (cart.value[itemName]) {\n        cart.value[itemName].quantity += quantity;\n      } else {\n        cart.value[itemName] = {\n          price: price,\n          photo: photo,\n          quantity: quantity,\n          foodId: foodId\n        };\n      }\n    }\n    // 计算购物车中商品的总数量\n    const totalQuantity = computed(() => {\n      return Object.values(cart.value).reduce((sum, item) => sum + item.quantity, 0);\n    });\n\n    // 如果商品数量减至0，则从购物车中移除\n    function decrementQuantity(itemName) {\n      if (cart.value[itemName] && cart.value[itemName].quantity > 0) {\n        cart.value[itemName].quantity -= 1;\n      }\n    }\n    const totalPrice = computed(() => {\n      return Object.values(cart.value).reduce((sum, item) => sum + item.quantity * item.price, 0);\n    });\n    function goToPayment() {\n      if (Object.keys(cart.value).length === 0) {\n        ElMessage.warning('购物车为空，请先添加商品！');\n        return;\n      }\n\n      // 从localStorage获取用户ID\n      const userId = localStorage.getItem('userId');\n      if (!userId) {\n        ElMessage.warning('请先登录！');\n        router.push('/login-component');\n        return;\n      }\n      const orderInfo = {\n        businessId: businessId.value,\n        businessName: businessName.value,\n        cart: cart.value\n      };\n      localStorage.setItem('orderInfo', JSON.stringify(orderInfo));\n      // 使用 uuidv4 生成订单ID\n      const orderId = uuidv4();\n\n      // 构建一个只包含 Cart 对象的数组\n      const carts = Object.values(cart.value).map(item => {\n        return {\n          foodId: item.foodId,\n          businessId: businessId.value,\n          userId: userId,\n          quantity: item.quantity,\n          orderId: orderId\n        };\n      });\n      cartApi.checkout(carts).then(data => {\n        console.log('Checkout successful:', data);\n        router.push({\n          name: 'Order',\n          params: {\n            businessName: businessName.value,\n            businessId: businessId.value\n          },\n          query: {\n            cart: JSON.stringify(cart.value)\n          }\n        });\n        ElMessage.success('结算成功！');\n      }).catch(error => {\n        console.error('Checkout error:', error);\n        ElMessage.error('结算失败，请重试');\n      });\n    }\n    function goBack() {\n      router.push('/Index-Component');\n    }\n    const __returned__ = {\n      route,\n      businessImg,\n      businessId,\n      businessName,\n      foods,\n      cart,\n      fetchFoods,\n      addToCart,\n      totalQuantity,\n      decrementQuantity,\n      totalPrice,\n      goToPayment,\n      goBack,\n      ref,\n      onMounted,\n      computed,\n      get useRoute() {\n        return useRoute;\n      },\n      get router() {\n        return router;\n      },\n      get uuidv4() {\n        return uuidv4;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get businessApi() {\n        return businessApi;\n      },\n      get cartApi() {\n        return cartApi;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","useRoute","router","v4","uuidv4","ElMessage","businessApi","cartApi","route","businessImg","params","businessId","businessName","foods","cart","console","log","value","fetchFoods","response","getFoodsByBusinessId","Array","isArray","error","addToCart","itemName","price","photo","foodId","quantity","totalQuantity","Object","values","reduce","sum","item","decrementQuantity","totalPrice","goToPayment","keys","length","warning","userId","localStorage","getItem","push","orderInfo","setItem","JSON","stringify","orderId","carts","map","checkout","then","data","name","query","success","catch","goBack"],"sources":["D:/back_elm/vue_elm/src/components/businesslist2Component.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div class=\"header\">\n      <div class=\"title\">\n        <div class=\"img1\" @click=\"goBack\">\n          <img :src=\"require('@/assets/img/BUSINESS/img_1.png')\" height=\"22\" width=\"13\"/>\n        </div>\n        <div class=\"img2\">\n          <img :src=\"require('@/assets/img/BUSINESS/img.png')\" height=\"auto\" width=\"100%\"/>\n        </div>\n      </div>\n      <div class=\"header-all\">\n        <div class=\"header-text\">\n          <!-- ... 其他代码 ... -->\n          <div class=\"text-left-header\">\n            <h>{{businessName}}</h>\n          </div>\n          <div class=\"star\">\n            <p1>4.7分 </p1>\n            <p2> 蜂鸟准时达</p2>\n            <p3>约39分钟  月售1000+</p3>\n          </div>\n          <div class=\"ticket\">\n            <img :src=\"require('@/assets/img/BannerImg/img_1.png')\" height=\"23\" width=\"100\"/>\n          </div>\n          <div class=\"for-sell\">\n            <p1>25减8 | 45减15</p1>\n            <p2>配送费优惠</p2>\n          </div>\n        </div>\n        <div class=\"text-picture\">\n          <img :src=\"businessImg\" height=\"155\" width=\"157\"/>\n        </div>\n      </div>\n      <div class=\"banner\">\n        <img :src=\"require('@/assets/img/BannerImg/img_3.png')\" height=\"15\" width=\"100\"/>\n        <p1>呈贡区汉堡薯条热销榜·第3名></p1>\n        <p2>每日更新</p2>\n      </div>\n    </div>\n    <div class=\"body\">\n      <!-- ... 其他代码 ... -->\n      <div class=\"recommend\">\n        <div class=\"recommend-left\">\n          <div>\n            <p1>点餐</p1>\n          </div>\n          <div>\n            <p2>评价</p2>\n          </div>\n          <div>\n            <p3>品牌</p3>\n          </div>\n        </div>\n        <div class=\"recommend-right\">\n          <p>好友拼单</p>\n        </div>\n      </div>\n\n      <div class=\"foodList\">\n        <div class=\"foodList-left\">\n          <div class=\"title0\">\n            <img :src=\"require('@/assets/img/BUSINESS/title/img.png')\" height=\"26\" width=\"30\"/>\n            <p> 学生专享</p>\n          </div>\n          <div class=\"title1\">\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_1.png')\" height=\"26\" width=\"30\"/>\n            <p>热销</p>\n          </div>\n          <div class=\"title1\">\n            <img :src=\"require('@/assets/img/BUSINESS/title/img.png')\" height=\"26\" width=\"30\"/>\n            <p> 优惠</p>\n          </div>\n          <div class=\"title2\">\n            <p>热销必点</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_2.png')\" height=\"16\" width=\"46\"/>\n          </div>\n          <div class=\"title2\">\n            <p> 新品尝鲜</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_3.png')\" height=\"13\" width=\"42\"/>\n          </div>\n          <div class=\"title2\">\n            <p>招牌全鸡</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_4.png')\" height=\"20\" width=\"42\"/>\n          </div>\n          <div class=\"title2\">\n            <p>休闲小吃</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_5.png')\" height=\"22\" width=\"39\"/>\n          </div>\n          <div class=\"title2\">\n            <p>卷堡主食</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_6.png')\" height=\"20\" width=\"42\"/>\n          </div>\n          <div class=\"title2\">\n            <p>饮品专区</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_7.png')\" height=\"20\" width=\"40\"/>\n          </div>\n          <div class=\"title2\">\n            <p>调料专区</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_8.png')\" height=\"18\" width=\"40\"/>\n          </div>\n          <div class=\"title2\">\n            <p>品牌活动</p>\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_9.png')\" height=\"19\" width=\"41\"/>\n          </div>\n\n\n        </div>\n        <div class=\"foodList-right\">\n          <div class=\"menus\" v-for=\"food in foods\" :key=\"food.foodId\">\n\n            <div class=\"menus-body\">\n              <div class=\"menus-body-left\">\n                <img :src=\"food.foodImg\" height=\"124\" width=\"150\"/>\n              </div>\n              <div class=\"menus-body-right\">\n                <div class=\"mbr-title\">\n                  <p1>{{ food.foodName }}</p1>\n                </div>\n                <div class=\"mbr-banner\">\n                  <p2>{{ food.foodExplain }}</p2>\n                </div>\n                <div class=\"mbr-count\">\n                  <p3>月售 {{ food月销售量 }}</p3>\n                </div>\n                <div class=\"mbr-sell\">\n                  <p4>{{ food.discount }}折</p4>\n                  <p5>学生价</p5>\n                </div>\n                <div class=\"mbr-price\">\n                  <p>&#165;{{ food.foodPrice }}</p>\n                  <div class=\"cart-actions\">\n                    <img\n                        v-if=\"cart[food.foodName] && cart[food.foodName].quantity > 0\"\n                        :src=\"require('@/assets/img/BUSINESS/menus/img_4.png')\"\n                        height=\"27\"\n                        width=\"28\"\n                        @click=\"decrementQuantity(food.foodName)\"\n                    />\n                    <span v-if=\"cart[food.foodName] && cart[food.foodName].quantity > 0\">{{ cart[food.foodName].quantity }}</span>\n                    <img\n                        :src=\"require('@/assets/img/BUSINESS/menus/img_1.png')\"\n                        height=\"27\"\n                        width=\"28\"\n                        @click=\"addToCart(food.foodName, food.foodPrice,food.foodImg,food.foodId,1)\"\n                    />\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n\n        </div>\n\n      <div class=\"footer\">\n        <!-- ... 其他代码 ... -->\n        <div class=\"cart-left\">\n          <div class=\"cart-left-icon\">\n            <i class=\"fa fa-shopping-cart\"></i>\n          </div>\n          <!-- 当购物车中有商品时显示数量 -->\n          <div class=\"car-left-icon-quantity\" v-if=\"totalQuantity > 0\">{{ totalQuantity }}</div>\n          <div style=\"margin-top: 15px\">价格: {{ totalPrice }}元</div>\n        </div>\n        <div class=\"cart-right\">\n          <div class=\"cart-right-item\" @click=\"goToPayment\">去结算</div>\n        </div>\n      </div>\n  </div>\n\n</template>\n\n\n<script setup>\nimport {ref, onMounted, computed} from 'vue';\nimport { useRoute } from 'vue-router';\nimport router from \"@/router\";\nimport { v4 as uuidv4 } from 'uuid';\nimport { ElMessage } from 'element-plus';\nimport { businessApi, cartApi } from '@/utils/api';\n\nconst route = useRoute();\nconst businessImg=ref(route.params.businessImg);\nconst businessId = ref(route.params.businessId);\nconst businessName = ref(route.params.businessName || '默认店铺名称')\nconst foods = ref([]);\nconst cart = ref({});\nconsole.log(businessId.value);\nconst fetchFoods = async () => {\n  try {\n    const response = await businessApi.getFoodsByBusinessId(businessId.value);\n    if (Array.isArray(response)) {\n      foods.value = response;\n    } else {\n      console.error('获取菜单数据失败: 返回数据格式不正确', response);\n    }\n  } catch (error) {\n    console.error('获取菜单数据失败:', error);\n  }\n};\n\nonMounted(() => {\n  fetchFoods();\n});\n\nfunction addToCart(itemName, price, photo, foodId, quantity) {\n  if (cart.value[itemName]) {\n    cart.value[itemName].quantity += quantity;\n  } else {\n    cart.value[itemName] = { price: price, photo: photo, quantity: quantity, foodId: foodId };\n  }\n}\n// 计算购物车中商品的总数量\nconst totalQuantity = computed(() => {\n  return Object.values(cart.value).reduce((sum, item) => sum + item.quantity, 0);\n});\n\n// 如果商品数量减至0，则从购物车中移除\nfunction decrementQuantity(itemName) {\n  if (cart.value[itemName] && cart.value[itemName].quantity > 0) {\n    cart.value[itemName].quantity -= 1;\n  }\n}\n\n\nconst totalPrice = computed(() => {\n  return Object.values(cart.value).reduce((sum, item) => sum + item.quantity * item.price, 0);\n});\n\nfunction goToPayment() {\n  if (Object.keys(cart.value).length === 0) {\n    ElMessage.warning('购物车为空，请先添加商品！');\n    return;\n  }\n\n  // 从localStorage获取用户ID\n  const userId = localStorage.getItem('userId');\n  if (!userId) {\n    ElMessage.warning('请先登录！');\n    router.push('/login-component');\n    return;\n  }\n\n  const orderInfo = {\n    businessId: businessId.value,\n    businessName: businessName.value,\n    cart: cart.value\n  };\n\n  localStorage.setItem('orderInfo', JSON.stringify(orderInfo));\n  // 使用 uuidv4 生成订单ID\n  const orderId = uuidv4();\n\n  // 构建一个只包含 Cart 对象的数组\n  const carts = Object.values(cart.value).map(item => {\n    return {\n      foodId: item.foodId,\n      businessId: businessId.value,\n      userId: userId,\n      quantity: item.quantity,\n      orderId: orderId\n    };\n  });\n\n  cartApi.checkout(carts)\n    .then(data => {\n      console.log('Checkout successful:', data);\n      router.push({\n        name: 'Order',\n        params: { \n          businessName: businessName.value, \n          businessId: businessId.value \n        },\n        query: { cart: JSON.stringify(cart.value) }\n      });\n      ElMessage.success('结算成功！');\n    })\n    .catch(error => {\n      console.error('Checkout error:', error);\n      ElMessage.error('结算失败，请重试');\n    });\n}\n\n\nfunction goBack() {\n  router.push('/Index-Component');\n}\n</script>\n\n<style scoped>\n.cart-actions {\n  display: flex;\n  align-items: center;\n}\n\n.cart-actions img {\n  margin-left: 5px; /* 根据需要调整间距 */\n  cursor: pointer;\n}\n/* ... 将 businessList.html 中的 CSS 样式复制到这里 ... */\n.wrapper {\n  width: 100%;\n  height: 100%;\n}\n\n.wrapper .header .title {\n  width: 430px;\n  height: 160px;\n  position: relative;\n\n\n}\n\n.wrapper .header .title .img2{\n  width: 100%;\n  height: 60%;\n\n\n}\n\n.wrapper .header .title .img1{\n  position: relative;\n  z-index: 1;\n  top: 108px; /* 你可以根据需要调整垂直位置 */\n  left: 18px; /* 你可以根据需要调整水平位置 */\n  width: 90%;\n\n\n}\n\n.wrapper .header .header-all{\n\n  width: 92%;\n  display: flex;\n  margin-left: 3.5vw;\n  margin-top: -3vw;\n  justify-content: space-between;\n  border: 1px solid red;\n  border-radius: 10px;\n}\n\n\n.wrapper .header .header-all .header-text{\n  flex: 7;\n  border-radius: 10px;\n  width: 100%;\n  box-sizing: border-box;\n  padding-left: 3vw;\n}\n\n.wrapper .header .header-all .header-text .text-left-header {\n\n  display: flex;\n  align-items: center;\n  margin-bottom: 2vw;\n}\n\n.wrapper .header .header-all .header-text .text-left-header h{\n  color: black;\n  font-size:5vw;\n  font-weight: bolder;\n}\n\n.wrapper .header .header-all .header-text .star{\n  display: flex;\n  font-size: 2.7vw;\n  align-items: center;\n  margin-bottom: 2vw;\n\n}\n\n\n.wrapper .header .header-all .header-text .star p1{\n  color: #ff4a25;\n  font-weight: bolder;\n}\n\n.wrapper .header .header-all .header-text .star p2{\n  color:#17BAF9;\n  padding:0 1vw 0 1vw;\n  border-radius: 5px;\n  border: 1px solid #17BAF9;\n  margin: 0 2vw 0 2vw;\n}\n\n.wrapper .header .header-all .header-text .star p3{\n  color: #999999;\n}\n\n.wrapper .header .header-all .header-text .for-sell{\n  display: flex;\n  align-items: center;\n  font-size: 2.7vw;\n  margin-bottom: 3vw;\n}\n\n.wrapper .header .header-all .header-text .for-sell p1{\n  border: 1px solid #ff8b39;\n  color:#ff8b39 ;\n  padding-left: 1vw;\n  padding-right: 1vw;\n  border-radius: 5px;\n}\n\n.wrapper .header .header-all .header-text .for-sell p2{\n  margin-left: 3vw;\n  border: 1px solid red;\n  color:red;\n  padding-left: 1vw;\n  padding-right: 1vw;\n  border-radius: 5px;\n}\n\n.wrapper .header .header-all .text-picture {\n  flex: 3;\n}\n\n.wrapper .header .header-all .text-picture img{\n  width: 13vw;\n  height: 13vw;\n  margin-left: 10vw;\n}\n.wrapper .header .banner{\n  display: flex;\n  align-items: center;\n  border: 1px red solid;\n  border-radius: 5px;\n  margin:2.5vw 4vw 0 3.5vw;\n  padding: 2vw 2vw 2vw 2vw ;\n  font-size: 3vw;\n  color:chocolate;\n}\n\n.wrapper .header .banner p2{\n  padding-left: 10vw;\n}\n\n\n/*主体点餐*/\n.wrapper .body .picture{\n  display: flex;\n  align-items: center;\n}\n.wrapper .body .picture img{\n  width: 95%;\n  height: 50%;\n  margin: 0 auto;\n\n}\n.wrapper .body .recommend{\n  display: flex;\n  margin: 2vw 4.5vw 2vw 0vw;\n  padding: 1vw 0 1vw 0;\n}\n.wrapper .body .recommend .recommend-left{\n  display: flex;\n  flex:7;\n  margin-right: 10vw;\n  justify-content:space-around;\n  align-items: center;\n}\n\n.wrapper .body .recommend .recommend-right{\n  flex: 3;\n  border: 1px solid red;\n  justify-content: space-around;\n  align-items: center;\n  border-radius: 10px;\n}\n\n.wrapper .body .recommend .recommend-right p{\n 