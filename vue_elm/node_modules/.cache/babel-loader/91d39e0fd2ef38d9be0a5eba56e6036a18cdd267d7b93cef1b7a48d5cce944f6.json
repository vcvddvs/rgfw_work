{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, computed, onMounted, watch } from 'vue';\nimport { useRoute } from 'vue-router';\nimport router from \"@/router\";\nexport default {\n  __name: 'OrderComponent',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const businessName = ref(route.params.businessName || '默认店铺名称');\n    const cart = ref({});\n    const deliveryAddresses = ref([]);\n    const showAddresses = ref(false);\n    const businessId = ref(route.params.businessId || 0);\n\n    // 从 localStorage 获取选中的地址\n    const selectedAddress = ref(JSON.parse(localStorage.getItem('selectedAddress') || '{}').address || '云南大学楸苑1栋');\n    const orderInfo = JSON.parse(localStorage.getItem('orderInfo') || '{}');\n    if (orderInfo.cart) {\n      cart.value = orderInfo.cart;\n      businessId.value = orderInfo.businessId;\n      businessName.value = orderInfo.businessName;\n    }\n\n    // 监听路由变化，更新订单信息\n    watch(route, newRoute => {\n      if (newRoute.name === 'Order') {\n        const newOrderInfo = JSON.parse(localStorage.getItem('orderInfo') || '{}');\n        if (newOrderInfo.cart) {\n          cart.value = newOrderInfo.cart;\n          businessId.value = newOrderInfo.businessId;\n          businessName.value = newOrderInfo.businessName;\n        }\n      }\n    });\n    if (route.query.cart) {\n      try {\n        cart.value = JSON.parse(route.query.cart);\n      } catch (error) {\n        console.error('解析购物车数据失败:', error);\n        cart.value = {}; // 确保在解析失败时设置为空对象\n      }\n    }\n    const goToAddressPage = () => {\n      const userId = localStorage.getItem('userId'); // 从localStorage获取userId\n      if (!userId) {\n        console.error('用户未登录');\n        return;\n      }\n      // 导航到 address 组件，并传递 userId\n      router.push({\n        name: 'Address',\n        params: {\n          userId: userId,\n          businessId: businessId.value,\n          businessName: businessName.value\n        }\n      });\n    };\n    onMounted(() => {\n      showAddresses.value = false;\n    });\n    const foodItems = computed(() => {\n      return Object.keys(cart.value).map(key => ({\n        title: key,\n        price: cart.value[key].price,\n        description: cart.value[key].description || '',\n        // 确保有默认值\n        quantity: cart.value[key].quantity,\n        photo: cart.value[key].photo // 包含照片路径\n      }));\n    });\n    const totalAmount = computed(() => {\n      return Object.values(cart.value).reduce((sum, item) => sum + item.quantity * item.price, 0);\n    });\n    const submitOrder = () => {\n      if (Object.keys(cart.value).length === 0) {\n        alert('购物车为空，请先添加商品！');\n        return;\n      }\n      const userId = localStorage.getItem('userId');\n      if (!userId) {\n        alert('用户未登录，请先登录！');\n        return;\n      }\n      const orderDate = new Date().toISOString().slice(0, 10); // 订单日期\n      const orderTotal = totalAmount.value; // 订单总价\n      const daIdSelected = null; // 使用选中的 daId\n\n      const order = {\n        userId: userId,\n        businessId: businessId.value,\n        orderDate: orderDate,\n        orderTotal: orderTotal,\n        daId: daIdSelected,\n        orderState: 0 // 未支付\n      };\n      const details = Object.values(cart.value).map(item => ({\n        foodId: item.foodId,\n        quantity: item.quantity\n      }));\n      fetch('http://localhost:8080/elm_servlet_war_exploded/api/orders/create', {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json'\n        },\n        body: JSON.stringify({\n          order,\n          details\n        }) // 发送包含 order 和 details 的请求体\n      }, console.log(\"Request body:\", {\n        order,\n        details\n      })).then(response => {\n        if (response.ok) {\n          return response.text();\n        } else {\n          throw new Error('Network response was not ok.');\n        }\n      }).then(data => {\n        console.log('Order submitted successfully:', data);\n        // 清空购物车或执行其他操作\n      }).catch(error => {\n        console.error('Error:', error);\n      });\n      router.push({\n        name: 'Payment',\n        query: {\n          totalAmount: totalAmount.value,\n          businessName: businessName.value\n        }\n      });\n    };\n    const __returned__ = {\n      route,\n      businessName,\n      cart,\n      deliveryAddresses,\n      showAddresses,\n      businessId,\n      selectedAddress,\n      orderInfo,\n      goToAddressPage,\n      foodItems,\n      totalAmount,\n      submitOrder,\n      ref,\n      computed,\n      onMounted,\n      watch,\n      get useRoute() {\n        return useRoute;\n      },\n      get router() {\n        return router;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","watch","useRoute","router","route","businessName","params","cart","deliveryAddresses","showAddresses","businessId","selectedAddress","JSON","parse","localStorage","getItem","address","orderInfo","value","newRoute","name","newOrderInfo","query","error","console","goToAddressPage","userId","push","foodItems","Object","keys","map","key","title","price","description","quantity","photo","totalAmount","values","reduce","sum","item","submitOrder","length","alert","orderDate","Date","toISOString","slice","orderTotal","daIdSelected","order","daId","orderState","details","foodId","fetch","method","headers","body","stringify","log","then","response","ok","text","Error","data","catch"],"sources":["C:/Users/asus/Desktop/vue_elm/vue_elm/src/components/OrderComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <header class=\"header\">\r\n      <div class=\"title\">\r\n        <div class=\"p1\">\r\n          <span class=\"iconfont icon-back\" style=\"font-size: 8vw\" @click=\"goBack\"></span>\r\n        </div>\r\n        <div class=\"p2\">\r\n          <p>确认订单</p>\r\n        </div>\r\n      </div>\r\n      <div class=\"banner\">\r\n        <span class=\"iconfont icon-guangbogonggao\" style=\"padding-left: 2vw\"></span>\r\n        <p style=\"padding-left: 2vw\">温馨提示： 请适量点餐,避免浪费</p>\r\n      </div>\r\n      <div class=\"location\">\r\n        <div class=\"location-text\">\r\n          <p>地址: {{ selectedAddress }}</p>\r\n          <img :src=\"require('@/assets/img/orders/img.png')\"\r\n               height=\"23\"\r\n               width=\"17\" alt=\"描述\"\r\n               @click=\"goToAddressPage \" />\r\n        </div>\r\n        <!-- 显示地址列表 -->\r\n        <div v-if=\"showAddresses\" class=\"address-list\">\r\n          <div\r\n              v-for=\"address in deliveryAddresses\"\r\n              :key=\"address.daId\"\r\n              class=\"address-item\"\r\n              @click=\"selectAddress(address)\"\r\n          >\r\n            {{  address.address }}\r\n          </div>\r\n        </div>\r\n        <div class=\"location-speed\">\r\n          <p>立即送出</p>\r\n          <div class=\"location-speed-to\">\r\n            <p>预计13:40送达</p>\r\n            <img :src=\"require('@/assets/img/orders/img.png')\" height=\"23\" width=\"17\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </header>\r\n    <main class=\"body\">\r\n      <div class=\"body-title\">\r\n        <div class=\"title1\">\r\n          <p>{{ businessName }}</p>\r\n\r\n        </div>\r\n        <div class=\"title2\">\r\n          <p>蜂鸟准时达</p>\r\n        </div>\r\n      </div>\r\n      <div class=\"body-text\">\r\n        <div v-for=\"item in foodItems\" :key=\"item.title\" class=\"food\">\r\n          <div class=\"food-left\">\r\n            <img :src=\"item.photo\" alt=\"商品图片\" />\r\n          </div>\r\n          <div class=\"food-right\">\r\n            <div class=\"food-right-title\">\r\n              <div class=\"title1\">\r\n                <p>{{ item.title }}</p>\r\n              </div>\r\n              <div class=\"price\">\r\n                <p>&#165;{{ item.price }}</p>\r\n              </div>\r\n            </div>\r\n            <div class=\"name\">\r\n              <p>{{ item.description }}</p>\r\n            </div>\r\n            <div class=\"number\">\r\n              <p>x {{ item.quantity }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 其他费用和总计 -->\r\n      </div>\r\n    </main>\r\n    <footer class=\"footer\">\r\n      <div class=\"foot-left\">\r\n        <div class=\"sum-price\">\r\n          <p1>合计</p1>\r\n          <p2>{{ totalAmount.toFixed(2) }}</p2> <!-- 格式化金额显示 -->\r\n        </div>\r\n      </div>\r\n      <div class=\"foot-right\" @click=\"submitOrder\">\r\n        <p>提交订单</p>\r\n      </div>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n<script setup>\r\nimport {ref, computed, onMounted, watch} from 'vue';\r\nimport { useRoute } from 'vue-router';\r\nimport router from \"@/router\";\r\n\r\nconst route = useRoute();\r\nconst businessName = ref(route.params.businessName || '默认店铺名称');\r\nconst cart = ref({});\r\nconst deliveryAddresses = ref([]);\r\nconst showAddresses = ref(false);\r\nconst businessId = ref(route.params.businessId || 0);\r\n\r\n// 从 localStorage 获取选中的地址\r\nconst selectedAddress = ref(JSON.parse(localStorage.getItem('selectedAddress') || '{}').address || '云南大学楸苑1栋');\r\n\r\nconst orderInfo = JSON.parse(localStorage.getItem('orderInfo') || '{}');\r\nif (orderInfo.cart) {\r\n  cart.value = orderInfo.cart;\r\n  businessId.value = orderInfo.businessId;\r\n  businessName.value = orderInfo.businessName;\r\n}\r\n\r\n// 监听路由变化，更新订单信息\r\nwatch(route, (newRoute) => {\r\n  if (newRoute.name === 'Order') {\r\n    const newOrderInfo = JSON.parse(localStorage.getItem('orderInfo') || '{}');\r\n    if (newOrderInfo.cart) {\r\n      cart.value = newOrderInfo.cart;\r\n      businessId.value = newOrderInfo.businessId;\r\n      businessName.value = newOrderInfo.businessName;\r\n    }\r\n  }\r\n});\r\n\r\nif (route.query.cart) {\r\n  try {\r\n    cart.value = JSON.parse(route.query.cart);\r\n  } catch (error) {\r\n    console.error('解析购物车数据失败:', error);\r\n    cart.value = {}; // 确保在解析失败时设置为空对象\r\n  }\r\n}\r\n\r\nconst goToAddressPage = () => {\r\n  const userId = localStorage.getItem('userId'); // 从localStorage获取userId\r\n  if (!userId) {\r\n    console.error('用户未登录');\r\n    return;\r\n  }\r\n  // 导航到 address 组件，并传递 userId\r\n  router.push({\r\n    name: 'Address',\r\n    params: { userId: userId, businessId: businessId.value, businessName: businessName.value }\r\n  });\r\n};\r\n\r\nonMounted(() => {\r\n  showAddresses.value = false;\r\n});\r\n\r\nconst foodItems = computed(() => {\r\n  return Object.keys(cart.value).map(key => ({\r\n    title: key,\r\n    price: cart.value[key].price,\r\n    description: cart.value[key].description || '', // 确保有默认值\r\n    quantity: cart.value[key].quantity,\r\n    photo: cart.value[key].photo // 包含照片路径\r\n  }));\r\n});\r\n\r\nconst totalAmount = computed(() => {\r\n  return Object.values(cart.value).reduce((sum, item) => sum + item.quantity * item.price, 0);\r\n});\r\n\r\nconst submitOrder = () => {\r\n  if (Object.keys(cart.value).length === 0) {\r\n    alert('购物车为空，请先添加商品！');\r\n    return;\r\n  }\r\n\r\n  const userId = localStorage.getItem('userId');\r\n  if (!userId) {\r\n    alert('用户未登录，请先登录！');\r\n    return;\r\n  }\r\n\r\n  const orderDate = new Date().toISOString().slice(0, 10); // 订单日期\r\n  const orderTotal = totalAmount.value; // 订单总价\r\n  const daIdSelected = null; // 使用选中的 daId\r\n\r\n  const order = {\r\n    userId: userId,\r\n    businessId: businessId.value,\r\n    orderDate: orderDate,\r\n    orderTotal: orderTotal,\r\n    daId: daIdSelected,\r\n    orderState: 0 // 未支付\r\n  };\r\n\r\n  const details = Object.values(cart.value).map(item => ({\r\n    foodId: item.foodId,\r\n    quantity: item.quantity\r\n  }));\r\n\r\n  fetch('http://localhost:8080/elm_servlet_war_exploded/api/orders/create', {\r\n        method: 'POST',\r\n        headers: {\r\n          'Content-Type': 'application/json',\r\n        },\r\n        body: JSON.stringify({ order, details }), // 发送包含 order 和 details 的请求体\r\n      }\r\n      ,console.log(\"Request body:\", { order, details }))\r\n      .then(response => {\r\n        if (response.ok) {\r\n          return response.text();\r\n        } else {\r\n          throw new Error('Network response was not ok.');\r\n        }\r\n      })\r\n      .then(data => {\r\n        console.log('Order submitted successfully:', data);\r\n        // 清空购物车或执行其他操作\r\n      })\r\n      .catch((error) => {\r\n        console.error('Error:', error);\r\n      });\r\n  router.push({\r\n    name: 'Payment',\r\n    query: { totalAmount: totalAmount.value, businessName: businessName.value }\r\n  });\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n/* 添加 CSS 样式 */\r\n.iconfont {\r\n  /* 确保这里有图标字体的样式 */\r\n  font-family: 'iconfont icon-back';\r\n}\r\n\r\n.wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n.wrapper .header .title {\r\n  display: flex;\r\n  align-items: center;\r\n  background-color: red;\r\n  text-align: center;\r\n  height: 18vw;\r\n\r\n\r\n}\r\n\r\n.wrapper .header .title .p1 {\r\n  flex: 1;\r\n  color: white;\r\n}\r\n\r\n\r\n.wrapper .header .title .p2 {\r\n  flex: 9;\r\n  color: white;\r\n  font-size: 4.5vw;\r\n  padding-right: 6vw;\r\n}\r\n\r\n.wrapper .header .banner{\r\n  display: flex;\r\n  font-size: 2.9vw;\r\n  align-items: center;\r\n  border: none;\r\n  background-color: #ffe99a;\r\n  color: coral;\r\n  border-radius: 10px;\r\n  width: 93%;\r\n  height: 33px;\r\n  margin: 0 auto;\r\n  margin-top: 2vw;\r\n\r\n}\r\n\r\n.wrapper .header .location{\r\n  border: 1px solid red;\r\n  width: 93%;\r\n  margin: 0 auto;\r\n  margin-top: 2vw;\r\n\r\n  border-radius: 10px;\r\n}\r\n\r\n.address-list {\r\n  position: absolute;\r\n  background-color: white;\r\n  border: 1px solid #ccc;\r\n  width:85%;\r\