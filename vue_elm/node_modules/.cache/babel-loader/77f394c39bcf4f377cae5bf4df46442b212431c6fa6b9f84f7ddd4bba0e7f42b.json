{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from 'vue';\nimport axios from 'axios';\nimport { useRouter } from 'vue-router';\nexport default {\n  __name: 'loginComponent',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const userId = ref('');\n    const password = ref('');\n    const login = async () => {\n      try {\n        const response = await axios.post('http://localhost:8080/login', {\n          userId: userId.value,\n          password: password.value\n        });\n        if (response.data && response.data.success) {\n          alert('登录成功');\n          localStorage.setItem('userId', response.data.data.userId); // 保存 userId 到本地存储\n          console.log('User ID stored in localStorage:', localStorage.getItem('userId'));\n          router.push('/Index-Component');\n        } else {\n          alert('登录失败:', response.data ? response.data.message : '未知错误');\n        }\n      } catch (error) {\n        alert('登录失败:', error.message || '网络错误');\n      }\n    };\n    const register = () => {\n      router.push('/Register-component');\n    };\n    const goHome = () => {\n      window.location.href = 'index.html';\n    };\n    const __returned__ = {\n      router,\n      userId,\n      password,\n      login,\n      register,\n      goHome,\n      ref,\n      get axios() {\n        return axios;\n      },\n      get useRouter() {\n        return useRouter;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","axios","useRouter","router","userId","password","login","response","post","value","data","success","alert","localStorage","setItem","console","log","getItem","push","message","error","register","goHome","window","location","href"],"sources":["C:/Users/asus/Desktop/vue_elm/vue_elm/src/components/loginComponent.vue"],"sourcesContent":["<script setup>\r\nimport { ref } from 'vue';\r\nimport axios from 'axios';\r\nimport { useRouter } from 'vue-router';\r\n\r\nconst router = useRouter();\r\nconst userId = ref('');\r\nconst password = ref('');\r\n\r\nconst login = async () => {\r\n  try {\r\n    const response = await axios.post('http://localhost:8080/login', {\r\n      userId: userId.value,\r\n      password: password.value\r\n    });\r\n    if (response.data && response.data.success) {\r\n      alert('登录成功');\r\n      localStorage.setItem('userId', response.data.data.userId);// 保存 userId 到本地存储\r\n      console.log('User ID stored in localStorage:', localStorage.getItem('userId'));\r\n      router.push('/Index-Component');\r\n    } else {\r\n      alert('登录失败:', response.data ? response.data.message : '未知错误');\r\n    }\r\n  } catch (error) {\r\n    alert('登录失败:', error.message || '网络错误');\r\n  }\r\n};\r\n\r\nconst register = () => {\r\n  router.push('/Register-component');\r\n};\r\n\r\nconst goHome = () => {\r\n  window.location.href = 'index.html';\r\n};\r\n</script>\r\n\r\n<template>\r\n  <el-form class=\"wrapper\" @submit.prevent=\"login\">\r\n    <div class=\"header\">\r\n      <p>欢迎使用  饿了么</p>\r\n      <h></h>\r\n    </div>\r\n    <el-form-item label=\"账户:\">\r\n      <el-input v-model=\"userId\" placeholder=\"账户\"></el-input>\r\n    </el-form-item>\r\n    <el-form-item label=\"密码：\">\r\n      <el-input type=\"password\" v-model=\"password\" placeholder=\"密码\"></el-input>\r\n    </el-form-item>\r\n    <div class=\"button-box\">\r\n      <el-button type=\"primary\" @click=\"login\">登录</el-button>\r\n    </div>\r\n    <div class=\"button-box\">\r\n      <el-button @click=\"register\">注册</el-button>\r\n    </div>\r\n    <div class=\"footer\">\r\n      <div @click=\"goHome\">\r\n        <i class=\"fa-solid fa-bowl-rice\"></i>\r\n        <p>首页</p>\r\n      </div>\r\n      <div>\r\n        <i class=\"fa-solid fa-store\"></i>\r\n        <p>全能超市</p>\r\n      </div>\r\n      <div>\r\n     