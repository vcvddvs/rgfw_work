\", _hoisted_11, [_cache[3] || (_cache[3] = _createElementVNode(\"p\", null, \"预计13:40送达\", -1 /* HOISTED */)), _createElementVNode(\"img\", {\n    src: require('@/assets/img/orders/img.png'),\n    height: \"23\",\n    width: \"17\"\n  }, null, 8 /* PROPS */, _hoisted_12)])])])]), _createElementVNode(\"main\", _hoisted_13, [_createElementVNode(\"div\", _hoisted_14, [_createElementVNode(\"div\", _hoisted_15, [_createElementVNode(\"p\", null, _toDisplayString($setup.businessName), 1 /* TEXT */)]), _cache[6] || (_cache[6] = _createElementVNode(\"div\", {\n    class: \"title2\"\n  }, [_createElementVNode(\"p\", null, \"蜂鸟准时达\")], -1 /* HOISTED */))]), _createElementVNode(\"div\", _hoisted_16, [(_openBlock(true), _createElementBlock(_Fragment, null, _renderList($setup.foodItems, item => {\n    return _openBlock(), _createElementBlock(\"div\", {\n      key: item.title,\n      class: \"food\"\n    }, [_createElementVNode(\"div\", _hoisted_17, [_createElementVNode(\"img\", {\n      src: item.photo,\n      alt: \"商品图片\"\n    }, null, 8 /* PROPS */, _hoisted_18)]), _createElementVNode(\"div\", _hoisted_19, [_createElementVNode(\"div\", _hoisted_20, [_createElementVNode(\"div\", _hoisted_21, [_createElementVNode(\"p\", null, _toDisplayString(item.title), 1 /* TEXT */)]), _createElementVNode(\"div\", _hoisted_22, [_createElementVNode(\"p\", null, \"¥\" + _toDisplayString(item.price), 1 /* TEXT */)])]), _createElementVNode(\"div\", _hoisted_23, [_createElementVNode(\"p\", null, _toDisplayString(item.description), 1 /* TEXT */)]), _createElementVNode(\"div\", _hoisted_24, [_createElementVNode(\"p\", null, \"x \" + _toDisplayString(item.quantity), 1 /* TEXT */)])])]);\n  }), 128 /* KEYED_FRAGMENT */)), _createCommentVNode(\" 其他费用和总计 \")])]), _createElementVNode(\"footer\", _hoisted_25, [_createElementVNode(\"div\", _hoisted_26, [_createElementVNode(\"div\", _hoisted_27, [_createVNode(_component_p1, null, {\n    default: _withCtx(() => _cache[7] || (_cache[7] = [_createTextVNode(\"合计\")])),\n    _: 1 /* STABLE */\n  }), _createVNode(_component_p2, null, {\n    default: _withCtx(() => [_createTextVNode(_toDisplayString($setup.totalAmount.toFixed(2)), 1 /* TEXT */)]),\n    _: 1 /* STABLE */\n  }), _createCommentVNode(\" 格式化金额显示 \")])]), _createElementVNode(\"div\", {\n    class: \"foot-right\",\n    onClick: $setup.submitOrder\n  }, _cache[8] || (_cache[8] = [_createElementVNode(\"p\", null, \"提交订单\", -1 /* HOISTED */)]))])]);\n}","map":{"version":3,"names":["class","key","_createElementBlock","_hoisted_1","_createElementVNode","_hoisted_2","_hoisted_3","_hoisted_4","style","onClick","_cache","args","_ctx","goBack","_hoisted_5","_hoisted_6","_toDisplayString","$setup","selectedAddress","src","require","height","width","alt","fetchDeliveryAddresses","_hoisted_7","_createCommentVNode","showAddresses","_hoisted_8","_Fragment","_renderList","deliveryAddresses","address","daId","$event","selectAddress","_hoisted_9","_hoisted_10","_hoisted_11","_hoisted_12","_hoisted_13","_hoisted_14","_hoisted_15","businessName","_hoisted_16","foodItems","item","title","_hoisted_17","photo","_hoisted_18","_hoisted_19","_hoisted_20","_hoisted_21","_hoisted_22","price","_hoisted_23","description","_hoisted_24","quantity","_hoisted_25","_hoisted_26","_hoisted_27","_createVNode","_component_p1","default","_withCtx","_createTextVNode","_","_component_p2","totalAmount","toFixed","submitOrder"],"sources":["C:\\Users\\asus\\Desktop\\vue_elm\\vue_elm\\src\\components\\OrderComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <header class=\"header\">\r\n      <div class=\"title\">\r\n        <div class=\"p1\">\r\n          <span class=\"iconfont icon-back\" style=\"font-size: 8vw\" @click=\"goBack\"></span>\r\n        </div>\r\n        <div class=\"p2\">\r\n          <p>确认订单</p>\r\n        </div>\r\n      </div>\r\n      <div class=\"banner\">\r\n        <span class=\"iconfont icon-guangbogonggao\" style=\"padding-left: 2vw\"></span>\r\n        <p style=\"padding-left: 2vw\">温馨提示： 请适量点餐,避免浪费</p>\r\n      </div>\r\n      <div class=\"location\">\r\n        <div class=\"location-text\">\r\n          <p>地址: {{ selectedAddress }}</p>\r\n          <img :src=\"require('@/assets/img/orders/img.png')\"\r\n               height=\"23\"\r\n               width=\"17\" alt=\"描述\"\r\n               @click=\"fetchDeliveryAddresses \" />\r\n        </div>\r\n        <!-- 显示地址列表 -->\r\n        <div v-if=\"showAddresses\" class=\"address-list\">\r\n          <div\r\n              v-for=\"address in deliveryAddresses\"\r\n              :key=\"address.daId\"\r\n              class=\"address-item\"\r\n              @click=\"selectAddress(address)\"\r\n          >\r\n            {{  address.address }}\r\n          </div>\r\n        </div>\r\n        <div class=\"location-speed\">\r\n          <p>立即送出</p>\r\n          <div class=\"location-speed-to\">\r\n            <p>预计13:40送达</p>\r\n            <img :src=\"require('@/assets/img/orders/img.png')\" height=\"23\" width=\"17\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </header>\r\n    <main class=\"body\">\r\n      <div class=\"body-title\">\r\n        <div class=\"title1\">\r\n          <p>{{ businessName }}</p>\r\n\r\n        </div>\r\n        <div class=\"title2\">\r\n          <p>蜂鸟准时达</p>\r\n        </div>\r\n      </div>\r\n      <div class=\"body-text\">\r\n        <div v-for=\"item in foodItems\" :key=\"item.title\" class=\"food\">\r\n          <div class=\"food-left\">\r\n            <img :src=\"item.photo\" alt=\"商品图片\" />\r\n          </div>\r\n          <div class=\"food-right\">\r\n            <div class=\"food-right-title\">\r\n              <div class=\"title1\">\r\n                <p>{{ item.title }}</p>\r\n              </div>\r\n              <div class=\"price\">\r\n                <p>&#165;{{ item.price }}</p>\r\n              </div>\r\n            </div>\r\n            <div class=\"name\">\r\n              <p>{{ item.description }}</p>\r\n            </div>\r\n            <div class=\"number\">\r\n              <p>x {{ item.quantity }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 其他费用和总计 -->\r\n      </div>\r\n    </main>\r\n    <footer class=\"footer\">\r\n      <div class=\"foot-left\">\r\n        <div class=\"sum-price\">\r\n          <p1>合计</p1>\r\n          <p2>{{ totalAmount.toFixed(2) }}</p2> <!-- 格式化金额显示 -->\r\n        </div>\r\n      </div>\r\n      <div class=\"foot-right\" @click=\"submitOrder\">\r\n        <p>提交订单</p>\r\n      </div>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n<script setup>\r\nimport {ref, computed, onMounted} from 'vue';\r\nimport { useRoute } from 'vue-router';\r\nimport axios from 'axios';\r\nimport router from \"@/router\";\r\n\r\nconst route = useRoute();\r\nconst businessName = ref(route.params.businessName || '默认店铺名称');\r\nconst cart = ref({});\r\nconst selectedAddress = ref('云南大学楸苑1栋'); // 初始地址\r\nconst deliveryAddresses = ref([]);\r\nconst showAddresses = ref(false); // 控制地址列表显示与隐藏\r\nconst businessId = ref(0); // 初始化 businessId 为 null\r\n\r\n\r\nif (route.query.cart) {\r\n  try {\r\n    cart.value = JSON.parse(route.query.cart);\r\n    console.log(\"Parsed cart:\", cart.value); // 确认解析后的购物车数据\r\n  } catch (error) {\r\n    console.error('解析购物车数据失败:', error);\r\n    cart.value = {}; // 确保在解析失败时设置为空对象\r\n  }\r\n}\r\nif (route.params.businessId) {\r\n  businessId.value = parseInt(route.params.businessId);\r\n}\r\n\r\nconst fetchDeliveryAddresses = async () => {\r\n  const userId = localStorage.getItem('userId'); // 从localStorage获取userId\r\n  if (!userId) {\r\n    console.error('用户未登录');\r\n    return;\r\n  }\r\n  try {\r\n    const response = await axios.get(`http://localhost:8080/api/deliveryaddresses?userId=${userId}`);\r\n    deliveryAddresses.value = response.data;\r\n    showAddresses.value = true; // 显示地址列表\r\n  } catch (error) {\r\n    console.error('获取地址失败:', error);\r\n  }\r\n};\r\n\r\nconst selectAddress = (address) => {\r\n  selectedAddress.value = address.address; // 设置选中的地址\r\n  showAddresses.value = false; // 隐藏地址列表\r\n};\r\n\r\nonMounted(() => {\r\n  showAddresses.value = false;\r\n});\r\n\r\nconst foodItems = computed(() => {\r\n  return Object.keys(cart.value).map(key => ({\r\n    title: key,\r\n    price: cart.value[key].price,\r\n    description: cart.value[key].description || '', // 确保有默认值\r\n    quantity: cart.value[key].quantity,\r\n    photo: cart.value[key].photo // 包含照片路径\r\n  }));\r\n});\r\n\r\nconst totalAmount = computed(() => {\r\n  return Object.values(cart.value).reduce((sum, item) => sum + item.quantity * item.price, 0);\r\n});\r\n\r\nconst submitOrder = () => {\r\n  if (Object.keys(cart.value).length === 0) {\r\n    alert('购物车为空，请先添加商品！');\r\n    return;\r\n  }\r\n\r\n  const userId = localStorage.getItem('userId');\r\n  if (!userId) {\r\n    alert('用户未登录，请先登录！');\r\n    return;\r\n  }\r\n\r\n  const orderDate = new Date().toISOString().slice(0, 10); // 订单日期\r\n  const orderTotal = totalAmount.value; // 订单总价\r\n  const daId = 1; // 假设有一个默认的送货地址ID\r\n\r\n  const order = {\r\n    userId: userId,\r\n    businessId: businessId.value,\r\n    orderDate: orderDate,\r\n    orderTotal: orderTotal,\r\n    daId: daId,\r\n    orderState: 0 // 未支付\r\n  };\r\n\r\n  const details = Object.values(cart.value).map(item => ({\r\n    foodId: item.foodId,\r\n    quantity: item.quantity\r\n  }));\r\n\r\n  fetch('http://localhost:8080/api/orders/create', {\r\n    method: 'POST',\r\n    headers: {\r\n      'Content-Type': 'application/json',\r\n    },\r\n    body: JSON.stringify({ order, details }), // 发送包含 order 和 details 的请求体\r\n  }\r\n  ,console.log(\"Request body:\", { order, details }))\r\n      .then(response => {\r\n        if (response.ok) {\r\n          return response.text();\r\n        } else {\r\n          throw new Error('Network response was not ok.');\r\n        }\r\n      })\r\n      .then(data => {\r\n        console.log('Order submitted successfully:', data);\r\n        // 清空购物车或执行其他操作\r\n      })\r\n      .catch((error) => {\r\n        console.error('Error:', error);\r\n      });\r\n  router.push({\r\n    name: 'Payment',\r\n    params: { totalAmount: totalAmount.value }\r\n  });\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n/* 添加 CSS 样式 */\r\n.wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n.wrapper .header .title {\r\n  display: flex;\r\n  align-items: center;\r\n  background-color: red;\r\n  text-align: center;\r\n  height: 18vw;\r\n\r\n\r\n}\r\n\r\n.wrapper .header .title .p1 {\r\n  flex: 1;\r\n  color: white;\r\n}\r\n\r\n\r\n.wrapper .header .title .p2 {\r\n  flex: 9;\r\n  color: white;\r\n  font-size: 4.5vw;\r\n  padding-right: 6vw;\r\n}\r\n\r\n.wrapper .header .banner{\r\n  display: flex;\r\n  font-size: 2.9vw;\r\n  align-items: center;\r\n  border: none;\r\n  background-color: #ffe99a;\r\n  color: coral;\r\n  border-radius: 10px;\r\n  width: 93%;\r\n  height: 33px;\r\n  margin: 0 auto;\r\n  margin-top: 2vw;\r\n\r\n}\r\n\r\n.wrapper .header .location{\r\n  border: 1px solid red;\r\n  width: 93%;\r\n  margin: 0 auto;\r\n  margin-top: 2vw;\r\n\r\n  border-radius: 10px;\r\n}\r\n\r\n.address-list {\r\n  position: absolute;\r\n  background-color: white;\r\n  border: 1px solid #ccc;\r\n  width:85%;\r\n  z-index: 100;\r\n  margin-top: 5px;\r\n  margin-left: 5px;\r\n  border-radius: 5px;\r\n  box-shadow: 0 2px 5px rgba(0,0,0,0.2);\r\n}\r\n\r\n.address-item {\r\n  padding: 10px;\r\n  border-bottom: 1px solid #eee;\r\n  cursor: pointer;\r\n}\r\n\r\n.address-item:last-child {\r\n  border-bottom: none;\r\n}\r\n\r\n.wrapper .header .location-text{\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n\r\n  font-size: 5vw;\r\n  font-weight: bolder;\r\n\r\n  margin: 0 2vw 0 2vw;\r\n}\r\n\r\n.wrapper .header .location-speed{\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n\r\n  font-weight: bolder;\r\n\r\n  margin: 0 2vw 0 2vw;\r\n}\r\n\r\n.wrapper .header .location-speed .location-speed-to{\r\n  display: flex;\r\n  align-items: center;\r\n  color: red;\r\n}\r\n\r\n/*主体部分*/\r\n.wrapper .body{\r\n  border: 1px solid red;\r\n  width: 93%;\r\n  margin: 0 auto;\r\n  margin-top: 2vw;\r\n  border-radius: 10px;\r\n}\r\n\r\n.wrapper .body .body-title{\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 0 2vw 0 2vw;\r\n  align-items: center;\r\n}\r\n\r\n.wrapper .body .body-title .title1{\r\n  color: #999999;\r\n  font-size: 4vw;\r\n}\r\n\r\n.wrapper .body .body-title .title2 p{\r\n  color: #17BAF9;\r\n  border: 1px solid #b3d4fc;\r\n  border-radius: 4px;\r\n  font-size: 3vw;\r\n  padding: 0.5vw 0.5vw 0.5vw 0.5vw;\r\n}\r\n\r\n\r\n.wrapper .body .body-text .food {\r\n\r\n  display: flex;\r\n  align-items: center;\r\n\r\n}\r\n\r\n.wrapper .body .body-text .food .food-left{\r\n  flex: 2;\r\n}\r\n\r\n.wrapper .body .body-text .food .food-left img{\r\n  width: 75px;\r\n\r\n}\r\n\r\n.wrapper .body .body-text .food .food-right{\r\n  flex: 8;\r\n}\r\n.wrapper .body .body-text .food .food-right .food-right-title{\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  height: 20px;\r\n  font-size: 3.6vw;\r\n  font-weight: bolder;\r\n  padding: 0 1vw 0 1vw;\r\n}\r\n\r\n.wrapper .body .body-text .food .food-right .name{\r\n  font-size: 2.8vw;\r\n  color: #9F9F9F;\r\n}\r\n\r\n.wrapper .body .body-text .food .food-right .number{\r\n  height: 18px;\r\n  color: #9F9F9F;\r\n}\r\n\r\n.wrapper .body .db-price{\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  font-size: 3.5vw;\r\n  padding: 0 2vw 0 2vw;\r\n  margin-bottom: 1vw;\r\n}\r\n\r\n.wrapper .body .ps-price{\r\n  padding: 0 2vw 0 2vw;\r\n}\r\n.wrapper .body .ps-price .price1{\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  height: 10px;\r\n}\r\n\r\n.wrapper .body .ps-price .price1 .j-psf{\r\n  display: flex;\r\n  align-items: center