{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted, computed } from 'vue';\nimport { useRoute } from 'vue-router';\nimport router from \"@/router\";\nimport { v4 as uuidv4 } from 'uuid';\nimport { ElMessage } from 'element-plus';\nexport default {\n  __name: 'businesslist2Component',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const businessImg = ref(route.params.businessImg);\n    const businessId = ref(route.params.businessId);\n    const businessName = ref(route.params.businessName || '默认店铺名称');\n    const foods = ref([]);\n    const cart = ref({});\n    console.log(businessId.value);\n    const fetchFoods = async () => {\n      try {\n        const response = await fetch(`http://localhost:8080/elm_servlet_war_exploded/api/foods?businessId=${businessId.value}`);\n        if (!response.ok) {\n          throw new Error('Network response was not ok');\n        }\n        const data = await response.json();\n        foods.value = data;\n      } catch (error) {\n        console.error('获取菜单数据失败:', error);\n      }\n    };\n    onMounted(() => {\n      fetchFoods();\n    });\n    function addToCart(itemName, price, photo, foodId, quantity) {\n      if (cart.value[itemName]) {\n        cart.value[itemName].quantity += quantity;\n      } else {\n        cart.value[itemName] = {\n          price: price,\n          photo: photo,\n          quantity: quantity,\n          foodId: foodId\n        };\n      }\n    }\n    // 计算购物车中商品的总数量\n    const totalQuantity = computed(() => {\n      return Object.values(cart.value).reduce((sum, item) => sum + item.quantity, 0);\n    });\n\n    // 如果商品数量减至0，则从购物车中移除\n    function decrementQuantity(itemName) {\n      if (cart.value[itemName] && cart.value[itemName].quantity > 0) {\n        cart.value[itemName].quantity -= 1;\n      }\n    }\n    const totalPrice = computed(() => {\n      return Object.values(cart.value).reduce((sum, item) => sum + item.quantity * item.price, 0);\n    });\n    function goToPayment() {\n      if (Object.keys(cart.value).length === 0) {\n        alert('购物车为空，请先添加商品！');\n        return;\n      }\n\n      // 从localStorage获取用户ID\n      const userId = localStorage.getItem('userId');\n      if (!userId) {\n        alert('用户未登录，请先登录！');\n        return;\n      }\n      const orderInfo = {\n        businessId: businessId.value,\n        businessName: businessName.value,\n        cart: cart.value\n      };\n      localStorage.setItem('orderInfo', JSON.stringify(orderInfo));\n      // 使用 uuidv4 生成订单ID\n      const orderId = uuidv4();\n\n      // 构建一个只包含 Cart 对象的数组\n      const carts = Object.values(cart.value).map(item => {\n        return {\n          foodId: item.foodId,\n          businessId: businessId.value,\n          userId: userId,\n          quantity: item.quantity,\n          orderId: orderId // 使用 uuidv4 生成的 orderId\n        };\n      });\n\n      // 发送请求\n      const token = localStorage.getItem('token');\n      if (!token) {\n        console.error('No authentication token found');\n        router.push('/login-component');\n        return;\n      }\n      fetch('http://localhost:8080/elm_servlet_war_exploded/api/cart/checkout', {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json',\n          'Authorization': `Bearer ${token}`\n        },\n        body: JSON.stringify(carts)\n      }).then(response => {\n        if (response.status === 403) {\n          // 处理认证失败\n          localStorage.removeItem('token');\n          router.push('/login-component');\n          throw new Error('Authentication failed. Please login again.');\n        }\n        if (!response.ok) {\n          throw new Error(`HTTP error! status: ${response.status}`);\n        }\n        return response.json();\n      }).then(data => {\n        console.log('Checkout successful:', data);\n        router.push({\n          name: 'Order',\n          params: {\n            businessName: businessName.value,\n            businessId: businessId.value\n          },\n          query: {\n            cart: JSON.stringify(cart.value)\n          }\n        });\n      }).catch(error => {\n        console.error('Error:', error);\n        ElMessage.error(error.message || '结算失败，请重试');\n      });\n    }\n    function goBack() {\n      router.push('/Index-Component');\n    }\n    const __returned__ = {\n      route,\n      businessImg,\n      businessId,\n      businessName,\n      foods,\n      cart,\n      fetchFoods,\n      addToCart,\n      totalQuantity,\n      decrementQuantity,\n      totalPrice,\n      goToPayment,\n      goBack,\n      ref,\n      onMounted,\n      computed,\n      get useRoute() {\n        return useRoute;\n      },\n      get router() {\n        return router;\n      },\n      get uuidv4() {\n        return uuidv4;\n      },\n      get ElMessage() {\n        return ElMessage;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","useRoute","router","v4","uuidv4","ElMessage","route","businessImg","params","businessId","businessName","foods","cart","console","log","value","fetchFoods","response","fetch","ok","Error","data","json","error","addToCart","itemName","price","photo","foodId","quantity","totalQuantity","Object","values","reduce","sum","item","decrementQuantity","totalPrice","goToPayment","keys","length","alert","userId","localStorage","getItem","orderInfo","setItem","JSON","stringify","orderId","carts","map","token","push","method","headers","body","then","status","removeItem","name","query","catch","message","goBack"],"sources":["D:/IDEA/vue_elm/vue_elm/src/components/businesslist2Component.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <div class=\"header\">\r\n      <div class=\"title\">\r\n        <div class=\"img1\" @click=\"goBack\">\r\n          <img :src=\"require('@/assets/img/BUSINESS/img_1.png')\" height=\"22\" width=\"13\"/>\r\n        </div>\r\n        <div class=\"img2\">\r\n          <img :src=\"require('@/assets/img/BUSINESS/img.png')\" height=\"auto\" width=\"100%\"/>\r\n        </div>\r\n      </div>\r\n      <div class=\"header-all\">\r\n        <div class=\"header-text\">\r\n          <!-- ... 其他代码 ... -->\r\n          <div class=\"text-left-header\">\r\n            <h>{{businessName}}</h>\r\n          </div>\r\n          <div class=\"star\">\r\n            <p1>4.7分 </p1>\r\n            <p2> 蜂鸟准时达</p2>\r\n            <p3>约39分钟  月售1000+</p3>\r\n          </div>\r\n          <div class=\"ticket\">\r\n            <img :src=\"require('@/assets/img/BannerImg/img_1.png')\" height=\"23\" width=\"100\"/>\r\n          </div>\r\n          <div class=\"for-sell\">\r\n            <p1>25减8 | 45减15</p1>\r\n            <p2>配送费优惠</p2>\r\n          </div>\r\n        </div>\r\n        <div class=\"text-picture\">\r\n          <img :src=\"businessImg\" height=\"155\" width=\"157\"/>\r\n        </div>\r\n      </div>\r\n      <div class=\"banner\">\r\n        <img :src=\"require('@/assets/img/BannerImg/img_3.png')\" height=\"15\" width=\"100\"/>\r\n        <p1>呈贡区汉堡薯条热销榜·第3名></p1>\r\n        <p2>每日更新</p2>\r\n      </div>\r\n    </div>\r\n    <div class=\"body\">\r\n      <!-- ... 其他代码 ... -->\r\n      <div class=\"recommend\">\r\n        <div class=\"recommend-left\">\r\n          <div>\r\n            <p1>点餐</p1>\r\n          </div>\r\n          <div>\r\n            <p2>评价</p2>\r\n          </div>\r\n          <div>\r\n            <p3>品牌</p3>\r\n          </div>\r\n        </div>\r\n        <div class=\"recommend-right\">\r\n          <p>好友拼单</p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"foodList\">\r\n        <div class=\"foodList-left\">\r\n          <div class=\"title0\">\r\n            <img :src=\"require('@/assets/img/BUSINESS/title/img.png')\" height=\"26\" width=\"30\"/>\r\n            <p> 学生专享</p>\r\n          </div>\r\n          <div class=\"title1\">\r\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_1.png')\" height=\"26\" width=\"30\"/>\r\n            <p>热销</p>\r\n          </div>\r\n          <div class=\"title1\">\r\n            <img :src=\"require('@/assets/img/BUSINESS/title/img.png')\" height=\"26\" width=\"30\"/>\r\n            <p> 优惠</p>\r\n          </div>\r\n          <div class=\"title2\">\r\n            <p>热销必点</p>\r\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_2.png')\" height=\"16\" width=\"46\"/>\r\n          </div>\r\n          <div class=\"title2\">\r\n            <p> 新品尝鲜</p>\r\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_3.png')\" height=\"13\" width=\"42\"/>\r\n          </div>\r\n          <div class=\"title2\">\r\n            <p>招牌全鸡</p>\r\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_4.png')\" height=\"20\" width=\"42\"/>\r\n          </div>\r\n          <div class=\"title2\">\r\n            <p>休闲小吃</p>\r\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_5.png')\" height=\"22\" width=\"39\"/>\r\n          </div>\r\n          <div class=\"title2\">\r\n            <p>卷堡主食</p>\r\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_6.png')\" height=\"20\" width=\"42\"/>\r\n          </div>\r\n          <div class=\"title2\">\r\n            <p>饮品专区</p>\r\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_7.png')\" height=\"20\" width=\"40\"/>\r\n          </div>\r\n          <div class=\"title2\">\r\n            <p>调料专区</p>\r\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_8.png')\" height=\"18\" width=\"40\"/>\r\n          </div>\r\n          <div class=\"title2\">\r\n            <p>品牌活动</p>\r\n            <img :src=\"require('@/assets/img/BUSINESS/title/img_9.png')\" height=\"19\" width=\"41\"/>\r\n          </div>\r\n\r\n\r\n        </div>\r\n        <div class=\"foodList-right\">\r\n          <div class=\"menus\" v-for=\"food in foods\" :key=\"food.foodId\">\r\n\r\n            <div class=\"menus-body\">\r\n              <div class=\"menus-body-left\">\r\n                <img :src=\"food.foodImg\" height=\"124\" width=\"150\"/>\r\n              </div>\r\n              <div class=\"menus-body-right\">\r\n                <div class=\"mbr-title\">\r\n                  <p1>{{ food.foodName }}</p1>\r\n                </div>\r\n                <div class=\"mbr-banner\">\r\n                  <p2>{{ food.foodExplain }}</p2>\r\n                </div>\r\n                <div class=\"mbr-count\">\r\n                  <p3>月售 {{ food月销售量 }}</p3>\r\n                </div>\r\n                <div class=\"mbr-sell\">\r\n                  <p4>{{ food.discount }}折</p4>\r\n                  <p5>学生价</p5>\r\n                </div>\r\n                <div class=\"mbr-price\">\r\n                  <p>&#165;{{ food.foodPrice }}</p>\r\n                  <div class=\"cart-actions\">\r\n                    <img\r\n                        v-if=\"cart[food.foodName] && cart[food.foodName].quantity > 0\"\r\n                        :src=\"require('@/assets/img/BUSINESS/menus/img_4.png')\"\r\n                        height=\"27\"\r\n                        width=\"28\"\r\n                        @click=\"decrementQuantity(food.foodName)\"\r\n                    />\r\n                    <span v-if=\"cart[food.foodName] && cart[food.foodName].quantity > 0\">{{ cart[food.foodName].quantity }}</span>\r\n                    <img\r\n                        :src=\"require('@/assets/img/BUSINESS/menus/img_1.png')\"\r\n                        height=\"27\"\r\n                        width=\"28\"\r\n                        @click=\"addToCart(food.foodName, food.foodPrice,food.foodImg,food.foodId,1)\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n\r\n        </div>\r\n\r\n      <div class=\"footer\">\r\n        <!-- ... 其他代码 ... -->\r\n        <div class=\"cart-left\">\r\n          <div class=\"cart-left-icon\">\r\n            <i class=\"fa fa-shopping-cart\"></i>\r\n          </div>\r\n          <!-- 当购物车中有商品时显示数量 -->\r\n          <div class=\"car-left-icon-quantity\" v-if=\"totalQuantity > 0\">{{ totalQuantity }}</div>\r\n          <div style=\"margin-top: 15px\">价格: {{ totalPrice }}元</div>\r\n        </div>\r\n        <div class=\"cart-right\">\r\n          <div class=\"cart-right-item\" @click=\"goToPayment\">去结算</div>\r\n        </div>\r\n      </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n\r\n<script setup>\r\nimport {ref, onMounted, computed} from 'vue';\r\nimport { useRoute } from 'vue-router';\r\nimport router from \"@/router\";\r\nimport { v4 as uuidv4 } from 'uuid';\r\nimport { ElMessage } from 'element-plus';\r\n\r\nconst route = useRoute();\r\nconst businessImg=ref(route.params.businessImg);\r\nconst businessId = ref(route.params.businessId);\r\nconst businessName = ref(route.params.businessName || '默认店铺名称')\r\nconst foods = ref([]);\r\nconst cart = ref({});\r\nconsole.log(businessId.value);\r\nconst fetchFoods = async () => {\r\n  try {\r\n    const response = await fetch(`http://localhost:8080/elm_servlet_war_exploded/api/foods?businessId=${businessId.value}`);\r\n    if (!response.ok) {\r\n      throw new Error('Network response was not ok');\r\n    }\r\n    const data = await response.json();\r\n    foods.value = data;\r\n  } catch (error) {\r\n    console.error('获取菜单数据失败:', error);\r\n  }\r\n};\r\n\r\nonMounted(() => {\r\n  fetchFoods();\r\n});\r\n\r\nfunction addToCart(itemName, price, photo, foodId, quantity) {\r\n  if (cart.value[itemName]) {\r\n    cart.value[itemName].quantity += quantity;\r\n  } else {\r\n    cart.value[itemName] = { price: price, photo: photo, quantity: quantity, foodId: foodId };\r\n  }\r\n}\r\n// 计算购物车中商品的总数量\r\nconst totalQuantity = computed(() => {\r\n  return Object.values(cart.value).reduce((sum, item) => sum + item.quantity, 0);\r\n});\r\n\r\n// 如果商品数量减至0，则从购物车中移除\r\nfunction decrementQuantity(itemName) {\r\n  if (cart.value[itemName] && cart.value[itemName].quantity > 0) {\r\n    cart.value[itemName].quantity -= 1;\r\n  }\r\n}\r\n\r\n\r\nconst totalPrice = computed(() => {\r\n  return Object.values(cart.value).reduce((sum, item) => sum + item.quantity * item.price, 0);\r\n});\r\n\r\nfunction goToPayment() {\r\n  if (Object.keys(cart.value).length === 0) {\r\n    alert('购物车为空，请先添加商品！');\r\n    return;\r\n  }\r\n\r\n  // 从localStorage获取用户ID\r\n  const userId = localStorage.getItem('userId');\r\n  if (!userId) {\r\n    alert('用户未登录，请先登录！');\r\n    return;\r\n  }\r\n  const orderInfo = {\r\n    businessId: businessId.value,\r\n    businessName: businessName.value,\r\n    cart: cart.value\r\n  };\r\n\r\n  localStorage.setItem('orderInfo', JSON.stringify(orderInfo));\r\n  // 使用 uuidv4 生成订单ID\r\n  const orderId = uuidv4();\r\n\r\n  // 构建一个只包含 Cart 对象的数组\r\n  const carts = Object.values(cart.value).map(item => {\r\n    return {\r\n      foodId: item.foodId,\r\n      businessId: businessId.value,\r\n      userId: userId,\r\n      quantity: item.quantity,\r\n      orderId: orderId // 使用 uuidv4 生成的 orderId\r\n    };\r\n  });\r\n\r\n  // 发送请求\r\n  const token = localStorage.getItem('token');\r\n  if (!token) {\r\n    console.error('No authentication token found');\r\n    router.push('/login-component');\r\n    return;\r\n  }\r\n\r\n  fetch('http://localhost:8080/elm_servlet_war_exploded/api/cart/checkout', {\r\n    method: 'POST',\r\n    headers: {\r\n      'Content-Type': 'application/json',\r\n      'Authorization': `Bearer ${token}`\r\n    },\r\n    body: JSON.stringify(carts),\r\n  })\r\n  .then(response => {\r\n    if (response.status === 403) {\r\n      // 处理认证失败\r\n      localStorage.removeItem('token');\r\n      router.push('/login-component');\r\n      throw new Error('Authentication failed. Please login again.');\r\n    }\r\n    if (!response.ok) {\r\n      throw new Error(`HTTP error! status: ${response.status}`);\r\n    }\r\n    return response.json();\r\n  })\r\n  .then(data => {\r\n    console.log('Checkout successful:', data);\r\n    router.push({\r\n      name: 'Order',\r\n      params: { businessName: businessName.value, businessId: businessId.value },\r\n      query: { cart: JSON.stringify(cart.value) }\r\n    });\r\n  })\r\n  .catch((error) => {\r\n    console.error('Error:', error);\r\n    ElMessage.error(error.message || '结算失败，请重试');\r\n  });\r\n}\r\n\r\n\r\nfunction goBack() {\r\n  router.push('/Index-Component');\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.cart-actions {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.cart-actions img {\r\n  margin-left: 5px; /* 根据需要调整间距 */\r\n  cursor: pointer;\r\n}\r\n/* ... 将 businessList.html 中的 CSS 样式复制到这里 ... */\r\n.wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n.wrapper .header .title {\r\n  width: 430px;\r\n  height: 160px;\r\n  position: relative;\r\n\r\n\r\n}\r\n\r\n.wrapper .header .title .img2{\r\n  width: 100%;\r\n  height: 60%;\r\n\r\n\r\n}\r\n\r\n.wrapper .header .title .img1{\r\n  position: relative;\r\n  z-index: 1;\r\n  top: 108px; /* 你可以根据需要调整垂直位置 */\r\n  left: 18px; /* 你可以根据需要调整水平位置 */\r\n  width: 90%;\r\n\r\n\r\n}\r\n\r\n.wrapper .header .header-all{\r\n\r\n  width: 92%;\r\n  display: flex;\r\n  margin-left: 3.5vw;\r\n  margin-top: -3vw;\r\n  justify-content: space-between;\r\n  border: 1px solid red;\r\n  border-radius: 10px;\r\n}\r\n\r\n\r\n.wrapper .header .header-all .header-text{\r\n  flex: 7;\r\n  border-radius: 10px;\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  padding-left: 3vw;\r\n}\r\n\r\n.wrapper .header .header-all .header-text .text-left-header {\r\n\r\n  display: flex;\r\n  align-items: center;\r\n  margin-bottom: 2vw;\r\n}\r\n\r\n.wrapper .header .header-all .header-text .text-left-header h{\r\n  color: black;\r\n  font-size:5vw;\r\n  font-weight: b